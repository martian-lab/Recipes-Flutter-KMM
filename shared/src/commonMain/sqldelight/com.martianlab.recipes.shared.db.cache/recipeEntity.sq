CREATE TABLE IF NOT EXISTS `recipeStageEntity` (
    `id` INTEGER NOT NULL,
    `recipeId` INTEGER NOT NULL,
    `imageURL` TEXT NOT NULL,
    `text` TEXT NOT NULL,
    `step` INTEGER as Int NOT NULL,
PRIMARY KEY(`id`),
FOREIGN KEY(`recipeId`) REFERENCES `recipeEntity`(`id`) ON UPDATE NO ACTION ON DELETE CASCADE );

CREATE INDEX IF NOT EXISTS `index_recipe_entity_recipeId` ON `recipeStageEntity` (`recipeId`);
INSERT INTO recipeStageEntity VALUES(2903,1515,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180322105039-1.jpg','Возьмите вилку и проткните плод несколько раз со всех сторон.',1);
INSERT INTO recipeStageEntity VALUES(2907,1515,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180322105135-2.jpg','Положите его на тарелку, накрыв салфеткой или пластиковой крышкой, чтобы он не взорвался в микроволновой печи. Выберите программу 30 секунд, и если плод еще не достаточно размягчится, поставьте еще на 30 секунд.',2);
INSERT INTO recipeStageEntity VALUES(2915,1515,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180322105158-4.jpg','Подождите, пока авокадо немного остынет и нарежьте его, как обычно.',3);
INSERT INTO recipeStageEntity VALUES(3247,1551,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921140104-1.jpg','Смешайте вино, чеснок, травы, лимонный сок и оливковое масло в миске. Свинину нарежьте кубиками, добавьте в маринад и хорошо перемешайте. Накройте крышкой и уберите в холодильнике для маринирования на 1 часа.',1);
INSERT INTO recipeStageEntity VALUES(3251,1551,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921140108-2.jpg','Порежьте свинину, яблоки и перец кусочками примерно одного размера. Поочередно нанизывайте на шампуры: яблоко, свинину, перец.',2);
INSERT INTO recipeStageEntity VALUES(3255,1551,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921140112-3.jpg','Нагрейте сковороду-гриль на среднем огне. Жарьте шампуры партиями по 3 минуты с каждой стороны (или пока свинина не будет готова).',3);
INSERT INTO recipeStageEntity VALUES(3259,1551,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921140117-4.jpg','Поместите готовый шашлык на блюдо, украсьте листьями мяты и подавайте с салатом.',4);
INSERT INTO recipeStageEntity VALUES(3295,1563,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135746-1.jpg','Натрите свеклу и смешайте с  сахаром и бальзамическим уксусом в кастрюле. Нагрейте смесь на среднем огне до тех пор, пока сахар не растворится, и смесь дойдет до кипения. Уменьшите огонь и тушите в течение 25 минут или пока жидкость почти не испарится. Добавьте соль и остудите в течение 10 минут.',1);
INSERT INTO recipeStageEntity VALUES(3299,1563,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135751-2.jpg','Сварите грибы в течении 2-х минут до тех пор, пока они не станут мягкими.',2);
INSERT INTO recipeStageEntity VALUES(3303,1563,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135819-3.jpg','Нагреть большую антипригарную сковородку на среднем огне. Варите котлеты из говядины и обжарьте в течение 4 минут с каждой стороны до готовности. Уберите со сковороды и накройте фольгой.',3);
INSERT INTO recipeStageEntity VALUES(3307,1563,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135804-4.jpg','Поместите нижние булочки на сервировочные тарелки. Сверху положите листья салата, помидоры, нарезанные кольцами, котлеты, гриб и посыпатьте сыром. Наверх положите тушенную свеклы и верхнюю булочку. Немедленно подавайте.',4);
INSERT INTO recipeStageEntity VALUES(3383,1583,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135515-1.jpg','Горошины черного и душистого перца вместе с зирой насыпьте на сухую сковородку, прогрейте на среднем огне, 2–3 мин., потряхивая сковородку. Затем всыпьте специи в ступку и растолките.',1);
INSERT INTO recipeStageEntity VALUES(3387,1583,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135519-2.jpg','Снимите зелень со стеблей (стебли здесь не понадобятся). Мелко нарежьте листочки тимьяна и шалфея. Добавьте измельченные специи и перемешайте.',2);
INSERT INTO recipeStageEntity VALUES(3391,1583,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135525-3.jpg','Смажьте колбаски растительным маслом, обваляйте в специях. Оберните каждую полоской бекона. Для надежности закрепите полоски предварительно вымоченными в воде деревянными зубочистками.',3);
INSERT INTO recipeStageEntity VALUES(3395,1583,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135530-4.jpg','Уложите на смазанную маслом решетку и жарьте на среднем жару, периодически переворачивая, 5–8 мин., в зависимости от толщины сосисок (колбасок). Следите, чтобы грудинка не подгорела, а жир, капающий на угли, не вызвал появления пламени.',4);
INSERT INTO recipeStageEntity VALUES(3439,1603,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135332-1.jpg','Поместите лемонграсс, чили, имбирь, сахар и рыбный соус в небольшой кухонный комбайн и обработайте в грубую пасту. Добавьте растительное масло и перемешайте.',1);
INSERT INTO recipeStageEntity VALUES(3443,1603,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135337-2.jpg','Очистите креветки, оставляя хвосты, и смешайте с маринадом. Наденьте на шампуры вдоль. Накройте и охладите в течение 1 часа, чтобы наполнить ароматом.',2);
INSERT INTO recipeStageEntity VALUES(3455,1603,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135343-3.jpg','Разогрейте гриль или барбекю и готовьте креветки по 1-2 минуты с каждой стороны или до тех пор, пока они не будут готовы.  Подавать со свежим огурцом, мятой и лаймом.',3);
INSERT INTO recipeStageEntity VALUES(3503,1615,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135132-1.jpg','Разрежьте зубчики чеснока вдоль пополам и натрите ими каждый стейк с обеих сторон.',1);
INSERT INTO recipeStageEntity VALUES(3507,1615,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135138-2.jpg','Для маринада измельчите цедру лайма, перемешайте с остальными ингредиентами, затем залейте им мясо и положите в холодильник на 2 ч.',2);
INSERT INTO recipeStageEntity VALUES(3511,1615,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135143-3.jpg','Обсушите стейки бумажными полотенцами и жарьте на смазанной растительным маслом решетке мангала или барбекю до желаемой степени готовности, примерно по 4 мин. с каждой стороны для средней прожарки. Затем посолите, поперчите и подавайте.',3);
INSERT INTO recipeStageEntity VALUES(3515,1615,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921135150-4.jpg','СОВЕТ: если вы сделаете этот маринад в двойном объеме, то оставшуюся после готовки часть можно будет использовать в качестве соуса, предварительно прокипятив и добавив в него по вкусу соли и коричневого сахара.',4);
INSERT INTO recipeStageEntity VALUES(4416,1860,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921163932-0-0t1a7488.jpg',replace('Лук нужно почистить, вымыть и нарезать полукольцами.\n','\n',char(10)),1);
INSERT INTO recipeStageEntity VALUES(4420,1860,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921163932-1-0t1a7514.jpg',replace('В миске смешайте уксус, мёд и горчицу. \nДобавьте в миску нарезанный лук и маринуйте 10-15 минут. ','\n',char(10)),2);
INSERT INTO recipeStageEntity VALUES(4424,1860,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921163932-2-0t1a7506.jpg','Колбаски достаём из упаковки, моем и оборачиваем беконом.  Ставим в духовку( предварительно разогретую до 200 градусов) на 15-20 минут в зависимости от размера сосисок. ',3);
INSERT INTO recipeStageEntity VALUES(4428,1860,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20180921163932-3-sosiski-650h470.jpg',replace('Булочки надрезаем по длинной стороне, чтобы получить кармашек и поджариваем срезом вниз. \nПомещаем внутрь колбаску и маринованный лук.\nПодаем к столу горячим. ','\n',char(10)),4);
INSERT INTO recipeStageEntity VALUES(4576,1892,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20181002161124-0-1.eskiz-.jpg',replace('Шаг 1. \nРешите какая сторона тыквы будет лицевой. \nНарисуете эскиз лица можно от руки или распечатайте эскиз и нанесите на тыкву. ','\n',char(10)),2);
INSERT INTO recipeStageEntity VALUES(4580,1892,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20181002161124-1-2.zadniaia-stenka.jpg',replace('Шаг 2. Ровно с противоположной стороны наметьте прямоугольник для отверстия, через который вы будете вычищать тыкву. Не делайте его слишком маленьким или большим. Через него без труда должна проходит ваша рука.\n','\n',char(10)),3);
INSERT INTO recipeStageEntity VALUES(4584,1892,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20181002161124-2-3.vnutrennosti.jpg',replace('Шаг 3. Вычищаем тыкву изнутри. \nВажно, чтобы внутри не осталось ткани и семян.','\n',char(10)),4);
INSERT INTO recipeStageEntity VALUES(4588,1892,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20181002161124-3-4.glazik.jpg',replace('Шаг 4. При помощи острого ножа / скальпеля /  долото начинаем вырезать лицо нашего Джека. \nСначала глаза.\nСоветуем выбрать эскиз в большим расстоянием между элементами. ','\n',char(10)),5);
INSERT INTO recipeStageEntity VALUES(4592,1892,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20181002161124-4-6.glaza-i-ulybka-obshii-plan-.jpg','Шаг 5. Аккуратно вырезаем оскал или улыбку (тут уже как вы решили). Старайтесь не сильно надавливать на кожуру, так как она от резких усилий может проломиться. ',6);
INSERT INTO recipeStageEntity VALUES(4596,1892,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20181002161124-5-7.finalochka-.jpg',replace('Шаг 6. Внутрь помещаем чайную свечу!\nС Днём Всех Святых!','\n',char(10)),846);
INSERT INTO recipeStageEntity VALUES(4600,1892,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20181002163832-7.finalochka-.jpg',replace('А почему вырезают тыквы на Halloween?\nНа самом деле история невероятно интересная.\nЭта традиция пришла не из США, как принято думать, а из Ирландии. \nПравда там местные резали репу и брюкву. \nВ Ирландском фольклоре есть легенда о пьянице-кузнице Джеке, который дважды обманул самого Дьявола. \nПервый раз встретившись с неожиданным гостем Джек предложил ему выпить в таверне. Когда пришло времени оплаты, он попросил собутыльника обратиться монетой, дабы расплатиться. Дьявол так и сделал, однако угодил к обманщику в карман, где был запрятан серебряный крестик. Дьявол попал в ловушку и был вынужден за свободу пообещать Джеку не чинить ему козни и не забирать ещё год. \nВторой раз они повстречались у фруктового сада. В этот раз Джек согласился последовать в Преисподнюю, но напоследок хотел бы съесть яблоко с верхушки ближайшего дерева. Дьявол полез за яблоком, и снова оказался в ловушке, ведь Джек выхватил нож и вырезал на стволе крест. Ещё 10 лет выторговал себе обманщик, а главное Дьявол не мог теперь забрать его в Ад. \nВскоре Джек умер, но Райские врата не открылись для него, а Ад был закрыт из-за договора с самим Дьяволом. \nВот и вынужден он теперь навсегда скитаться по земле, освещая себе дорогу фонарем из тыквы, что была рядом с местом, где он умер, да угольком, что напоследок ему кинул Дьявол. \n\n','\n',char(10)),1);
INSERT INTO recipeStageEntity VALUES(6255,2275,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190418182727-zqwj-dct.jpg',replace('Нам понадобится как можно больше луковой шелухи.\nЯйца можно взять белые или коричневые, на ваш выбор. Главное, чтобы они были комнатной температуры. \n','\n',char(10)),1);
INSERT INTO recipeStageEntity VALUES(6259,2275,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190418181815-1-650-yf-470.jpg','Шелуху от лука собираем и укладываем в широкую кастрюлю, заливаем чистой водой. Оставляем настаиваться не менее чем на полчаса, после чего можно ставить кастрюлю на плиту. Когда вода закипит, убавляем огонь и даём повариться порядка двадцати минут. После этого снимаем кастрюлю с плиты и оставляем остывать. Затем процеживаем отвар, полностью удалив из него луковую шелуху. ',2);
INSERT INTO recipeStageEntity VALUES(6263,2275,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190418181816-2-650h470_iaicovmarle.jpg','На яйцо помещаем листики трав. Можно по одному или составлять композиции. Также можно использовать канцелярский скотч для создания рисунка или нитки. Яйцо с листиками оборачиваем марлей, можно использовать капроновые колготки или носки.',3);
INSERT INTO recipeStageEntity VALUES(6267,2275,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190418181816-3-650h470_iaico-v-vode.jpg','Во время приготовления периодически переворачиваем яйца с помощью ложки, чтобы они качественно и равномерно прокрасились со всех сторон. Когда яйца сварятся, заливаем их холодной водой и даём полностью остыть. Затем протираем хлопчатобумажным полотенцем и, по сути, всё готово. Далее по желанию яйца можно смазать подсолнечным маслом.',4);
INSERT INTO recipeStageEntity VALUES(6271,2275,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190418181816-4-650h470_final.jpg',replace('Полезный совет: Цвет зависит от количества шелухи. \nЕсли использовать красный лук или ялтинский, цвет получится более бордовым или фиолетовым.','\n',char(10)),5);
INSERT INTO recipeStageEntity VALUES(6300,2284,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190422182831-0-kusochki.jpg','Курицу промываем и просушиваем. Нарезаем на небольшие кусочки. ',1);
INSERT INTO recipeStageEntity VALUES(6304,2284,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190422182831-1-v-marinade.jpg','Готовим маринад: смешиваем 1 ч.л. парики, 1 ч.л. куркумы, 1 ч.л. кинзы, пюре из 2 зубчиков чеснока, соль, чёрный  перец и 3 ст.л. оливкового масла. Можно добавить перец чили, если хотите, чтобы сате получилось острее. Курицу хорошенько перемешиваем в получившейся смеси и ставим мариноваться от получаса до 4 часов.',2);
INSERT INTO recipeStageEntity VALUES(6308,2284,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190422182831-2-sous.jpg',replace('Пока мясо маринуется приготовим идеальный соус. \n3 ст.л. оливкового масла ставим в сотейнике на огонь. Измельчаем луковицу и имбирь (как можно мельче). Когда масло нагреется добавляем в него лук и имбирь - всё перемешиваем. \nДалее специи: паприка, куркума и кинза по 1 чайной ложке, 2 зубчика чеснока, соль, чёрный перец - по вкусу. \nКогда лук станет мягким, вливаем 200 мл кокосового молока и винный уксус. Готовим на сильном огне около 10 минут, постоянно помешивая, пока смесь не станет сметанообразной. ','\n',char(10)),3);
INSERT INTO recipeStageEntity VALUES(6312,2284,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190422182831-3-sate-650-na-470.jpg',replace('Когда мясо готово, насаживаем его на деревянные шпажки  и отправляем  на гриль или гриль-сковороду. \nВ первом случае, жарим при 200 градусах, примерно, 10-12 минут. На сковороде - около 15-20 минут до полной готовности. Подавать с соусом. ','\n',char(10)),4);
INSERT INTO recipeStageEntity VALUES(6316,2288,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190422185810-0-zelen.jpg','Смешиваем зелень со специями. Лимоны нарезаем на половинки. ',1);
INSERT INTO recipeStageEntity VALUES(6320,2288,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190422185810-1-na-skovorode.jpg','Форель потрошим, очищаем от чешуи. Натираем рыбу солью и перцем, сбрызгиваем лимонным соком, делаем на поверхности небольшие разрезы ножом и смазываем форель оливковым маслом. Внутрь кладем смесь из зелени и половинку тархуна или листья тархуна. ',2);
INSERT INTO recipeStageEntity VALUES(6324,2288,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190422185810-2-kartoha.jpg','Картошку чистим, нарезаем на круги примерно 3 мм толщиной, выкладываем на гриль и присылаем специями по вкусу.',3);
INSERT INTO recipeStageEntity VALUES(6328,2288,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190422185810-3-rybka.jpg',replace('Через полчаса выкладываем рыбу на решетку и обжарьте ее по 4 минуты с каждой стороны.\nМожно использовать гриль-сковороду, приготовление займет на 5 минут подольше. ','\n',char(10)),4);
INSERT INTO recipeStageEntity VALUES(6343,2296,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190423124810-0-kruzhochki-baklazhanov.jpg','Баклажан моем, нарезаем на кружочки толщиной, примерно, 5 мм, солим и перчим. ',1);
INSERT INTO recipeStageEntity VALUES(6346,2296,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190423124810-1-zelen.jpg','Зелень и лук мелко шинкуем.',2);
INSERT INTO recipeStageEntity VALUES(6349,2296,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190423124810-2-na-skovorode.jpg','Баклажаны можно обжарить на гриль или гриль-сковороде. Выкладываем кружочки и немного приправляем перцем. Обжариваем по 2-3 минуты с каждой стороны. Разрезаем булочку пополам и чуть поджариваем половинки.',3);
INSERT INTO recipeStageEntity VALUES(6352,2296,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190423124810-3-sous.jpg','Зелень и лук смешиваем с соевым йогуртом или пастой. ',4);
INSERT INTO recipeStageEntity VALUES(6355,2296,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190423124810-4-final-650-na-470.jpg',replace('Собираем бургер. \nПоловинку булочки смазываем небольшим количеством соуса, кладём листик салата, сверху обжаренные баклажаны и  поливаем соусом. Можно повторить еще раз. Накрываем второй половинкой булочки.','\n',char(10)),5);
INSERT INTO recipeStageEntity VALUES(8829,2837,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190712170407-0-1a7a6640.jpg','Смешайте в глубокой миске: злаки, клюкву, дробленый микс орехов, кокосовую стружку и шоколадные чипсы. Засыпьте ореховую муку.',1);
INSERT INTO recipeStageEntity VALUES(8833,2837,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190712170407-1-1a7a6648.jpg','Смешайте масло с медом. Взбейте его венчиком. Введите в ореховую смесь и тщательно перемешайте. ',2);
INSERT INTO recipeStageEntity VALUES(8837,2837,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190712170407-2-1a7a6795.jpg','Противень застелите пергаментом и разложите смесь ровным слоем, примерно 5-7 мм толщиной. ',3);
INSERT INTO recipeStageEntity VALUES(8841,2837,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20190712170407-3-1a7a6912.jpg',replace('Запекайте 15-20 минут при температуре 180 градусов. \n','\n',char(10)),4);
INSERT INTO recipeStageEntity VALUES(15234,4238,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407115253-0-grecha.jpg','Гречку отварите в подсоленной воде до готовности. ',1);
INSERT INTO recipeStageEntity VALUES(15238,4238,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407115253-1-luk-i-kartoshka.jpg','Нарежьте лук полукольцами, картофель некрупной соломкой или кубиками, чеснок измельчите ножом. В глубокую сковороду налейте растительное масло, выложите овощи и обжаривайте на среднем огне 5-7 мин. Влейте бульон и поставьте на слабый огонь и варите до готовности картофеля.',2);
INSERT INTO recipeStageEntity VALUES(15242,4238,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407115253-2-skovoroda.jpg','Добавьте гречку, посолите, поперчите, влить сливки, прогреть на огне 3-4 минуты.',3);
INSERT INTO recipeStageEntity VALUES(15246,4238,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407115253-3-krem-sup.jpg','Всё измельчите в блендере.',4);
INSERT INTO recipeStageEntity VALUES(15250,4238,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407115253-4-pechen.jpg','Печень промойте, срежьте всё лишнее, посолите, поперчите и поджарьте до готовности на сливочном масле.',5);
INSERT INTO recipeStageEntity VALUES(15254,4238,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407115254-5-final.jpg','Суп из этого количества продуктов получится очень густым. Если вы любите более жидкий вариант, добавьте в суп  бульон. Украсьте зеленью. ',6);
INSERT INTO recipeStageEntity VALUES(15278,4246,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407145741-0-muka.jpg','Муку просейте через сито, добавьте дрожжи, ванилин и все перемешайте.',1);
INSERT INTO recipeStageEntity VALUES(15282,4246,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407145741-1-iaico.jpg','Влейте в другую миску молоко, 2 яйца, соль и сахар и всё хорошо перемешайте.',2);
INSERT INTO recipeStageEntity VALUES(15286,4246,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407145741-2-opara-s-mem.jpg','Добавьте изюм и 2/3 драже M&amp;M`s. Накройте тесто полотенцем и оставьте на 1 час.',3);
INSERT INTO recipeStageEntity VALUES(15290,4246,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407145741-3-testo-v-forme-podoshlo.jpg','Разделите тесто на равные части. каждую часть скатайте в шар и положите в смазанные маслом формы. Снова оставьте на 30 минут подниматься.',4);
INSERT INTO recipeStageEntity VALUES(15294,4246,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200407145741-4-final.jpg',replace('Выпекайте в разогретой до 190 градусов духовке примерно 30 минут. Выньте куличи из духовки и дайте им остыть. \nПриготовьте глазурь. Смешайте сахарную пудру, лимонный сок и вбитые белки 3 яиц. Покройте глазурью куличи и посыпьте их оставшимися драже.','\n',char(10)),5);
INSERT INTO recipeStageEntity VALUES(15414,4278,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413150118-0-shafran.jpg','Шафран замочите в 50 мл кипятка. ',1);
INSERT INTO recipeStageEntity VALUES(15418,4278,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413150118-1-zamochili.jpg','Клюкву залейте кипятком.',2);
INSERT INTO recipeStageEntity VALUES(15422,4278,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413150118-2-smes.jpg','Приготовьте опару. Дрожжи разведите в тёплом молоке (33-35 °С). Добавьте 150 г муки и 50 г сахара. Все перемешайте и поставьте на 60-90 минут в тёплое место. Когда опара начнет опадать, значит, она готова.',3);
INSERT INTO recipeStageEntity VALUES(15426,4278,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413150118-3-pena.jpg','Желтки разотрите с сахаром добела. Белки взбейте до пены. Натрите цедру лимона.',4);
INSERT INTO recipeStageEntity VALUES(15430,4278,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413150118-4-testo.jpg','В готовую опару добавьте все оставшиеся ингредиенты, кроме масла и клюквы. Замесите тесто и постепенно введите мягкое масло. Замесите эластичное, слегка прилипающее к рукам, тесто. Отправьте на расстойку на 1,5 часа. За время расстойки сделайте 2 обминки. ',5);
INSERT INTO recipeStageEntity VALUES(15434,4278,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413150119-5-iagody.jpg','Затем в расстоявшееся тесто введите отжатую клюкву, слегка обваленную в муке. ',6);
INSERT INTO recipeStageEntity VALUES(15438,4278,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413150119-6-v-forme.jpg','Снова замесите тесто, разделите на нужное количество частей и разложите по формам. Заполните форму на 1/3. Оставьте подниматься на 40-60 минут.',7);
INSERT INTO recipeStageEntity VALUES(15442,4278,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413150119-7-final.jpg','Выпекайте при температуре 180 °С в течение 35-40 минут. Перед украшением глазурью кулич следует остудить.',8);
INSERT INTO recipeStageEntity VALUES(15448,4283,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413195303-0-1.jpg','Сначала подготовьте форму для выпечки. Если у вас нет нужной, то можно использовать форму для кекса. Растопите масло. ',1);
INSERT INTO recipeStageEntity VALUES(15453,4283,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413195303-1-2.jpg','Разбейте 3 яйца в отдельную миску, но белок от одного яйца вылейте в отдельный сосуд. Он нам пригодится для глазури.',2);
INSERT INTO recipeStageEntity VALUES(15458,4283,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413195303-2-3.jpg','К яйцам добавьте сахар, затем взбейте миксером до однородной массы и влейте кефир.',3);
INSERT INTO recipeStageEntity VALUES(15463,4283,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413195303-3-4.jpg','Добавьте в миску растопленное масло, а также соду с ванилином и опять взбейте миксером. ',4);
INSERT INTO recipeStageEntity VALUES(15468,4283,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413195303-4-5.jpg','Всыпьте просеянную муку и хорошо перемешайте.',5);
INSERT INTO recipeStageEntity VALUES(15473,4283,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413195303-5-6.jpg','Переправьте готовое тесто в форму для кулича. Выпекайте при 180 градусах в течение 45 минут. Остудите и покройте сахарной глазурью.',6);
INSERT INTO recipeStageEntity VALUES(15478,4288,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413202530-0-keshiu.jpg','Орехи замолчите на ночь. Если нет времени - поварите в течение 15 минут. ',1);
INSERT INTO recipeStageEntity VALUES(15483,4288,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413202530-1-slivki-smes.jpg','В блендере измельчите орехи. Добавьте сливки, сироп, соль и масло. Все смешайте.',2);
INSERT INTO recipeStageEntity VALUES(15488,4288,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413202530-2-smes.jpg','Добавьте какао-порошки, ванильную эссенцию и всё еще раз взбейте. Масса должна быть без комочков. ',3);
INSERT INTO recipeStageEntity VALUES(15493,4288,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200413202530-3-v-forme.jpg','Форму подготовьте, застелите пищевой пленкой. Можно добавить ягоды или сухофрукты. Переложите массу в форму и уберите на 2 часа или на ночь в холодильник. ',4);
INSERT INTO recipeStageEntity VALUES(15498,4293,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414202719-0-1.jpg',replace('Творог протрите через мелкое сито.\nДобавьте желтки, сливочное масло, сахарную пудру и ванильный сахар. Заварите на водяной бане до t 80°С.\nПеремешайте.','\n',char(10)),1);
INSERT INTO recipeStageEntity VALUES(15503,4293,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414202719-1-2.jpg',replace('В заваренный творог добавьте шоколад  и перемешайте до однородности. Затем остудите смесь до комнатной температуры.\n','\n',char(10)),2);
INSERT INTO recipeStageEntity VALUES(15508,4293,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414202719-2-3.jpg','Взбейте сливки и введите в творожную массу. Форму для пасхи застелите мокрой марлей и поставьте на блюдце верхушкой вниз. ',3);
INSERT INTO recipeStageEntity VALUES(15513,4293,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414202719-3-4.jpg',replace('Внутрь формы плотно утрамбуйте творожную смесь. Закройте края марли. Сверху можно положить пресс.\nУберите в холодильник на 5-10 часов\nАккуратно извлеките застывшую пасху из формы. \n.','\n',char(10)),4);
INSERT INTO recipeStageEntity VALUES(15518,4293,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414202719-4-5.jpg','Растопите молочный шоколад и смешайте его с 1 ст. л. сливок. Полейте пасху сверху. Готово!',5);
INSERT INTO recipeStageEntity VALUES(15523,4298,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414220426-0-shutterstock_1606653118.jpg',replace('Яйца отварите вкрутую. В небольшом объеме теплой воды (от 200 до 700 мл) растворите краситель. Добавите  в каждую емкость по 1 ч. л. столового уксуса и хорошо перемешайте. (Это зависит от красителя, лучше уточнить на упаковке).\n','\n',char(10)),1);
INSERT INTO recipeStageEntity VALUES(15528,4298,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414220426-1-shutterstock_1663416538.jpg',replace('Аккуратно с помощью столовой ложки в каждую емкость выложите по одному яйцу. Чем дольше яйца находятся в растворе с краской, тем ярче они окрасятся.\nДостаньте яйца, когда вам понравится цвет, и выложите на бумажное полотенце. Дайте обсохнуть.','\n',char(10)),2);
INSERT INTO recipeStageEntity VALUES(15533,4298,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414220426-2-shutterstock_260747420.jpg',replace('Есть очень полезный лайфхак, как избежать разводов от краски. Погружайте яйцо в раствор при помощи обычног металлического венчика. Внутрь поместите яйцо и опустите в емкость. \nТакже после окрашивания можно опускать яйцо не на салфетку, а на подставку, собранную из булавок. Воткните булавки по краям квадрата со стороной 2 см в плотную губку или картон. ','\n',char(10)),3);
INSERT INTO recipeStageEntity VALUES(15538,4303,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414234545-0-opara.jpg','Разведите дрожжи с медом в 150 мл теплой воды, добавьте немного муки. Замесите опару, накройте полотенцем и поставьте в теплое место на 20 минут.',1);
INSERT INTO recipeStageEntity VALUES(15543,4303,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414234545-1-kastrblia.jpg','250 мл молока подогрейте, добавьте в него сливочное масло. Яйцо слегка взбейте.',2);
INSERT INTO recipeStageEntity VALUES(15548,4303,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414234545-2-testo-podoshlo.jpg','Добавьте в опару молоко с маслом, яйцо, введите оба вида муки и соль. Замесите тесто и оставьте его в теплом месте на 2 часа.',3);
INSERT INTO recipeStageEntity VALUES(15553,4303,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414234545-3-kolbasa.jpg','Разделите тесто на 4 части. Раскатайте каждую часть в прямоугольник и сверните его трубочкой.',4);
INSERT INTO recipeStageEntity VALUES(15558,4303,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414234545-4-kosa.jpg','Скрепите получившиеся колбаски у основания. Сплетите косу, перекладывая колбаски через две, и оставите на полчаса. Желток взбейте с оставшимся молоком, смажьте тесто, присыпьте маком.',5);
INSERT INTO recipeStageEntity VALUES(15563,4303,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200414234545-5-final.jpg','Выпекайте халу 10 минут при температуре 200°С в предварительно разогретой духовке, затем убавьте температуру до 180°С и выпекайте еще 25–30 минут.',6);
INSERT INTO recipeStageEntity VALUES(15570,4310,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418133113-0-smes-1.jpg','Дрожжи разведите в тёплом молоке с ч. л. сахара и поставьте в тёплое место на 10-15 минут.',1);
INSERT INTO recipeStageEntity VALUES(15574,4310,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418133113-1-testo-izna.jpg','Одно яйцо и 2 желтка взбейте с солью, обычным и ванильным сахаром (100 г)  до белой густой пены. Сливочное масло (40г) растопите. Муку просейте с куркумой и мускатным орехом. Влейте в муку готовые дрожжи, яичную смесь и перемешайте.',2);
INSERT INTO recipeStageEntity VALUES(15578,4310,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418133113-2-testo-smes.jpg','Затем добавьте сливочное масло и вымешивайте тесто 10-15 минут, пока оно не станет отлипать от поверхностей и от рук.',3);
INSERT INTO recipeStageEntity VALUES(15582,4310,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418133113-3-testo.jpg',replace('Скруглите, обмажьте сливочным маслом и положите в миску, так же смазанную маслом.\nНакрыть пищевой плёнкой и поставьте подходить в тёплое место на полтора часа.','\n',char(10)),4);
INSERT INTO recipeStageEntity VALUES(15586,4310,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418133113-4-nachinka.jpg','Заварите мак, затем смешайте с молоком и сахаром, прогрейте, помешивая и отставьте набухать. Затем остывший мак перемешайте со сливочным маслом (120г) комнатной.',5);
INSERT INTO recipeStageEntity VALUES(15590,4310,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418133113-5-smazali.jpg',replace('Подошедшее тесто обомните, разделите на 4 части, скруглите каждую, накройте плёнкой и оставите на столе, на 10 минут. Затем каждую раскатайте в прямоугольник. \nОчень тонко! Намажьте 1/4 смесью мака со сливочным маслом и посыпьте цукатами 1/4 от общего количества.','\n',char(10)),6);
INSERT INTO recipeStageEntity VALUES(15594,4310,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418133113-6-svernuli.jpg',replace('Завернуите тесто плотно в рулет.\nНакройте плёнкой, пока тоже самое проделываете с остальными частями. Теперь, разрежьте рулет вдоль по длине, но не до конца.  Скрутите спиралькой в башенку, разрезом наружу и уложите в форму для кулича.','\n',char(10)),7);
INSERT INTO recipeStageEntity VALUES(15598,4310,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418133113-7-final.jpg',replace('Дайте подойти в течение 25-30 минут в теплом месте.\nВ это время хорошо прогреть духовку до 200°С.\nПодошедшие куличи установить на противень.\nНакройте смятой фольгой. Выпекайте около 30 минут, За 10 минут до конца снизить температуру до 170°С. Украсьте по вкусу, когда кулич остынет. ','\n',char(10)),8);
INSERT INTO recipeStageEntity VALUES(15658,4322,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418144808-0-kubiki.jpg','Кулич нарежьте кубиками. ',1);
INSERT INTO recipeStageEntity VALUES(15662,4322,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418144808-1-krem.jpg','Сметану взбейте с сахарной пудрой. ',2);
INSERT INTO recipeStageEntity VALUES(15666,4322,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200418144808-2-pod-kremom.jpg','В тарелку или миску для подачи смешайте кубики кулича, ягоды и полейте все кремом. Уберите на 3-4 часа в холодильник. Вкусный десерт готов!',3);
INSERT INTO recipeStageEntity VALUES(15785,4353,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200430160301-0-iaica.jpg','Отварите яйца и мелко нарежьте.',1);
INSERT INTO recipeStageEntity VALUES(15789,4353,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200430160301-1-ogurcy.jpg','Натрите огурцы на крупной терке. ',2);
INSERT INTO recipeStageEntity VALUES(15793,4353,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200430160302-2-svekla.jpg','Отварите свеклу, дайте слегка остыть, затем натрите на крупной терке. ',3);
INSERT INTO recipeStageEntity VALUES(15797,4353,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200430160302-3-kolbasa.jpg','Нарежьте вареную колбасу кубиками. ',4);
INSERT INTO recipeStageEntity VALUES(15801,4353,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200430160302-4-kefir.jpg','Сложите все ингредиенты в кастрюлю и добавьте кефир. Хорошо размешайте. ',5);
INSERT INTO recipeStageEntity VALUES(15805,4353,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200430160302-5-zelen.jpg','Мелко нарежьте зелень и добавьте в суп.',6);
INSERT INTO recipeStageEntity VALUES(15809,4353,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200430160302-6-final.jpg','Суп готов!',7);
INSERT INTO recipeStageEntity VALUES(15977,4397,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601112515-0-iaico.jpg','Аккуратно разбейте яйцо в мерный стакан, не повредив целостность желтка. Влейте в воронку, благодаря ней белок обволакивает желток и получается классическая форма.',2);
INSERT INTO recipeStageEntity VALUES(15981,4397,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601112515-1-voronka.jpg','Воду в кастрюле поставьте на огонь. Добавьте уксус, а затем соль и перец по вкусу. Когда на поверхности появятся первые пузырьки, при помощи вилки или венчика создайте воронку.',1);
INSERT INTO recipeStageEntity VALUES(15985,4397,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601112515-2-final.jpg',replace('Варите 3-4 минуты. \nСекрет яйца-пашот, в сваренном белке, но при этом жидкой кремовой консистенции желтка. ','\n',char(10)),3);
INSERT INTO recipeStageEntity VALUES(15989,4401,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601152650-0-iaica.jpg',replace('Яйца промойте. Положите в кастрюлю и залейте водой.\nПосолите. ','\n',char(10)),1);
INSERT INTO recipeStageEntity VALUES(15993,4401,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601152650-1-final.jpg',replace('Когда вода закипит, варите еще 10 минут. \nЯйца вкрутую готовы. ','\n',char(10)),2);
INSERT INTO recipeStageEntity VALUES(15997,4405,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601154822-0-iaica.jpg','Яйца промойте и залейте водой, добавьте щепотку соли. ',1);
INSERT INTO recipeStageEntity VALUES(16001,4405,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601154822-1-final.jpg','Доведите до кипения и варите еще 3-4 минуты. ',2);
INSERT INTO recipeStageEntity VALUES(16005,4409,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601161924-0-iaica.jpg','Яйца хорошо промойте. ',1);
INSERT INTO recipeStageEntity VALUES(16009,4409,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601161924-1-gotovye-iaica.jpg','Залейте водой и поставьте на огонь. В воду можно добавить соль или немного уксуса. Доведите до кипения и после варите еще 5-6 минут.',2);
INSERT INTO recipeStageEntity VALUES(16013,4409,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200601161924-2-final.jpg','Яйца в мешочке готовы.',3);
INSERT INTO recipeStageEntity VALUES(16313,4497,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630192157-0-1.jpg','Филе индейки нарежьте кубиками и обжарьте на растительном масле 5 минут. Добавьте в сковороду овощную смесь и кукурузу и тушите на медленном огне 1 минуту. Добавьте сметану, зелень и посолите по вкусу.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(16317,4497,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630192157-1-2.jpg','Перец разрежьте вдоль на половинки, очистите от сердцевины, выложите на противень или в форму для выпечки.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(16321,4497,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630192157-2-3.jpg','В половинки-кораблики положите обжаренную смесь, сверху - половинки черри.<br> <br> ',3);
INSERT INTO recipeStageEntity VALUES(16325,4497,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630192157-3-4.jpg','Посыпьте фаршированные перцы тертым сыром.',4);
INSERT INTO recipeStageEntity VALUES(16329,4497,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630192157-4-5.jpg','Запекайте в духовке при температуре 200 градусов 15-20 минут.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(16333,4501,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630193621-0-1.jpg','Посолите рыбу и обжарьте на оливковом масле по 1-2 минуты с каждой стороны.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(16337,4501,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630193621-1-2.jpg','Обжарьте галеты «Звездное трио» на медленном огне с каждой стороны по 5-6 минут на оливковом масле.',2);
INSERT INTO recipeStageEntity VALUES(16341,4501,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630193621-2-3.jpg','<p>Выложите рыбу и галеты на тарелку, украсьте базиликом и долькой лимона. Подавайте с йогуртом, предварительно добавив в него соль по вкусу. <br> </p>',3);
INSERT INTO recipeStageEntity VALUES(16345,4505,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630194605-0-1.jpg','Яйца выложите в глубокую металлическую миску и слегка взбейте венчиком. Добавьте сахар, лимонный сок и еще раз все перемешайте. Поставьте миску на водяную баню и готовьте до консистенции жидкой сметаны, постоянно помешивая.<br> <br> ',1);
INSERT INTO recipeStageEntity VALUES(16349,4505,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630194605-1-2.jpg','Снимите миску с водяной бани, добавьте масло и натертую цедру. Хорошо перемешайте и остудите до комнатной температуры. Смешайте с кукурузой и уберите в холодильник на 30 минут.<br> <br> ',2);
INSERT INTO recipeStageEntity VALUES(16353,4505,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630194605-2-3.jpg','Раскатайте тесто толщиной 0,5 см и выложите его в формочки для выпекания тарталеток. Выпекайте в разогретой до 180°С духовке в течение 25 минут.<br> <br> ',3);
INSERT INTO recipeStageEntity VALUES(16357,4505,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630194605-3-4.jpg','Готовые тарталетки остудите и достаньте из формочек.',4);
INSERT INTO recipeStageEntity VALUES(16361,4505,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630194605-4-5.jpg','<p>Лимонно-кукурузный крем хорошо перемешайте и наполните им тарталетки.</p>',5);
INSERT INTO recipeStageEntity VALUES(16365,4509,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630195846-0-1.jpg','Охлажденные сливки взбейте до легкой пены с сахарной пудрой, затем аккуратно смешайте с мягким творогом.<br> <br> ',1);
INSERT INTO recipeStageEntity VALUES(16369,4509,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630195846-1-2.jpg','Крупно поломайте печенье. В креманку слоями выкладывайте сливочную смесь, кукурузу, печенье.<br> <br> ',2);
INSERT INTO recipeStageEntity VALUES(16373,4509,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200630195846-2-3.jpg','Сверху посыпьте измельченным печеньем.Украсьте листиком мяты.<br> <br> ',3);
INSERT INTO recipeStageEntity VALUES(16433,4525,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708192723-0-shag1.jpg','<p>Творог протираем столовой ложкой через сито до получения воздушной массы. <i>Включаем духовку – разогреваем до 180 градусов (режимы «Для выпечки» или «Верхний и нижний жар». Не используем «Гриль»).</i></p>',1);
INSERT INTO recipeStageEntity VALUES(16437,4525,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708192723-1-shag2.jpg','<p>Поочередно постепенно добавляем в творожную массу сахар, рисовую муку и яйцо. Аккуратно перемешиваем массу лопаткой, чтобы не потерять воздушной консистенции.</p>',2);
INSERT INTO recipeStageEntity VALUES(16441,4525,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708192723-2-shag3.jpg','<p>Из полученной творожной массы формуем шарики приблизительно по 50 г – или, как вам больше нравится, но не слишком маленькие, чтобы они не стали сухими после запекания.</p>',3);
INSERT INTO recipeStageEntity VALUES(16445,4525,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708192723-3-shag4.jpg','<p>Шарики панируем в рисовой муке и приминаем их руками с двух сторон - формуем сырники. Выкладываем сырники на застеленный пергаментной бумагой противень. <i>Запекаем на противне 20 минут при температуре 180 гр. До золотистой корочки.</i></p>',4);
INSERT INTO recipeStageEntity VALUES(16449,4529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708193948-0-granola_shag1.jpg','Для приготовления домашней гранолы смешиваем овсяные хлопья, мед, орехи и растительное масло.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(16453,4529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708193948-1-granola_shag2.jpg','<p>Выкладываем смесь тонким слоем на выстланный пергаментной бумагой противень и запекаем 15 минут при температуре 180 градусов. <i>Полностью остужаем гранолу и храним ее в бумажном пакете или банке для круп.</i></p>',2);
INSERT INTO recipeStageEntity VALUES(16457,4529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708193948-2-granola_shag3.jpg','<p>Для малинового пюре берем свежую или замороженную малину и взбиваем ее в блендере с сахарной пудрой – пропорции по вкусу.</p>',3);
INSERT INTO recipeStageEntity VALUES(16461,4529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708193948-3-granola_shag4.jpg','<p>На одну порцию утреннего нескучного завтрака берем примерно 2 ст.л гранолы, йогурт и малиновое пюре и (или) сезонные ягоды. Выкладываем слоями в высоком прозрачном бокале.</p>',4);
INSERT INTO recipeStageEntity VALUES(16465,4533,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708200743-0-naggetsy-iz-indeiki-shag01.jpg','<p>Филе индейки (для запекания все-таки лучше брать филе бедра) нарезать брусочками примерно 3 х 2 см. Выложить в миску, посолить, добавить простоквашу и перемешать. <i>Убираем в холодильник минимум на один час, чтобы индейка хорошенько промариновалась. Включаем духовку и разогреваем до 180 г.</i></p>',1);
INSERT INTO recipeStageEntity VALUES(16469,4533,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708200744-1-naggetsy_iz_indeiki_shag02.jpg','<p>Достаем по очереди каждый кусочек из маринада, панируем в кукурузной муке и выкладываем на противень, покрытый пергаментной бумагой. <i>Запекаем 15 минут в духовке при температуре 180 градусов в режимах «Верхний и нижний жар» или «Для выпечки».</i></p>',2);
INSERT INTO recipeStageEntity VALUES(16473,4533,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708200744-2-naggety_iz_indeiki_shag3.jpg','<p>Пока наггетсы запекаются, отвариваем капусту согласно инструкции на упаковке. Главное – не переварить, разноцветные «деревца» должны быть крепенькими и упругими. Сливаем воду, при желании, добавляем небольшое количество сливочного масла.</p>',3);
INSERT INTO recipeStageEntity VALUES(16477,4533,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200708200744-3-naggetsy-iz-indeiki-shag-4.jpg','<p>Подаем наггетсы со сметаной и «лесом» разноцветной капусты.</p>',4);
INSERT INTO recipeStageEntity VALUES(16481,4537,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709085512-0-salat_shag1.jpg','<p>Зелёный горошек достать из баночки, дать воде стечь. Перед подачей заправить оливковым маслом. Стручковую фасоль бланшировать в соленой воде при слабом кипении до готовности в течение 1 - 2 минут. Шоково охладить в холодной воде со льдом, откинуть на сито и дать жидкости стечь. </p>',1);
INSERT INTO recipeStageEntity VALUES(16485,4537,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709085513-1-salat_shag2.jpg','<p>Киноа отварить согласно инструкции на упаковке, добавить соль, лимонный сок, оливковое масло и мелко нарубленную мяту. Перемешать.</p>',2);
INSERT INTO recipeStageEntity VALUES(16489,4537,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709085513-2-salat_shag3.jpg','<p>Листья шпината вымыть в проточной воде и откинуть на сито, дать воде стечь. Мелкие листья выложить на большую порционную тарелку целиком, крупные - разорвать пополам. Очищенный огурец разрезать вдоль пополам, затем нарезать поперек под прямым углом на дольки толщиной 5 мм. Авокадо разрезать пополам, удалить косточку. Одну половину аккуратно очистить от кожуры.</p>',3);
INSERT INTO recipeStageEntity VALUES(16493,4537,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709085513-3-salat_shag4.jpg','Готовим соус и собираем салат: Тархун измельчаем ножом. Огурец натираем на мелкой терке. Тщательно смешивает йогурт с тархуном и огурцом, солим и заправляем лимонным соком по вкусу. Соль и сок добавляем постепенно. Добавляем – и пробуем – снова добавляем.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(16497,4541,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709090009-0-frikadelki-shag1.jpg','Чтобы приготовить фрикадельки, как в АндерСоне, необходимо: пропустить индейку и луковицу через мясорубку, добавить к фаршу яйцо и посолить.',1);
INSERT INTO recipeStageEntity VALUES(16501,4541,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709090010-1-frikadelki-shag2.jpg','<p>Вылепить маленькие кругленькие аккуратные фрикадельки. Взять небольшую узкую прямоугольную разделочную доску, накрыть ее пищевой пленкой и слегка присыпать мукой. Выложить фрикадельки на доске так, чтобы они не соприкасались друг с другом.</p><p></p><p><i>Поставить доску в морозильную камеру на ровную поверхность. Оставить фрикадельки в морозильной камере на ночь. Замороженные фрикадельки можно аккуратно снимать с пленки и складывать в один пакет: замороженные они не слипнутся и не потеряют форму. Хранить фрикадельки в морозильной камере.</i></p>',2);
INSERT INTO recipeStageEntity VALUES(16505,4541,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709090010-2-frikadelki-shag3.jpg','<p>В небольшую сковороду с высокими бортами наливаем сливки и бульон (или воду). Добавляем фрикадельки и тушим до готовности при закрытой крышке. </p> <p><i>Когда фрикадельки приготовятся, снимаем крышку и выпариваем соус до загустения. В такой соус для взрослых вкусно добавить душистый перец, прованские травы или даже куркуму. Но для деток до 7 лет все-таки не стоит.</i></p>',3);
INSERT INTO recipeStageEntity VALUES(16509,4541,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709090010-3-frikadelki-shag4.jpg','<p>С подачей на этот раз можно не придумывать ничего особенного – дети и без того любят все сливочное. Так что в АндерСоне просто выкладываем фрикадельки в глубокую веселую тарелку, рядом укладываем молодой горошек, поливаем соусом. </p> <p>Вот такое простое в приготовлении, но продуманное диетическое блюдо по рецепту шеф-повара любимого кафе АндерСон на вашей домашней кухне.</p>',4);
INSERT INTO recipeStageEntity VALUES(16637,4577,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709161122-0-ovoshi.jpg','Очистите лук и морковь и нарежьте.',1);
INSERT INTO recipeStageEntity VALUES(16641,4577,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709161122-1-variat.jpg','Курицу положите в кастрюлю и залейте водой. После закипания снизьте огонь, снимите пену, затем добавьте крупно нарезанные лук и морковь. Посолите и приправьте перцем по вкусу.',2);
INSERT INTO recipeStageEntity VALUES(16645,4577,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200709161122-2-final.jpg','Бульон будет выкипать, поэтому периодически восполняйте объем жидкости. Проверьте на соль.&nbsp;<br>Варите бульон 1-2 часа до готовности.&nbsp;<br><br><i>Совет: Если хотите получить золотистый бульон, процедите его 1-2 раза за время варки.&nbsp;</i>',3);
INSERT INTO recipeStageEntity VALUES(17367,4731,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200819142158-iabloki.jpg','Нарежьте яблоки тонкими кружочками.',915);
INSERT INTO recipeStageEntity VALUES(17371,4731,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200819142320-miska.jpg','Смешайте сахар с корицей и добавьте к яблокам. Все перемешайте. ',916);
INSERT INTO recipeStageEntity VALUES(17375,4731,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20200819142638-final.jpg','Выложите яблоки на решётку духовки и готовьте при температуре 90 градусов 2–3 часа, перевернув их в середине приготовления. Яблоки должны подсушиться, но остаться немного мягкими.',917);
INSERT INTO recipeStageEntity VALUES(17927,4867,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201110151022-0-_a7a1422.jpg','Очищенные овощи измельчите. Картофель нарежьте на небольшие кусочки, примерно одинакового размера. Лук мелко нарежьте. Морковь натрите на терке.<div>Обжарьте лук с морковью на сковороде.<br><br><br></div>',1);
INSERT INTO recipeStageEntity VALUES(17931,4867,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201110151022-1-_a7a1426.jpg','Налейте в кастрюлю необходимое количество воды и поставьте на огонь. Доведите воду до кипения и добавьте измельченные овощи - лук, морковь и картофель. Оставьте вариться минут на 5-7.&nbsp;<div>В это время мелко нарежьте соленые огурцы. Отправьте огурцы в кастрюлю с супом. Перемешайте, доведите до кипения и варите все вместе еще минут 7.</div>',2);
INSERT INTO recipeStageEntity VALUES(17935,4867,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201110151022-2-_a7a1429.jpg','Консервированную горбушу выложите на тарелку и разомните её вилкой. Отправьте её, так же, в суп.&nbsp;<div>Добавьте в кастрюлю немного соли и специи по своему вкусу. Учитывайте, что в консервах есть немного соли. Поэтому вначале попробуйте суп.&nbsp;&nbsp;</div><div>Все ингредиенты еще раз перемешайте и оставьте вариться все вместе, на небольшом огне, еще минут 10.</div>',3);
INSERT INTO recipeStageEntity VALUES(17939,4867,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201110151023-3-_a7a1461.jpg','Ароматный суп с горбушей готов! Разлейте его в подготовленные тарелки и подавайте к столу. В тарелку можно добавить немного сметаны и свежей зелени.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(18011,4891,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201111153821-0-v-blender.jpg','Очищенные зубчики чеснока отправьте в блендер.&nbsp;<div>Добавьте 3 ст. ложки лимонного сока, ледяную воду и 1/2 ч. ложки соли.</div>',1);
INSERT INTO recipeStageEntity VALUES(18015,4891,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201111153821-1-blender-v-nachale.jpg','Взбейте до однородного состояния.',2);
INSERT INTO recipeStageEntity VALUES(18019,4891,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201111153821-2-blender.jpg','Добавьте в три этапа 375 г растительного масла и взбивайте по 5-10 секунд, чтобы соус не расслоился.',3);
INSERT INTO recipeStageEntity VALUES(18083,4903,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201111163543-0-m__a7a4016.jpg','Тыкву натрите на мелкой тёрке.',1);
INSERT INTO recipeStageEntity VALUES(18087,4903,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201111163543-1-m__a7a4019.jpg','Соедините с яйцом, солью и сахаром.',2);
INSERT INTO recipeStageEntity VALUES(18091,4903,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201111163543-2-m__a7a4024.jpg','Добавьте кефир с содой, перемешайте и постепенно засыпайте муку, чтобы получилось не очень жидкое тесто.',3);
INSERT INTO recipeStageEntity VALUES(18095,4903,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201111163543-3-m__a7a4072_(1).jpg','Сковороду смажьте маслом и разогрейте на среднем огне. Выкладывайте по паре ложек тыквенной смеси и обжаривайте оладьи по 3–5 минут с каждой стороны.',4);
INSERT INTO recipeStageEntity VALUES(18447,4975,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113154422-0-_a7a6829.jpg','Приготовьте апельсиновый соус: отожмите сок из апельсина, соедините с сахаром, солью и разогрейте до 70-80 градусов (то есть до состояния, когда сок не кипит, но уже близок к этому).&nbsp;<div>Интенсивно вмешайте крахмал для получения густой консистенции. Добавьте свежевыжатый сок имбиря и лимона.</div>',1);
INSERT INTO recipeStageEntity VALUES(18451,4975,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113154422-1-_a7a6823.jpg','Томаты и клубнику нарежьте мелкими кубиками, смешайте и заправьте апельсиновым соусом. Соль и перец добавьте по вкусу.',2);
INSERT INTO recipeStageEntity VALUES(18455,4975,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113154422-2-_a7a6820.jpg','Нарежьте хлеб тонкими ломтиками и обжарьте до золотистой корочки.',3);
INSERT INTO recipeStageEntity VALUES(18459,4975,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113154422-3-_a7a6855.jpg','На обжаренный тост выложите сыр страчетелла, готовый салат из томатов и клубники и украсьте веточками кинзы.',4);
INSERT INTO recipeStageEntity VALUES(18463,4979,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113155434-0-_a7a6655.jpg','Взбейте яйца с сахаром и солью. Добавьте кефир, перемешайте. Всыпьте смесь муки с содой, перемешайте до однородности.<div>Влейте 150 мл крутого кипятка и пару ложек того масла, на котором собираетесь жарить блинчики. Перемешайте до однородности. Тесто должно получиться не слишком жидким.</div>',1);
INSERT INTO recipeStageEntity VALUES(18467,4979,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113155434-1-_a7a6658.jpg','Нарежьте клубнику тонкими дольками.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(18471,4979,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113155434-2-_a7a6661.jpg','Разогрейте сковороду для блинов, смажьте маслом, выложите несколько ломтиков клубники и сразу же залейте тестом.<div><div>Выпекайте панкейки как обычно. Готовые складывайте в кастрюлю под крышку. Подавайте с медом или сгущенным молоком.</div></div>',3);
INSERT INTO recipeStageEntity VALUES(18499,4987,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113165931-0-sparzha.jpg','Спаржу вымойте, обсушите, удалите твердые концы.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(18503,4987,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113165931-1-obernuli.jpg','Оберните каждую в ломтик бекона.',2);
INSERT INTO recipeStageEntity VALUES(18507,4987,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113165931-2-sparzha-na-skovorode.jpg','На сковороде разогрейте немного растительного масла. Аккуратно выложите спаржу. Обжарьте на среднем огне от 3-5 минут.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(18511,4987,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201113165931-3-final.jpg','Готовую спаржу выложите на бумажное полотенце, чтобы убрать&nbsp; излишки жира. Подавайте горячим.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(18543,4999,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201115211047-0-_a7a8297.jpg','<div>Смешайте муку, желтки, соль, холодное сливочное масло, сахар и молотый миндаль и вымесите гладкое, однородное тесто. Если тесто получается рассыпчатым, добавьте 1–2 ст. л. холодной воды и вымесите еще раз.</div><div>Сформируйте из теста шар, оберните пищевой пленкой и уберите в холодильник на 30 минут.</div>',1);
INSERT INTO recipeStageEntity VALUES(18547,4999,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201115211047-1-_a7a8316.jpg','Отщипните от теста кусочек масой 20–22 г и слепите печенье в форме полумесяца. Так слепите все печенье. Выложите печенье на противень, застеленный пергаментом.',2);
INSERT INTO recipeStageEntity VALUES(18551,4999,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201115211047-2-_a7a0130.jpg','<div>Выпекайте печенье 20 минут в заранее разогретой духовке при температуре 175 °С.</div><div>Немного остудите печенье и еще горячим обваляйте в сахарной пудре. Когда печенье полностью остынет, посыпьте еще одним слоем сахарной пудры.</div>',3);
INSERT INTO recipeStageEntity VALUES(18635,5027,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201115231456-0-_a7a8002.jpg','Лук мелко нарежьте и обжаривайте в 25 г разогретого сливочного масла с листочками тимьяна на минимальном огне около 15 минут. Готовый лук переложите в блендер.',1);
INSERT INTO recipeStageEntity VALUES(18639,5027,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201115231456-1-_a7a8020.jpg','В сковороду, где жарился лук, добавьте еще немного масла и обжарьте куриную печень на среднем огне с обеих сторон до румяной корочки. Не пережарьте: печень должна остаться внутри розовой, а на ощупь не быть твердой. Когда вся печень будет готова, влейте в сковороду портвейн, посолите, приправьте перцем и перемешайте. Остудите.',2);
INSERT INTO recipeStageEntity VALUES(18643,5027,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201115231456-2-_a7a8023.jpg','К луку в блендер добавьте печень, 55 г сливочного масла, сок и цедру апельсина. Измельчите до однородности. Переложите паштет в миску, закройте пищевой пленкой так, чтобы она касалась поверхности паштета, и поставьте в холодильник на 12–24 часа.',3);
INSERT INTO recipeStageEntity VALUES(18647,5027,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201115231456-3-_a7a8029.jpg','Приготовьте соус: очистите и измельчите имбирь и перчик чили. Смешайте имбирь, чили и апельсиновый конфитюр со смородиной, взбейте блендером почти до однородности и поставьте в холодильник на 12–24 часа.',4);
INSERT INTO recipeStageEntity VALUES(18651,5031,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116110127-0-_a7a8485.jpg','Говядину помойте и промокните бумажным полотенцем, посолите и приправьте перцем по вкусу. Разогрейте в сковороде сливочное и подсолнечное масло и жарьте мясо на самом сильном огне по 1 минуте с каждой стороны, запечатывая сок внутри.',1);
INSERT INTO recipeStageEntity VALUES(18655,5031,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116110307-_a7a8495.jpg','<div>Оберните мясо фольгой, положите обратно в сковороду и накройте крышкой. Убавьте огонь до среднего минимума и томите мясо 40–45 минут, периодически переворачивая.</div><div>Готовый ростбиф остудите и нарежьте тонкими ломтиками. Сладкий перец нарежьте соломкой, манго почистите и нарежьте кубиками.</div>',2);
INSERT INTO recipeStageEntity VALUES(18659,5031,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116110241-_a7a8488.jpg','<div>Приготовьте заправку: смешайте оливковое масло, мед, соевый соус, дижонскую горчицу, бальзамический уксус, сок лайма, пропущенный через пресс чеснок и мелко порубленные листочки мяты. Все тщательно перемешайте.</div><div>Выложите на тарелку руколу, мангольд и шпинат, сверху положите ростбиф, перец и манго.&nbsp;</div>',3);
INSERT INTO recipeStageEntity VALUES(18663,5031,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116110127-3-final.jpg','Полейте салат заправкой перед подачей.',4);
INSERT INTO recipeStageEntity VALUES(18667,5035,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116130240-0-_a7a8402.jpg','Сварите в подсоленной воде картофель, морковь и свеклу. Остудите и очистите овощи.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(18671,5035,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116130240-1-_a7a8406.jpg','Сварите в подсоленной воде картофель, морковь и свеклу. Остудите и очистите овощи.',2);
INSERT INTO recipeStageEntity VALUES(18675,5035,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116130240-2-_a7a8409.jpg','&nbsp;Красный лук очистите, нарежьте полукольцами и обжарьте до золотистого цвета.',3);
INSERT INTO recipeStageEntity VALUES(18679,5035,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116130240-3-_a7a8412.jpg','Добавьте куриное филе к луку, перемешайте и остудите.',4);
INSERT INTO recipeStageEntity VALUES(18683,5035,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116130240-4-_a7a8423.jpg','Отварные овощи натрите на терке. Смешайте свеклу с мелко порубленными грецкими орехами.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(18687,5035,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116130641-_a7a8426.jpg','Выложите салат в подготовленную форму по слоям, промазывая каждый майонезом: сначала картофель, затем морковь, курицу с луком и свеклу с орехами.',6);
INSERT INTO recipeStageEntity VALUES(18691,5035,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116130240-6-final.jpg','Украсьте салат гранатовыми зернами и уберите на 6–8 часов в холодильник.',7);
INSERT INTO recipeStageEntity VALUES(18695,5039,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116132644-0-1a7a7935.jpg','В 330 мл теплого молока добавьте сахар, желтки и 50 г растопленного сливочного масла. Перемешайте.',1);
INSERT INTO recipeStageEntity VALUES(18699,5039,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116132644-1-1a7a7938.jpg','Смешайте в другой миске муку, дрожжи и соль. Влейте молочно-яичную смесь. Замесите тесто.',2);
INSERT INTO recipeStageEntity VALUES(18703,5039,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116132644-2-1a7a7995.jpg','Оставьте тесто подниматься в теплом месте: тесто должно в три раза увеличиться в объеме.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(18707,5039,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116132644-3-1a7a8012.jpg','Подошедшее тесто раскатайте в прямоугольник, смажьте растопленным маслом и посыпьте маковой начинкой.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(18711,5039,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116132644-4-1a7a8018.jpg','Сверните тесто в рулет и разрежьте вдоль, деля его на две части, но не прорезая один край до конца (оставьте 5 см от края).&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(18715,5039,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116132644-5-_a7a9119.jpg','Внахлест выложите части друг на друга, формируя жгут.',6);
INSERT INTO recipeStageEntity VALUES(18719,5039,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116132644-6-final.jpg','<div>Выложите плетенку на противень и оставьте расстаиваться на 20 минут.&nbsp;</div><div>Смешайте 60 мл молока и яйцо и хорошо смажьте плетенку.&nbsp;</div><div>Пеките плетенку в заранее разогретой духовке при 180 °С в течение 30–40 минут.</div>',7);
INSERT INTO recipeStageEntity VALUES(18811,5055,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201116192729-0-0t1a5389.jpg','<div>Смешайте в миске муку, крахмал, соду, соль и ванильный сахар.</div><div>В чистую сухую банку насыпье половину мучной смеси. Сверху высыпьте половину сахара, затем оставшуюся муку и оставшийся сахар.&nbsp;</div><div>Сверху слоями высыпьте орехи, цукаты и вишню. Плотно закройте крышкой и привяжите бирочку с инструкцией.<br><br><div>Инструкция для бирки:</div><div><br></div><div>Вам понадобится 120 г сливочного масла комнатной температуры и 1 яйцо. Взбейте масло и яйцо в пышную массу, добавьте все содержимое банки и перемешайте. Вымесите тесто.</div><div>Сформируйте из теста 10–12 печений, выложите на противень и уберите на 30 минут в холодильник. Пеките печенье в разогретой до 180 °С духовке 13–15 минут до золотистого цвета.</div></div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(18883,5079,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117115726-0-final.jpg','<div>Наполните льдом охлажденный бокал.</div><div>Влейте просекко, аперитив Aperol и минеральную воду.&nbsp;</div><div>Украсьте ломтиком апельсина.</div>',1);
INSERT INTO recipeStageEntity VALUES(18899,5087,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117120437-0-final.jpg','<div>Водку и ликер смешайте в бокале со льдом, чтобы охладить.&nbsp;</div><div>Взбейте в шейкере сливки с сахарным сиропом.</div><div>На тарелку высыпьте немного какао, бокал для подачи опустите горлышком в воду на 3 мм, а затем окуните в какао и слегка прокрутите.</div><div>Перелейте водку с ликером в бокал, украшенный какао. Добавьте сливки.</div>',1);
INSERT INTO recipeStageEntity VALUES(18903,5091,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117132756-0-_a7a9806.jpg','<div>Яблоки очистите от кожуры и нарежьте маленькими кубиками. Лук-шалот нарежьте тонкими полукольцами.</div><div>Разогрейте в сковороде 60 г сливочного масла и обжарьте лук-шалот до мягкости</div>',1);
INSERT INTO recipeStageEntity VALUES(18907,5091,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117132756-1-_a7a9809.jpg','Добавьте в сковороду яблоки и мед и готовьте на среднем огне 5–7 минут. Следите, чтобы яблоки остались целыми. Снимите соус с огня и немного остудите.',2);
INSERT INTO recipeStageEntity VALUES(18911,5091,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117132756-2-_a7a9800.jpg','С утиного филе снимите кожу, помойте филе, посолите и приправьте перцем. В сковороде растопите оставшееся сливочное масло и обжарьте филе по 3 минуты с каждой стороны — или до образования корочки. Сковороду с филе снимите с огня, плотно закройте фольгой и оставьте на 5–7 минут.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(18915,5091,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117132756-3-_a7a9813.jpg','Острым ножом нарежьте филе утки тонкими ломтиками.',4);
INSERT INTO recipeStageEntity VALUES(18919,5091,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117132756-4-_a7a9816.jpg','Выложите в тарталетки соус из яблок, сверху ломтики утки, полейте медом и подавайте.',5);
INSERT INTO recipeStageEntity VALUES(18923,5095,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117134423-0-_a7a9579.jpg','Сварите и очистите яйца.',1);
INSERT INTO recipeStageEntity VALUES(18927,5095,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117134423-1-_a7a9582.jpg','Сварите и очистите креветки.',2);
INSERT INTO recipeStageEntity VALUES(18931,5095,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117134423-2-_a7a9585.jpg','Мелко нарежьте 10 креветок, яйца, моцареллу, желтый перец.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(18935,5095,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117134423-3-_a7a9592.jpg','Добавьте икру, майонез, соль, перец и аккуратно перемешайте.',4);
INSERT INTO recipeStageEntity VALUES(18939,5095,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201117134424-4-_a7a9601.jpg','Выложите салат в тарталетки и украсьте целыми креветками и зеленью.',5);
INSERT INTO recipeStageEntity VALUES(18943,5099,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123144605-0-_a7a6420.jpg','Разогрейте духовку до 180 °С. Слегка раскатайте слоеное тесто до толщины 3 мм. Формочкой вырежьте кружки диаметром 4 см.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(18947,5099,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123144605-1-_a7a6749.jpg','Положите на противни на небольшом расстоянии друг от друга и выпекайте 8–10 мин., до золотистого цвета. Остудите основы.',2);
INSERT INTO recipeStageEntity VALUES(18951,5099,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123144605-2-_a7a6745.jpg','<span style="color: rgb(29, 33, 31); font-family: &quot;PT Sans&quot;; font-size: 16px; background-color: rgb(255, 255, 255);">Положите на основу немного сыра с голубой плесенью и слегка размажьте по тесту. Добавьте ломтики груши и виноград.</span>',3);
INSERT INTO recipeStageEntity VALUES(18955,5103,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123150218-0-_a7a7102.jpg','Варите яйца в течение 7 минут. Если варить яйца дольше, то желток потемнеет и приобретет синий оттенок.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(18959,5103,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123150219-1-_a7a7117.jpg','<div>Остудите яйца в холодной воде, очистите от скорлупы, аккуратно разрежьте на две половины и отделите белок от желтка.</div><div>Разомните желток вилкой.</div>',2);
INSERT INTO recipeStageEntity VALUES(18963,5103,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123150219-2-_a7a7120.jpg','<div>Зелень укропа мелко нарежьте, чеснок пропустите через пресс.</div><div>Соедините в миске желток, чеснок, укроп и майонез. Посолите по вкусу, добавьте щепотку черного молотого перца.</div><div>Тщательно перемешайте начинку. Наполните начинкой половинки белка.</div>',3);
INSERT INTO recipeStageEntity VALUES(18967,5103,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123150219-3-_a7a7124.jpg','<div>Шпроты выложите на бумажную салфетку, чтобы удалить излишки масла.</div><div>В середину каждого фаршированного яйца положите рыбку.</div><div>Украсьте фаршированные яйца красной икрой и листиком петрушки.</div>',4);
INSERT INTO recipeStageEntity VALUES(19011,5115,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123154747-0-_a7a7549.jpg','Очистите картофель. Порежьте и сварите в большом количестве подсоленной воды.',1);
INSERT INTO recipeStageEntity VALUES(19015,5115,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123154747-1-_a7a7553.jpg','Лук-шалот нарежьте и обжарьте до прозрачного состояния на смеси растительного и сливочного масла. Добавьте нарезанный корень сельдерея.',2);
INSERT INTO recipeStageEntity VALUES(19019,5115,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123154747-2-_a7a7559.jpg','Добавьте белое вино. Увеличьте огонь и дайте овощам покипеть, периодически помешивая.<div>Лук-шалот и сельдерей в вине добавьте в кастрюлю с картофелем и варите, пока картофель не станет мягким.</div>',3);
INSERT INTO recipeStageEntity VALUES(19023,5115,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123154747-3-_a7a7841.jpg','Тем временем, пожарьте гребешки на разогретом сливочном масле с добавлением белого вина.',4);
INSERT INTO recipeStageEntity VALUES(19027,5115,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123154747-4-_a7a7563.jpg','<div>Овощной суп обработайте блендером до состояния пюре.</div><div>Разлейте суп по тарелкам, сверху аккуратно выложите готовые гребешки с зеленью и можете подавать на стол.</div>',5);
INSERT INTO recipeStageEntity VALUES(19031,5119,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123160009-0-_a7a7566.jpg','Поставьте кастрюлю с водой и креветками на огонь. Бульон посолите, поперчите по вкусу и добавьте лавровый лист. Варите около 5-7 минут после закипания. Вывалите в тарелку креветки, а бульон процедите и снова поставьте на огонь.',1);
INSERT INTO recipeStageEntity VALUES(19035,5119,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123160009-1-_a7a7575.jpg','<div>Очистите картофель, морковь и лук. Нарежьте картофель средними кусочками, а морковь и лук мелкими.</div><div>Положите в кипящий бульон картофель, морковь и лук. Варите при среднем кипении около 15 мин.</div>',2);
INSERT INTO recipeStageEntity VALUES(19039,5119,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123160009-2-_a7a7579.jpg','<div>Измельчите плавленые сырки. Можете натереть на терке, а можно нарезать кубиками.</div><div>Положите сырки в кипящий бульон и перемешайте. Через 5-7 минут сыр полностью растворится.</div>',3);
INSERT INTO recipeStageEntity VALUES(19043,5119,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201123160009-3-_a7a7842.jpg','<div>Очистите от панциря вареные креветки. Несколько штук можете оставить с хвостиком для декора.</div><div>Обжарьте креветки на сковороде.</div><div>Положите очищенные креветки в суп и дайте им пару минут покипеть. Добавьте измельченную петрушку.</div>',4);
INSERT INTO recipeStageEntity VALUES(19111,5135,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201124165845-0-1a7a4720.jpg','Нарежьте огурцы и оливки на небольшие кружочки. Если получаются слишком большие части, разрежьте их пополам.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(19115,5135,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201124165845-1-1a7a4269.jpg','Нашинкуйте перья зеленого лука.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(19119,5135,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201124165845-2-1a7a4275.jpg','Нарежьте сыр кубиками',3);
INSERT INTO recipeStageEntity VALUES(19123,5135,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201124165845-3-salat_s_tofu.jpg','Смешайте все ингредиенты, поперчите и заправьте маслом.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(19255,5159,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201125180245-0-m__a7a1324.jpg','Баклажаны порезать крупными кубиками и обжарить на оливковом масле.&nbsp;Остудить.',1);
INSERT INTO recipeStageEntity VALUES(19259,5159,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201125180245-1-m__a7a1328.jpg','Кинзу крупно порезать.',2);
INSERT INTO recipeStageEntity VALUES(19263,5159,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201125180245-2-m__a7a1331.jpg','Лук порезать полукольцами.',3);
INSERT INTO recipeStageEntity VALUES(19267,5159,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201125180245-3-m__a7a1334.jpg','Помидоры порезать четвертинками, если черри - пополам.',4);
INSERT INTO recipeStageEntity VALUES(19271,5159,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201125180245-4-m__a7a1346.jpg','Все соединить. Посолить, поперчить.',5);
INSERT INTO recipeStageEntity VALUES(19307,5171,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127122018-0-shokolad.jpg','Шоколад растопите с маслом на водяной бане.<div>Перемешайте до однородности.&nbsp;</div>',1);
INSERT INTO recipeStageEntity VALUES(19311,5171,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127122018-1-shokokrem.jpg','<div>Яйца взбейте миксером .&nbsp;</div><div>Перемешайте с шоколадно-масляной массой.&nbsp;</div><div>Смешайте сахар, просеянные муку и какао и разрыхлитель.&nbsp;</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(19315,5171,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127122018-2-testosvetl.jpg','<div>Всыпайте в яично-шоколадную смесь и взбейте миксером на небольшой скорости.&nbsp;</div><div>Влейте пиво, еще раз взбейте до однородности.&nbsp;</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(19319,5171,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127122018-3-testo.jpg','<div>Подмешайте вишню.&nbsp;</div><div>Выстелите прямоугольную форму для выпечки размером 25*20 см пергаментом, выложите тесто.&nbsp;</div><div>Выпекайте в заранее нагретой до 180 градусов духовке 25 минут.&nbsp;</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(19323,5175,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127123909-0-muka.jpg','Просейте через мелкое сито сухие ингредиенты: муку, сахар, дрожжи. Можно добавить пряности, которые заодно выступят натуральным красителем (около 2-3 граммов): куркуму, острый карри в порошке и т.д.',1);
INSERT INTO recipeStageEntity VALUES(19327,5175,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127123909-1-testo.jpg','<div>Добавьте мягкое сливочное масло, молоко и воду.</div><div>Начните вымешивать руками или насадкой крюк. Примерно через 5-10 минут тесто станет гладким, послушным.</div>',2);
INSERT INTO recipeStageEntity VALUES(19331,5175,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127123909-2-opara.jpg','Добавьте соль. На этом же этапе, помимо соли, можно добавить бамбуковый уголь (2-3 г). Тогда ваше тесто станет чёрным. Или другие натуральные красители. После добавления вымешивайте тесто буквально минуту.',3);
INSERT INTO recipeStageEntity VALUES(19335,5175,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127123909-3-bulochki.jpg','<div>Поделите тесто на три штучки. Округлите шарики теста.</div><div>Разложите на расстоянии друг от друга и накройте плёнкой. Теперь нужно подождать 30-40 минут, чтобы булочки выросли вдвое.&nbsp;</div><div>Взбейте одно яйцо. Кисточкой нанесите на булочки тонкий слой. Сверху щедро посыпьте белым кунжутом.&nbsp;</div>',4);
INSERT INTO recipeStageEntity VALUES(19339,5175,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127123909-4-zapekat.jpg','<div>Выпекайте при 180-190 градусах. Примерно 15 минут для булочек такого размера. Всё просто — как только они стали румяными благодаря яичной обмазке, можно доставать.</div>',5);
INSERT INTO recipeStageEntity VALUES(19379,5187,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127135242-0-1a7a3394.jpg','Оливки и маслины отделите от косточек и нарежьте.',1);
INSERT INTO recipeStageEntity VALUES(19383,5187,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127135243-1-1a7a3417.jpg','Натрите пармезан на мелкой терке.',2);
INSERT INTO recipeStageEntity VALUES(19387,5187,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127135243-2-1a7a3402.jpg','Орехи мелко нарубите.',3);
INSERT INTO recipeStageEntity VALUES(19391,5187,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127135243-3-1a7a3427.jpg','<div>В блендер положите все ингредиенты и взбивайте до кашеобразного состояния, но НЕ до однородного пюре!&nbsp;</div><div>Оливковым маслом регулируйте консистенцию.</div>',4);
INSERT INTO recipeStageEntity VALUES(19395,5191,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127140737-0-1a7a3661.jpg','Огурцы тщательно очистите от кожи и нарежьте крупными полукруглыми ломтиками. Сложите в глубокий салатник или широкую миску, в которой будет удобно смешивать.',1);
INSERT INTO recipeStageEntity VALUES(19399,5191,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127140737-1-1a7a3670.jpg','Помидоры нарежьте крупными дольками. Добавьте в салатник к огурцам и перемешайте. Мешать при этом греческий салат лучше всего руками, подливая на каждом этапе понемногу оливкового масла.',2);
INSERT INTO recipeStageEntity VALUES(19403,5191,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127140737-2-1a7a3676.jpg','Сладкий зеленый перец очистите от семян и перепонок и нарежьте крупными кубиками. Добавьте в салатник. Перемешайте с другими овощами.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(19407,5191,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127140738-3-1a7a3666.jpg','Красный лук нарежьте тонкими полукольцами, а затем руками разделите на тонкие лепестки. Переложите к остальным овощам, снова все перемешайте.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(19411,5191,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127140738-4-final.jpg','<div>Добавьте в салатник маслины без косточек (можно купить уже избавленные от косточек маслины, а можно очистить их самостоятельно, раздавив плоской стороной ножа каждую маслину и вынув косточку).&nbsp;</div><div>Твердую фету нарежьте острым ножом на прямоугольные ломтики толщиной в 1 см. Чтобы сыр не лип к лезвию, нож можно предварительно окунуть в теплую воду.&nbsp;</div><div>Еще раз перемешайте овощи в салатнике и разложите по банкам. Сверху на каждую порцию выложить по два ломтика феты. Щедро полейте оливковым маслом.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(19415,5195,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127141619-0-1a7a3580.jpg','Сделайте пюре из манго.',1);
INSERT INTO recipeStageEntity VALUES(19419,5195,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127141619-1-1a7a3590.jpg','Ложечкой достаньте мякоть маракуйя.',2);
INSERT INTO recipeStageEntity VALUES(19423,5195,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127141620-2-1a7a3597.jpg','<div>В бокал со льдом налейте все ингредиенты, перемешайте барной ложкой.&nbsp;</div><div>Украсьте дольками манго и половиной маракуйи.</div>',3);
INSERT INTO recipeStageEntity VALUES(19427,5199,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127143234-0-1a7a6833.jpg','Грецкие орехи поджарьте на сухой сковороде или в духовке.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(19431,5199,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127143234-1-1a7a6853.jpg','Измельчите до средней степени - не мелко и не крупно.',2);
INSERT INTO recipeStageEntity VALUES(19435,5199,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127143234-2-1a7a6860.jpg','Засыпьте сахарным песком, налейте белки и все перемешайте.',3);
INSERT INTO recipeStageEntity VALUES(19439,5199,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127143234-3-1a7a6869.jpg','<div>Первый лист теста выложите в квадратную форму размером 25х30см, смажьте растопленным сливочным маслом, повторите так еще 3 раза.</div><div>Распределите часть начинки поверх теста.</div><div>Вновь один лист теста выложите в форму, смажьте растопленным сливочным маслом, повторите так еще 3 раза.</div><div>Распределите часть начинки поверх теста.</div><div>Повторяйте так, пока не закончится тесто и начинка.</div><div>Последний слой смажьте желтком, не прорезая ножом нарисуйте ромбики и выложите на них орешки.&nbsp;</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(19443,5199,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127143234-4-1a7a6947.jpg','<div>Отправьте в духовку на 185? выпекаться примерно 30 мин до янтарного цвета.&nbsp;</div><div>Когда пахлава достигнет янтарного цвета, вытащите из духовки, разрежьте на ромбики и налейте сверху жидкий мед. Размажьте его кисточкой и опять поставьте в духовку минут на 10.&nbsp;</div><div>Оставьте пахвалу на ночь в форме.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(19475,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162102-0-1a7a1726.jpg','Мелко нарежьте лук.',1);
INSERT INTO recipeStageEntity VALUES(19479,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162102-1-1a7a1734.jpg','Нарежьте отварные яйца небольшими кубиками.',2);
INSERT INTO recipeStageEntity VALUES(19483,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162102-2-1a7a1744.jpg','Авокадо нарежьте на маленькие кусочки.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(19487,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162102-3-1a7a3779.jpg','Томаты черри порежьте пополам.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(19491,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162103-4-1a7a3515.jpg','Куриную грудку обжарьте.',5);
INSERT INTO recipeStageEntity VALUES(19495,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162103-5-1a7a3787.jpg','Нарежьте куриную грудку.&nbsp;',6);
INSERT INTO recipeStageEntity VALUES(19499,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162103-6-1a7a6447.jpg','Бекон обжарьте на раскаленной сковороде, пока он не станет немного хрустящим.',7);
INSERT INTO recipeStageEntity VALUES(19503,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162103-7-sous.jpg','Чеснок пропустите через пресс и смешайте с двумя видами горчицы и маслом.',8);
INSERT INTO recipeStageEntity VALUES(19507,5207,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127162103-8-final.jpg','<div>В тарелку выложите салатные листья.</div><div>Сверху полосками вложите остальные ингредиенты.</div><div>Заправку подайте отдельно.&nbsp;</div>',9);
INSERT INTO recipeStageEntity VALUES(19511,5211,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127163349-0-ryba.jpg','<div>Для приготовления рыбного бульона употребляется частиковая рыба (судак, окунь и др.).</div><div>Бульон из частиковой рыбы приготовляется так: рыбу очистите от чешуи, разрежьте брюшко, выньте внутренности, промойте, нарежьте рыбу на порционные куски, из голов выньте жабры.&nbsp;</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(19515,5211,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127163349-1-bulon.jpg','<div>Приготовленную таким образом рыбу положите в кастрюлю, залейте холодной водой, добавьте соль, коренья и лук, нарезанный ломтиками, и варите при слабом кипении 25–30 минут; после этого куски рыбы вынуть, а голову и хвост продолжайте варить еще 15–20 минут. Куски рыбы можно положить в тарелки с супом или использовать как самостоятельное второе блюдо.&nbsp;</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(19519,5215,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127165124-0-luk.jpg','<div>Морковь натрите на мелкой терке, чеснок и лук очень мелко нарежьте.</div><div>На оливковом масле обжарьте лук до прозрачности.</div>',1);
INSERT INTO recipeStageEntity VALUES(19523,5215,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127165124-1-morkov.jpg','Добавьте чеснок и морковь, пассируйте овощи 3-5 минут.',2);
INSERT INTO recipeStageEntity VALUES(19527,5215,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127165124-2-tomaty.jpg','Томаты в собственном соку мелко нарежьте, петрушку нашинкуйте.',3);
INSERT INTO recipeStageEntity VALUES(19531,5215,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127165124-3-zelen.jpg','Добавьте в сковороду к овощам нарезанные помидоры, сок из них и петрушку. Приправьте соус солью и сушеным базиликом.',4);
INSERT INTO recipeStageEntity VALUES(19535,5215,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127165124-4-sous.jpg','Тушите соус на небольшом огне в течении 30 минут. При необходимости добавьте немного воды или увеличьте время приготовления для получения нужной консистенции.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(19539,5219,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127171107-0-sous.jpg','Смешайте мёд с горчицей и оливковым маслом до однородной консистенции.',1);
INSERT INTO recipeStageEntity VALUES(19543,5219,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127171107-1-miaso.jpg','Нарежьте говядину на тонкие ломтики.',2);
INSERT INTO recipeStageEntity VALUES(19547,5219,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127171107-2-syr.jpg','Натрите пармезан на мелкой терке.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(19551,5219,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201127171107-3-final.jpg','<div>На дно и по краям тарелки тонким слоем выложите медово-горчичную заправку, затем сверху равномерным слоем говядину. Посыпьте солью и перцем.</div><div>На говядину выложите рукколу, а каперсы, выложите вокруг рукколы.</div><div>Сверху на рукколу выложите пармезан и полейте всё кремом-бальзамиком.&nbsp;</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(19575,5227,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130113528-0-sito.jpg','Черную смородину разморозьте, и прокрутите в блендере. Протрите смородину через сито, чтобы получить 150 г пюре.',1);
INSERT INTO recipeStageEntity VALUES(19579,5227,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130113528-1-kastriulia.jpg','Положите в сотейник сахар, влейте воду (200 г), доведите до кипения, и кипятите около 1 минуты. Добавьте лимонную кислоту и кипятите еще 1 минуту.',2);
INSERT INTO recipeStageEntity VALUES(19583,5227,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130113528-2-voda.jpg','Параллельно размешайте крахмал с водой (30 г). Влейте крахмальную смесь в сироп, и подождите пока он заварится. Остудите и поставьте кисель в холодильник.',3);
INSERT INTO recipeStageEntity VALUES(19587,5227,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130113528-3-bez_semechek.jpg','Смешайте смородиновое пюре с киселем, добавьте лимонный сок и перелейте в мороженицу.',4);
INSERT INTO recipeStageEntity VALUES(19591,5227,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130113528-4-stakanchiki.jpg','Готовое мороженое разложите по стаканчикам, и поставьте в морозилку на 2-3 часа.',5);
INSERT INTO recipeStageEntity VALUES(19595,5231,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130115031-0-blender.jpg','Все ягоды и фрукты отправьте в чашу измельчителя или блендера, добавьте немного молока и взбивайте всё несколько секунд до однородной консистенции. Масса не должна получиться очень жидкой или наоборот очень густой.',1);
INSERT INTO recipeStageEntity VALUES(19599,5231,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130115031-1-smuzi.jpg','Затем добавьте 2 столовых ложек овсяных отрубей, перемешайте. Отруби можно заменить на овсяные хлопья и всё измельчить вместе с фруктами и ягодами.',2);
INSERT INTO recipeStageEntity VALUES(19603,5231,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130115031-2-iagodnyi_smuzi_boul.jpg','Перелейте в глубокую пиалу и начинайте украшать свежими ягодами, фруктами, орехами и так далее. Это может быть всё что угодно, в нашем варианте это – зерна граната, семена чиа и манго.',3);
INSERT INTO recipeStageEntity VALUES(19607,5235,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130122114-0-blender.jpg','<div>Очистите и нарежьте бананы крупными кусочками.&nbsp;</div><div>Вымойте и обсушите финики и клубнику.&nbsp;</div><div>Удалите косточки фиников и плодоножки у ягод клубники.</div><div>Поместите клубнику и кусочки бананов в чашу блендера. Взбейте все 1 минуту, до однородности.</div>',1);
INSERT INTO recipeStageEntity VALUES(19611,5235,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130122114-1-smuzi-sdelan.jpg','<div>Влейте миндальное молоко. Количеством добавляемого молока можно регулировать густоту напитка.</div><div>Добавьте в чашу блендера 8-10 фиников и взбейте все еще 1 минуту.</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(19615,5235,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130122114-2-klubnichnyi_smuzi_na_mindalnom_moloke.jpg','Разлейте напиток по бокалам. По желанию, украсьте клубникой и листьями мяты и подавайте к столу.',3);
INSERT INTO recipeStageEntity VALUES(19619,5239,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130123407-0-greipfrut.jpg','Очистите грейпфрут и удалить все белые стеночки и прожилки вручную, сохранив лишь мякоть.',1);
INSERT INTO recipeStageEntity VALUES(19623,5239,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130123407-1-imbir.jpg','Мелко нарежьте или натрите имбирь.',2);
INSERT INTO recipeStageEntity VALUES(19627,5239,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130123407-2-blender.jpg','Добавьте все ингредиенты в блендер и измельчите до однородности.',3);
INSERT INTO recipeStageEntity VALUES(19631,5239,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130123407-3-smuzi.jpg','Разлейте по бокалам.',4);
INSERT INTO recipeStageEntity VALUES(19807,5271,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130163016-0-kartofel.jpg','Картофель очистите и натрите на мелкой терке.',1);
INSERT INTO recipeStageEntity VALUES(19811,5271,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130163016-1-luk.jpg','Лук мелко порежьте и обжарьте на сковороде до золотистого цвета.',2);
INSERT INTO recipeStageEntity VALUES(19815,5271,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130163016-2-smes.jpg','В миске соедините тертый картофель, яйцо, жареный лук, мелко порезанный укроп, муку, разрыхлитель, соль и перец по вкусу.',3);
INSERT INTO recipeStageEntity VALUES(19819,5271,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130163017-3-vafli.jpg','<div>Вафельницу смажьте маслом и выпекайте вафли в течении 5 минут.</div><div>Подавайте вафли с яйцом пашот и сыром Бри.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(19823,5271,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201130163017-4-final.jpg','<div>Для приготовления яиц пашот:</div><div>Отрежьте прямоугольный кусочек пищевой пленки, выложите его в небольшую глубокую емкость и смажьте пленку растительным маслом.</div><div>В углубление разбейте яйцо, посолите.</div><div>Соберите концы пленки вместе, завяжите узелком или обвяжите ниткой. В кастрюле вскипятите воду, уменьшите огонь и опустите мешочек с яйцом в воду.</div><div>Варите яйцо в пленке 1-2 минуты, в зависимости от желаемой консистенции. Готовое яйцо пашот выньте из воды и аккуратно снимите пищевую пленку.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(19896,5284,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201133312-0-baklazhany.jpg','Баклажаны разрежьте вдоль, проткните вилкой мякоть, сбрызните оливковым маслом, посолите, и запеките в духовке на режиме гриль или при максимальной температуре 15 минут.',1);
INSERT INTO recipeStageEntity VALUES(19900,5284,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201133312-1-mindalt.jpg','Тщательно растолките в ступке миндаль.',2);
INSERT INTO recipeStageEntity VALUES(19904,5284,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201133312-2-piure.jpg','<div>Выскребите мякоть баклажан и порубите ее.</div><div>Добавьте миндаль, лимонный сок, копченую паприку, пармезан, оливковое масло, мед.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(19908,5284,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201133312-3-final_2.jpg','Приправьте по вкусу солью и черным перцем, посыпьте листиками орегано или тимьяна.',4);
INSERT INTO recipeStageEntity VALUES(19948,5296,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201141353-0-tvorog.jpg','<div>Сыр натрите на тёрке, добавьте яйцо, творог и специи, перемешивайте.</div><div>Постепенно добавьте муку и разрыхлитель.</div>',1);
INSERT INTO recipeStageEntity VALUES(19952,5296,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201141353-1-massa.jpg','Вымешивайте сырное тесто, оно должно получиться послушным и слегка липнуть к рукам.',2);
INSERT INTO recipeStageEntity VALUES(19956,5296,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201141353-2-na_protivne.jpg','<div>Скатайте колбаски, смажьте желтком и посыпьте по желанию кунжутом.</div><div>Выпекайте в разогретой заранее духовке при 200 градусах 15-20 минут.</div>',3);
INSERT INTO recipeStageEntity VALUES(19960,5300,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201143913-0-maionez.jpg','Майонез смешайте с измельченным чесноком.',1);
INSERT INTO recipeStageEntity VALUES(19964,5300,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201143914-1-krabovye_palochki.jpg','<div>Яйца и крабовые палочки натрите.&nbsp;</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(19968,5300,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201143914-2-zelen.jpg','Зелень измельчите.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(19972,5300,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201143914-3-vtoroi_sloi.jpg','<div>Начинку выкладывайте по длинной стороне лаваша.&nbsp;</div><div>Приступайте к сборке: фольга, сверху пергамент, далее 2 листа лаваша внахлест оставляя хвостик 10 см.&nbsp;</div><div>На лаваш поместите 1\3 майонеза, зелень, яйца.&nbsp;</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(19976,5300,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201143914-4-vtoroi_lavash.jpg','<div>Дальше поместите еще один лаваш. На него добавьте 1\3 майонеза, крабовые палочки.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(19980,5300,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201143914-5-tretii_lavash.jpg','<div>Положите сверху еще один лаваш. Далее намажьте 1\3 майонеза и посыпьте двумя видами сыра.&nbsp;</div><div><br></div>',6);
INSERT INTO recipeStageEntity VALUES(19984,5300,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201143914-6-skatat.jpg','<div>Заверните все в тугой рулет.&nbsp;</div><div><br></div>',7);
INSERT INTO recipeStageEntity VALUES(19988,5300,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201143914-7-v_folge.jpg','Разрежьте пополам. Каждый рулет заверните в бумагу, потом в фольгу. Уберите на ночь в холодильник.',8);
INSERT INTO recipeStageEntity VALUES(20012,5308,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201152916-0-piure.jpg','С помощью блендера доведите сливу до состояния пюре.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(20016,5308,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201152916-1-sirop.jpg','Вылейте пюре в сотейник. Поставьте на средний огонь и доведите до кипения, но не кипятите.',2);
INSERT INTO recipeStageEntity VALUES(20020,5308,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201152916-2-sahar.jpg','Пока пюре нагревается, смешайте сахар и пектин.&nbsp;<div>Как только на поверхности сливы появились первые пузырьки, добавьте сахарную смесь маленькими порциями, постоянно помешивая.&nbsp;</div><div>После этого варите смесь до температуры 106 градусов на маленьком огне, постоянно помешивая (примерно 20 минут).</div>',3);
INSERT INTO recipeStageEntity VALUES(20024,5308,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201152916-3-v_forme.jpg','Пока варится пюре смешайте лимонную кислоту и воду.&nbsp;<div>Смажьте форму для мармелада растительным маслом.&nbsp;</div><div>Как только пюре достигнет температуры 106 градусов, уберите сотейник с огня и вмешайте лимонную кислоту.&nbsp;</div><div>После этого быстрыми движениями перелейте мармелад в форму.&nbsp;</div><div>Оставьте мармелад остывать на ночь при комнатной температуре.&nbsp;</div>',4);
INSERT INTO recipeStageEntity VALUES(20028,5308,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201152916-4-v_sahare.jpg','На следующий день достаньте мармелад из формы, нарежьте на порционные кусочки и обильно обсыпьте сахаром или сахарной пудрой.',5);
INSERT INTO recipeStageEntity VALUES(20032,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155508-0-luk.jpg','Нарежьте лук, болгарский перец и чеснок.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(20036,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155508-1-kurica.jpg','Куриное филе посолите и поперчите по вкусу, сбрызните оливковым маслом и запеките в духовке в режиме гриль или обжарьте на сковороде до золотистой корочки.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20040,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155509-2-perec.jpg','На сковороде обжарьте болгарский перец, лук, чеснок.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(20044,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155509-3-boby.jpg','Затем добавьте кукурузу, фасоль, халапеньо и рубленные томаты. Тушите минут 10, не забывая постоянно помешивать, чтобы кукуруза не пригорела.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(20048,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155509-4-syr.jpg','Сыр натрите на крупной тёрке, а курицу разберите на волокна.',5);
INSERT INTO recipeStageEntity VALUES(20052,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155509-5-sloi_syra.jpg','На половину тортильи выложите сыр.',6);
INSERT INTO recipeStageEntity VALUES(20056,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155509-6-sloi_kuricy.jpg','Затем добавьте курицу.',7);
INSERT INTO recipeStageEntity VALUES(20060,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155509-7-finalnyi_sloi.jpg','<div>На курицу выложите овощную начинку и измельченную кинзу.&nbsp;</div><div>Накройте начинку второй половиной тортильи и немного поджарьте на сковороде.</div><div><br></div>',8);
INSERT INTO recipeStageEntity VALUES(20064,5312,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201155509-8-final.jpg','Разрежьте тортилью на треугольники и подавайте с соусом гуакамоле и сметаной.&nbsp;',9);
INSERT INTO recipeStageEntity VALUES(20068,5316,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201161811-0-kusochki-tykvy.jpg','Тыкву почистите, мелко нарежьте, залейте горячей водой так, чтобы она была полностью покрыта, и варите до готовности.',1);
INSERT INTO recipeStageEntity VALUES(20072,5316,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201161811-1-piure-iz-tykvy.jpg','Тыкву разомните вилкой или толкушкой, чтобы оставались маленькие кусочки тыквы.',2);
INSERT INTO recipeStageEntity VALUES(20076,5316,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201161811-2-belki-vzbitye.jpg','Муку перемешайте с разрыхлителем и солью.<div>Яйца с сахаром взбивайте миксером, постепенно вливая молоко, пока сахар полностью не растворится.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(20080,5316,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201161811-3-testo.jpg','<div>Всыпьте муку с разрыхлителем и продолжайте взбивать до получения однородного теста.</div><div>Кардамон раздавите плоской стороной ножа, выньте зерна и добавьте в тесто.</div><div>Сливочное масло растопите и влейте в тесто, добавьте тыквенное пюре и все вымешайте.</div>',4);
INSERT INTO recipeStageEntity VALUES(20084,5316,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201201161811-4-vafli.jpg','Разогрейте вафельницу, смажьте ее оливковым маслом и испеките вафли.<div>Готовые вафли посыпьте сахарной пудрой.</div>',5);
INSERT INTO recipeStageEntity VALUES(20132,5324,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202123626-0-sous.jpg','Смешайте майонез, кефир, сметану и кетчуп. Добавьте хмели-сунели, карри, щепотку соли, перец по вкусу и соус табаско. Выдавите в соус два зубчика чеснока. Все тщательно перемешайте и оставьте настаиваться.',1);
INSERT INTO recipeStageEntity VALUES(20136,5324,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202123626-1-sosiski.jpg','Оберните каждую сосиску в ломтик сыра. Дальше оберните каждую сосиску в две полоски бекона.&nbsp;<div>Поместите сосиски на противень и отправьте в духовку разогретую до 180 градусов на 10-15 минут.</div>',2);
INSERT INTO recipeStageEntity VALUES(20140,5324,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202123626-2-ovoshi.jpg','Все овощи мелко нарежьте.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(20144,5324,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202123626-3-sloi_odin.jpg','Разделите лаваш на две части (так получится два хот-дога).&nbsp;<div>Намажьте соус на лаваш и посыпьте пекинской капустой.</div>',4);
INSERT INTO recipeStageEntity VALUES(20148,5324,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202123626-4-ogurcy_v_lavashe.jpg','Дальше добавьте огурцы.',5);
INSERT INTO recipeStageEntity VALUES(20152,5324,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202123626-5-ovoshi_v_lavashe.jpg','Потом добавьте помидоры.',6);
INSERT INTO recipeStageEntity VALUES(20156,5324,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202123626-6-luk_v_lavashe.jpg','Следующим слоем выложите лук. Далее полейте все соусом и поместите две сосиски с сыром и беконом.&nbsp;',7);
INSERT INTO recipeStageEntity VALUES(20160,5324,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202123626-7-finish.jpg','Сверните лаваш конвертиком.&nbsp;<div>Разогрейте сковороду гриль на сильном огне. Подрумяньте хот-дог с каждой стороны примерно одну минуту.</div>',8);
INSERT INTO recipeStageEntity VALUES(20164,5328,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202133348-0-sous.jpg','Приготовьте домашний майонез.<div>Смешайте с помощью погружного блендера яйцо и подсолнечное масло. Добавьте соль и лимонный сок.&nbsp;</div>',1);
INSERT INTO recipeStageEntity VALUES(20168,5328,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202133348-1-specii.jpg','Маринованный огурчики мелко накрошите.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20172,5328,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202133348-2-finalnyi_sous.jpg','Смешайте три столовые ложки домашнего майонеза и огурчики. Добавьте две столовые ложки рассола из-под огурцов, белый винный уксус, горчицу. Приправьте паприкой, чесноком, луком.<div>Все перемешайте, добиваясь однородной консистенции.&nbsp;</div><div>Оставьте соус настояться 10-15 минут.&nbsp;</div>',3);
INSERT INTO recipeStageEntity VALUES(20228,5340,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202154452-0-testo.jpg','Замесите дрожжевое тесто из всех ингредиентов, кроме яичного желтка и молока.&nbsp;<div>Просейте половину муки, добавьте сахар, дрожжи и другие сухие ингредиенты. Нагрейте воду до 36-38 градусов.&nbsp;</div><div>Добавьте в мучную смесь воду и масло. Перемешивайте все на низкой скорости деревянной лопаткой или миксером с насадкой «крюк». Постепенно добавляйте остаток муки.&nbsp;</div><div>Вымешивайте тесто руками или миксером до гладкого, эластичного состояния 10-15 минут.</div><div>Накройте тесто чистым влажным полотенцем или пищевой пленкой и оставьте подходить в теплом месте на 1-1,5 часа, пока оно не увеличится в объеме вдвое.</div>',1);
INSERT INTO recipeStageEntity VALUES(20232,5340,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202154452-1-batony.jpg','Подготовьте противень - застелите его бумагой или ковриком для выпечки.&nbsp;<div>Выложите подошедшее тесто на припыленную мукой рабочую поверхность, слегка обомните и сформируйте два продолговатых батона.&nbsp;</div><div>Выложите получившиеся заготовки на подготовленный противень, накройте полотенцем или пищевой пленкой и оставьте подходить в теплом месте еще на 30-40 минут.</div>',2);
INSERT INTO recipeStageEntity VALUES(20236,5340,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202154452-2-nadrezy.jpg','Разогрейте духовку до 180 градусов.&nbsp;<div>Смажьте подошедшие батоны при помощи кулинарной кисти смесью желтка и молока и сделайте на их поверхности острым ножом или бритвой по три косых надреза.&nbsp;<div>Выпекайте батоны в разогретой духовке 25-30 минут до готовности.</div></div>',3);
INSERT INTO recipeStageEntity VALUES(20280,5352,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202170741-0-vse-testo.jpg','В чаше миксера смешайте воду, мед и дрожжи. Оставьте на 5-10 минут или пока смесь не начнет пузыриться.&nbsp;<div>Добавьте молоко, йогурт, муку, разрыхлитель, пищевую соду и соль. Используя крючок для теста, перемешивайте, пока мука полностью не смешается, примерно 2-4 минуты. Тесто должно быть липким. Слегка присыпьте мукой и руками замесите тесто в шар.&nbsp;</div><div>Накройте миску полиэтиленовой пленкой и оставьте в теплом месте на 1 час, пока тесто не увеличится вдвое.&nbsp;</div>',1);
INSERT INTO recipeStageEntity VALUES(20284,5352,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202170741-1-nachinka.jpg','Сделайте начинку. В миске смешайте шпинат, зелень, моцареллу, фету, чеснок и щепотку красного перца.',2);
INSERT INTO recipeStageEntity VALUES(20288,5352,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202170742-2-kruzhochki.jpg','Когда все будет готово, разделите тесто на 8 равных шариков.&nbsp;<div>Раскатайте каждый шарик в круг.</div>',3);
INSERT INTO recipeStageEntity VALUES(20292,5352,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202170742-3-testo.jpg','<font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Добавьте в центр примерно 3 столовые ложки сырной смеси.&nbsp;</font><font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Сверните тесто вокруг сыра, защипывая, чтобы запечатать его.&nbsp;</font>',4);
INSERT INTO recipeStageEntity VALUES(20296,5352,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202170742-4-raskatali.jpg','<font style="font-size: 16px; box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; background-color: rgb(255, 255, 255); vertical-align: inherit;">Используя скалку, раскатайте каждый кусок теста в большой овал, примерно 12-15 сантиметров в диаметре и 1-1,5 сантиметра толщиной.&nbsp;</font><font style="font-size: 16px; box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; background-color: rgb(255, 255, 255); vertical-align: inherit;">Повторите то же самое с оставшимся тестом.</font>',5);
INSERT INTO recipeStageEntity VALUES(20300,5352,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202170742-5-obzharili.jpg','<font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Нагрейте большую чугунную сковороду на среднем или сильном огне, чтобы сковорода была очень горячей.&nbsp;</font><font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Смажьте обе стороны наана 4 столовыми ложками топленого масла.&nbsp;</font><font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Сбрызните сковороду чайной ложкой оливкового масла, а затем аккуратно распределите масло по сковороде бумажным полотенцем.&nbsp;</font><div><font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Положите наан на раскаленную сковороду, сразу накройте крышкой и готовьте 1 минуту, пока не образуются пузырьки.&nbsp;</font><font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Переверните и готовьте без крышки еще 1-2 минуты, пока на нижней стороне не появятся большие поджаренные пятна.&nbsp;</font></div><div><font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Снимите со сковороды и заверните в чистое кухонное полотенце.&nbsp;</font><font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Повторите то же самое с остальными частями наана, заворачивая их в полотенце в процессе работы.&nbsp;</font></div>',6);
INSERT INTO recipeStageEntity VALUES(20304,5352,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202170742-6-final.jpg','<font style="box-sizing: border-box; color: rgb(17, 17, 17); font-family: Bentham, serif; font-size: 16px; background-color: rgb(255, 255, 255); vertical-align: inherit;">Смажьте теплый наан 2 столовыми ложками топленого масла и подавайте.&nbsp;</font>',7);
INSERT INTO recipeStageEntity VALUES(20308,5356,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202174518-0-_a7a5587.jpg','<div>Творог, сахар (90 г) и ванильный сахар (10 г) соедините со сметаной (20-30 % - 100 г) и измельчите блендером в однородную массу.&nbsp;</div><div>Если нет блендера, то творог необходимо протереть через сито, соединить с сахаром и сметаной, яйцом и взбить.</div><div>В последнюю очередь добавьте крахмал и хорошо перемешайте .</div>',1);
INSERT INTO recipeStageEntity VALUES(20312,5356,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202174518-1-_a7a5576.jpg','Просеянную муку соедините с просеянным какао-порошком, разрыхлителем, содой и солью.',2);
INSERT INTO recipeStageEntity VALUES(20316,5356,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202174518-2-_a7a5580.jpg','Яйца взбивайте с сахаром и ванильным сахаром в пышную светлую массу в течение 5-7 минут.<div>Добавьте растопленное и охлажденное масло и хорошо смешайте.</div>',3);
INSERT INTO recipeStageEntity VALUES(20320,5356,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202174518-3-_a7a5584.jpg','Добавьте в 2 приема чередуя&nbsp; мучную смесь и сметану. Тщательно смешайте лопаткой.',4);
INSERT INTO recipeStageEntity VALUES(20324,5356,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202174518-4-_a7a5590.jpg','<div>На дно формы вылейте тесто. Ложкой разровняйте и сделайте в центре углубление диаметром 12-13 см.</div><div>В углубление выложите творожную массу, придавая ей округлую форму.</div><div>Слегка постучите формой о стол, чтобы вышли пузырьки воздуха и начинка равномерно распределилась.</div><div>Выпекайте в разогретой до 160°С духовке, на среднем уровне в течение 40-50 минут, без конвекции.</div>',5);
INSERT INTO recipeStageEntity VALUES(20328,5356,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201202174518-5-_a7a5638_1.jpg','<div>После выпекания, выключите духовку, приоткройте дверцу и оставьте пирог на 30 минут.</div><div>Пирог полностью остудите при комнатной температуре и поместите в холодильник минимум на 2 часа.</div><div><br></div><div><br></div>',6);
INSERT INTO recipeStageEntity VALUES(20332,5360,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201203135406-0-sok.jpg','Выжмите сок из лимонов и лайма.',1);
INSERT INTO recipeStageEntity VALUES(20336,5360,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201203135406-1-led.jpg','Поколите лёд.',2);
INSERT INTO recipeStageEntity VALUES(20340,5360,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201203135406-2-so-ldom.jpg','Перемешайте половину сока лимона, половину сока лайма, половину сахарной пудры и половину колотого льда в блендере.&nbsp;<div>Перелейте в кувшин объемом 4 литра. Повторите то же самое с остальными ингредиентами и вылейте их в кувшин. Поместите в морозильную камеру, пока смесь не станет твердой.</div>',3);
INSERT INTO recipeStageEntity VALUES(20344,5360,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201203135407-3-bezalkogolnyi-kokteil-margarita.jpg','Выньте из морозильной камеры за 30 минут до подачи. Натрите ободки стаканов лаймовой долькой; насыпьте соль в плоскую миску и окуните в соль ободки стакана.&nbsp;<div>Добавьте содовую в смесь лимона и лайма, помешивая, пока она не станет жидкой.&nbsp;</div><div>Разлейте по подготовленным стаканам.</div>',4);
INSERT INTO recipeStageEntity VALUES(20452,5384,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204120709-0-riazhenka.jpg','Смешайте ряженку, майонез и сметану. Добавьте несколько капель лимонного сока. Добавьте соль и сахар.',1);
INSERT INTO recipeStageEntity VALUES(20456,5384,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204120709-1-specii.jpg','Смешайте хмели-сунели, карри и паприку. Раздавите зубчики чеснока с помощью пресса для чеснока.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20460,5384,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204120710-2-sous.jpg','Смешайте смесь из первого шага со специями и чесноком до однородности.&nbsp;<div>По возможности оставьте соус&nbsp;&nbsp;на ночь настояться.</div>',3);
INSERT INTO recipeStageEntity VALUES(20480,5392,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204123915-0-ingredienty.jpg','В посуде с горячей водой растворите сахар. Пока полученный сироп остывает до 35 – 40 °С, подсушите ржаной хлеб в духовке до состояния сухарей.&nbsp;<div>В сладкую воду всыпьте квасное сусло и перемешайте. Добавьте в емкость теплые сухари и 1,5 ч. л. дрожжей. При этом хлеб должен быть слегка теплым или холодным, иначе квас не получится. Оптимальной будет температура 37 °С. Все хорошо перемешайте, чтобы на дне не осталось взвесей. Добавьте изюм и тмин.</div>',1);
INSERT INTO recipeStageEntity VALUES(20484,5392,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204123915-1-final.jpg','Прикройте сверху марлей, сложенной 5 – 6 раз и завяжите ее резинкой.&nbsp;<div>Кастрюлю оставьте в тепле, но не на солнце. Заготовку нужно попробовать на вкус через 24 ч. Если напиток стал достаточно вкусным и хмельным, то его пора охладить.&nbsp;</div><div>Спустя 48 ч жидкость процедите и разлейте по бутылкам.&nbsp;</div><div>Квас из сусла готов.</div>',2);
INSERT INTO recipeStageEntity VALUES(20488,5396,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204124949-0-1a7a1319-2.jpg','Зелень и чеснок мелко порубить.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(20492,5396,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204124949-1-1a7a1328-2.jpg','Все компоненты для маринада перемешать.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20496,5396,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204124949-2-1a7a1312.jpg','Данное количество маринада рассчитано на 1 килограмм овощей.&nbsp; Овощи нарезать и выложить в пакет, залить их соусом, завязать и встряхнуть. Оставить пропитываться на 15 минут.<div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(20500,5400,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204130333-0-pitahaia.jpg','Нарежьте дыню и питахайю.',1);
INSERT INTO recipeStageEntity VALUES(20504,5400,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204130333-1-med.jpg','Смешайте соус из меда, лимонного сока и оливкового масла. Заправьте им дыню и питахайю.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20508,5400,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204130333-2-klubnika.jpg','Сделайте из клубники мелкий тартар.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(20512,5400,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204130333-3-final.jpg','Выложите на тарелку дыню, пармскую ветчину, сыр и украсьте клубничным тартаром.',4);
INSERT INTO recipeStageEntity VALUES(20576,5412,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204143916-0-seld.jpg','<div>Очищаем сельдь от кожи, внутренностей и костей.</div><div>Нарезаем филе на ломтики.</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(20580,5412,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204143916-1-marinad.jpg','В небольшую миску наливаем горячую воду, добавляем перец черный и душистый, сахар, уксус и растительное масло. Размешиваем.',2);
INSERT INTO recipeStageEntity VALUES(20584,5412,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204143916-2-luk.jpg','Чистим лук, нарезаем его кольцами или полукольцами.',3);
INSERT INTO recipeStageEntity VALUES(20588,5412,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204143916-3-v-banke.jpg','<div>Выкладываем в баночку слоями: лук и ломтики рыбы. Лавровый лист добавить между слоями.</div><div>Заливаем все маринадом, закрываем баночку крышкой и убираем в холодильник часов на 7- 8.&nbsp;</div><div>Во время маринования пару-тройку раз баночку встряхиваем или переворачиваем, чтобы маринад распределился по всей сельди.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(20592,5416,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204145332-0-luk.jpg','Очистите шалот и чеснок, мелко нарежьте.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(20596,5416,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204145332-1-bekon.jpg','Бекон нарежьте кубиками со стороной 6–7 мм.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20600,5416,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204145332-2-bekon_obzharennyi.jpg','Нагрейте сковороду с оливковым маслом, положите бекон, вытопите жир.',3);
INSERT INTO recipeStageEntity VALUES(20604,5416,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204145333-3-luk_s_bekonom.jpg','Добавьте шалот и чеснок, посолите, поперчите, обжаривайте на слабом огне 3–4 мин.',4);
INSERT INTO recipeStageEntity VALUES(20608,5416,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204145333-4-vino.jpg','Полейте уксусом, готовьте 1 мин. Добавьте мед и вино, увеличьте огонь до средне сильного, готовьте, пока соус не выпарится наполовину.',5);
INSERT INTO recipeStageEntity VALUES(20612,5420,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204161919-0-zelen.jpg','Раздавите, очистите и порубите чеснок. Положите сверху на чеснок петрушку и порубите все вместе как можно мельче.',1);
INSERT INTO recipeStageEntity VALUES(20616,5420,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204161919-1-limon.jpg','Натрите цедру половины лимона и выжмите сок.',2);
INSERT INTO recipeStageEntity VALUES(20620,5420,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204161919-2-zelen_i_chesnok.jpg','Растопите сливочное масло в маленьком сотейнике, положите чеснок и петрушку, а также цедру лимона, немного посолите и прогревайте на среднем огне 1 мин.',3);
INSERT INTO recipeStageEntity VALUES(20624,5420,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204161919-3-vino.jpg','Влейте вино и лимонный сок, выпарите вполовину.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(20628,5420,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204161919-4-gotovo.jpg','Добавьте каперсы и сливки, доведите до кипения, готовьте 1 мин. Поперчите.',5);
INSERT INTO recipeStageEntity VALUES(20632,5424,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204165255-0-pechene.jpg','<div>Печенье и шоколад крупно поломайте.</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(20636,5424,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204165255-1-shokolad.jpg','<div>На водяной бане растопите сливочное масло и шоколад.</div><div>Добавьте какао-порошок, хорошо перемешайте.</div><div>Затем добавьте сгущенное молоко. Перемешайте и доведите до кипения, всё время помешивая. Как начнет закипать, сразу выключите, влейте коньяк и снова хорошо перемешайте.</div>',2);
INSERT INTO recipeStageEntity VALUES(20640,5424,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204165255-2-orehi.jpg','Измельчите орехи.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(20644,5424,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204165255-3-kolbasa.jpg','<div>Готовую массу вылейте в печенье. Добавьте орехи. Перемешайте.</div><div>Заверните в пленку, придавая форму колбасы. Заверните в фольгу и положите в морозилку на 3 часа.</div><div>Через 3-4 часа освободите от пленки, обваляйте в сахарной пудре.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(20648,5428,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204172334-0-popolam-i-travy.jpg','<div>Хорошенько вымойте и обсушите картофель. Не очищая, разрежьте его пополам по всеи? длине.</div><div>Розмарин и тимьян разделите на мелкие веточки, пару нежных кончиков оставьте для украшения, остальное вложите между половинками картофелин.</div>',1);
INSERT INTO recipeStageEntity VALUES(20652,5428,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204172334-1-v-folge.jpg','<div>Соедините половинки и заверните каждую картофелину в фольгу.&nbsp;</div><div>Выложите картофель на противень и отправьте в разогретую до 200 °С духовку на 30–40 минут в зависимости от размера картофеля.</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(20656,5428,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204172335-2-miakot.jpg','<div>Слегка остудите, разверните картофель и уберите зелень (она уже отдала свои? аромат).</div><div>Выньте ложкой из картофеля мякоть.</div>',3);
INSERT INTO recipeStageEntity VALUES(20660,5428,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201204172335-3-v-miske.jpg','<div>При помощи толкушки или пресса растолките в пюре, вмешав масло и теплое молоко до желаемой консистенции.</div><div>Добавьте соль и перец, листочки с оставшихся веточек тимьяна и розмарина.&nbsp;</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(20664,5432,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207120434-0-iaica.jpg','Отделите желтки от белков.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(20668,5432,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207120434-1-muka.jpg','Муку высыпьте на рабочую поверхность, посередине сделайте «колодец» и влейте яичные желтки.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20672,5432,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207120434-2-testo.jpg','Вилкой взбейте яичную массу до однородности, перемешайте с мукой. Месите тесто до состояния эластичности.&nbsp;<div>Заверните в пищевую пленку и положите в холодильник на полчаса.&nbsp;</div>',3);
INSERT INTO recipeStageEntity VALUES(20676,5432,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207120434-3-poloski.jpg','Раскатайте тесто с помощью паста-машины.',4);
INSERT INTO recipeStageEntity VALUES(20680,5432,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207120434-4-gnezda.jpg','Разрежьте на тонкие полоски и придайте необходимую форму.',5);
INSERT INTO recipeStageEntity VALUES(20684,5436,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207121535-0-testo.jpg','<div>Насыпьте на рабочий стол муку горкой и создайте в центре воронку.</div><div>Обсыпьте горку по периметру небольшим количеством соли.</div><div>Разбейте в воронку яйца и перемешайте вилкой желтки с белками.</div><div>Соберите немного муки с краёв воронки.</div><div>Продолжайте перемешивать муку с яйцами, вбирая её постепенно, от центра к краям. Когда замес вилкой станет сложным, перейдите на замес теста пальцами.</div><div>Изначально оно будет немного зернистым.</div><div>Когда тесто примет более плотную консистенцию, перейдите на полный ручной замес, во время которого периодически растягивайте тесто во все стороны и «шмякайте» им по поверхности рабочего стола. Не забывайте периодически посыпать рабочий стол мукой.</div><div>Создайте из теста шар и оставьте его отдыхать на полчаса.</div>',1);
INSERT INTO recipeStageEntity VALUES(20688,5436,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207121535-1-plasty.jpg','Раскатайте тесто с помощью паста-машины.',2);
INSERT INTO recipeStageEntity VALUES(20692,5436,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207121535-2-pasta.jpg','Придайте тесту желаемую форму.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(20696,5440,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207122958-0-luk.jpg','Лук нашинкуйте тонкими кольцами. Растопите на сковороде немного сливочного масла с добавлением растительного и жарьте лук до золотистого цвета.',1);
INSERT INTO recipeStageEntity VALUES(20700,5440,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207122959-1-kartoshka.jpg','<div>Фундук, если нужно, очистите от шелухи и измельчите в блендере в крошку. Кедровые орешки можно оставить целиком.</div><div>Картофель очистите, нарежьте крупными ломтиками, отварите в подсоленной воде с перцем горошком и лавровым листом.</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(20704,5440,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207122959-2-v-kastriule.jpg','<div>Жидкость слейте, сохранив примерно полстакана.</div><div>Растолките картофель толкушкой, вмешав оставшееся сливочное масло и теплые сливки. Вмешайте орехи и жареный лук. Если получилось слишком густо, влейте немного оставшейся от варки картофеля воды.</div><div>Попробуйте, если требуется, добавьте еще соли и перца.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(20708,5444,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207124121-0-pochistili.jpg','Картофель очистите, нарежьте крупными ломтиками и сварите в подсоленнои? воде.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(20712,5444,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207124121-1-zelen-i-chesnok.jpg','Чеснок и петрушку (тархун) мелко нарежьте.',2);
INSERT INTO recipeStageEntity VALUES(20716,5444,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207124121-2-v-kastriule.jpg','Растолките картофель в пюре, вмешаи?те сливки, масло и сметану. Добавьте паприку, корицу, петрушку и чеснок, вмешаи?те желтки.',3);
INSERT INTO recipeStageEntity VALUES(20720,5444,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207124121-3-v-forme.jpg','Выложите пюре в форму для запекания и отправьте в разогретую до 180 °С духовку минут на 20.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(20724,5448,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207124646-0-varitsia.jpg','Картошку хорошенько вымойте. Прямо в кожуре залейте водой и отварите до готовности. Воду слейте.',1);
INSERT INTO recipeStageEntity VALUES(20728,5448,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207124646-1-zelen.jpg','Зелень мелко нашинкуйте.',2);
INSERT INTO recipeStageEntity VALUES(20732,5448,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207124646-2-s-zeleniu.jpg','Заправьте картофель маслом и зеленью, посолите и поперчите. Подавайте.',3);
INSERT INTO recipeStageEntity VALUES(20748,5456,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207131331-0-testo.jpg','В стакан влейте разболтанное яйцо, долейте воды доверху, вылейте в мисочку. Добавьте туда соль, лимонный сок и еще 1/2 стакана воды. Размешайте и добавьте эту смесь в просеянную муку. Замесите однородное тесто.',1);
INSERT INTO recipeStageEntity VALUES(20752,5456,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207131331-1-paket.jpg','Переложите тесто на рабочую поверхность, вымешивайте его 5–7 мин. до полной однородности и эластичности. Скатайте в шар, накройте полотенцем и оставьте на 20 мин. Затем раскатайте в пласт (середина должна быть толще, чем края), присыпьте мукой, опять накройте, оставьте на 10 мин.<div>Масло размягчите при комнатной температуре и разомните в миске, чтобы не осталось комков. Присыпьте 3 ч. л. муки и тщательно перемешайте. Положите масло в морозильник на 30 мин.</div>',2);
INSERT INTO recipeStageEntity VALUES(20756,5456,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207131331-2-kraia.jpg','Затем выньте, нарежьте тонкими пластинками и выложите их на пласт теста: по длинным сторонам до краев нужно оставить 1,5–2 см, а короткие края завернуть поверх масла и скрепить между собой. Защипните открытые стороны получившегося прямоугольника.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(20760,5456,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207131331-3-slozhit.jpg','Положите его длинной стороной перед собой; зрительно разделите прямоугольник на 4 вертикальные полосы; затем сложите его по этим «сгибам» вчетверо.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(20764,5456,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207131331-4-sloi.jpg','Положите заготовку в холодильник на 20 мин., выньте и раскатайте на присыпанной мукой поверхности в прямоугольник толщиной 1 см. Повторите с ним тот же процесс складывания вчетверо, чтобы получилось 16 слоев. Положите в холодильник на 30 мин. Затем повторите ту же операцию (раскатывания-складывания-охлаждения) еще 2 раза – 64 и 256 слоев. Охлаждайте 30 мин.',5);
INSERT INTO recipeStageEntity VALUES(20768,5460,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207132940-0-chai.jpg','Заварите любой ваш любимый чай: чёрный, зелёный, фруктовый или матчу.',1);
INSERT INTO recipeStageEntity VALUES(20772,5460,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207132940-1-syr.jpg','Сливочный сыр перемешайте до однородной кремообразной массы с сахарной пудрой и сливками.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20776,5460,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207132940-2-penka.jpg','Взбейте с помощью вспенивателя для молока, миксера или блендера.',3);
INSERT INTO recipeStageEntity VALUES(20780,5460,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207132940-3-final.jpg','<div>В кружку налейте чай на 2/3 объёма, сверху выложите сливочно-сырную шапочку.</div>',4);
INSERT INTO recipeStageEntity VALUES(20784,5464,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207134759-0-1a7a1816.jpg','<div>Яйцо отварите.</div><div>Лук нарежьте мелкими кубиками, переложите в миску, добавьте уксус и воду. Маринуйте 10 минут.</div><div>Лишнее масло с тунца слейте, переложите тунец в миску.</div><div>Вареное куриное яйцо почистите, натрите на мелкой терке и добавьте к тунцу.</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(20788,5464,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207134759-1-1a7a1845.jpg','<div>Следом добавьте к рыбе маринованный лук, соль и перец по вкусу.</div><div>Переложите все в блендер, измельчите на высоких оборотах до кремовой консистенции, отрегулируйте вкус. Готовый паштет подавайте к столу.</div>',2);
INSERT INTO recipeStageEntity VALUES(20792,5468,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207140457-0-kartoshka.jpg','<div>Сварите яйца вкрутую, охладите под струей холодной воды и очистите.&nbsp;</div><div>Картофель очистите, нарежьте мелкими кубиками, залейте 2 стаканами кипящей воды, доведите до кипения и варите почти до готовности, 5-7 мин. Снимите с огня и остудите картофель в отваре, 30 мин.</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(20796,5468,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207140457-1-zelen.jpg','Зеленый лук мелко нарежьте. Белую часть всыпьте в кастрюлю, добавьте соль и вареные желтки. Разотрите пестиком до состояния однородной пасты, разведите остывшим отваром с картофелем.',2);
INSERT INTO recipeStageEntity VALUES(20800,5468,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207140457-2-krab.jpg','Крабовое мясо разберите на волокна.',3);
INSERT INTO recipeStageEntity VALUES(20804,5468,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207140457-3-ogurcy.jpg','По желанию огурцы очистите от кожуры, нарежьте мелкими кубиками.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(20808,5468,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207140457-4-redis.jpg','Редис нарежьте тонкими ломтиками.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(20812,5468,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207140457-5-iaico.jpg','Белки измельчите.',6);
INSERT INTO recipeStageEntity VALUES(20816,5468,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207140457-6-final.jpg','<div>Добавьте овощи, крабовое мясо и зеленую часть лука в кастрюлю, влейте простоквашу. Размешайте, приправьте перцем, закройте крышкой, поставьте в холодильник минимум на 2-3 ч. Подавайте окрошку холодной.</div><div><br></div>',7);
INSERT INTO recipeStageEntity VALUES(20820,5472,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207151348-0-sous.jpg','Для заправки: в маленькой баночке, закрывающейся крышкой, соедините масло, уксус, горчицу и чеснок. Закройте крышку и потрясите, чтобы все ингредиенты хорошо перемешались. Оставьте как минимум на 15 минут.',1);
INSERT INTO recipeStageEntity VALUES(20824,5472,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207151349-1-svekla.jpg','Свеклу, предварительно отваренную или испеченную, очистите и нарежьте небольшими дольками.',2);
INSERT INTO recipeStageEntity VALUES(20828,5472,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207151349-2-final.jpg','<div>В большой миске соедините рукколу (если листья слишком крупные, порвите их), нут и лук. Добавьте заправку, предварительно вытащив из нее чеснок. Перемешайте.</div><div>Разложите салат по тарелкам. Сверху выложите свеклу. Поперчите, посолите. Подавайте сразу.</div>',3);
INSERT INTO recipeStageEntity VALUES(20832,5476,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207153737-0-pasta.jpg','Закиньте лапшу в кипящую воду и варите в течение 3-х минут.<div>Разведите крахмал в 20 мл холодной воды.</div>',1);
INSERT INTO recipeStageEntity VALUES(20836,5476,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207153737-1-krevetki.jpg','Нарежьте овощи, грибы, имбирь, чеснок и кинзу мелкой соломкой, отрежьте 3 тонких пластинки чили.<div>В сильно разогретый вок влейте подсолнечное масло, накалите до появления дыма, обжарьте креветки в течение 1 минуты, отложите креветки, помойте сковородку.</div>',2);
INSERT INTO recipeStageEntity VALUES(20840,5476,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207153737-2-griby.jpg','В сильно разогретый вок влейте подсолнечное масло, накалите до появления дыма, обжарьте овощи и грибы в течение 2-х минут, добавьте горошек и ростки сои, имбирь, чеснок, пластинки чили перца, обжаривайте еще 1 минуту.',3);
INSERT INTO recipeStageEntity VALUES(20844,5476,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207153737-3-perec.jpg','Добавьте кунжутное масло, соевый соус, растворенный в воде крахмал, кунжутные семечки, кинзу и обжаренные креветки, тушите в течение 30 секунд.',4);
INSERT INTO recipeStageEntity VALUES(20848,5476,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201207153737-4-pasta_s_ovoshami.jpg','Соедините лапшу с овощами и креветками, снимите с огня и добавьте кунжутное масло.',5);
INSERT INTO recipeStageEntity VALUES(20852,5480,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208115058-0-luk.jpg','Лук мелко нарежьте.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(20856,5480,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208115058-1-ryba.jpg','Лосось порежьте кубиками, перемешайте с каперсами и мелко порезанным луком.',2);
INSERT INTO recipeStageEntity VALUES(20860,5480,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208115058-2-sous.jpg','Смешайте горчицу, лимонный сок и масло, если нужно посолите и поперчите. Добавьте соус к рыбе. Разделить на две порции.',3);
INSERT INTO recipeStageEntity VALUES(20864,5480,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208115058-3-avokado.jpg','Авокадо почистите и порежьте либо кубиками, либо ломтиками.',4);
INSERT INTO recipeStageEntity VALUES(20868,5480,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208115058-4-final.jpg','При помощи формы-кольца выложите нижний слой из авокадо, сверху положите слой из лосося. Сверху украсьте лимоном, рукколой или укропом.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(20872,5484,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208123140-0-goviadina.jpg','Перед приготовлением блюда мясо необходимо промыть в холодной воде и зачистить от пленок.<div>Далее говядину нарубите вручную до размера кубиков примерно равных 0,3–0,5 см. Можно также пропустить через мясорубку с решеткой с крупными отверстиями.</div>',1);
INSERT INTO recipeStageEntity VALUES(20876,5484,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208123140-1-sous.jpg','Смешайте французскую горчицу, соус уорчестер, соус табаско (либо аналог), черный перец.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20880,5484,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208123140-2-miaso.jpg','В мясо добавьте соус.&nbsp;Тщательно перемешайте и отправьте в холодильник на 30 минут.',3);
INSERT INTO recipeStageEntity VALUES(20884,5484,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208123140-3-final.jpg','Мелко порубите каперсы, маринованные огурцы и лук. Багет нарежьте ломтиками и отправьте подсушиться в духовой шкаф на 10 минут. Можно вместо багета использовать тостовый хлеб.<div>Достаньте уже подмариновавшееся мясо из холодильника и выложите его в формовочное кольцо.&nbsp;</div><div>Добавьте в качестве гарнира лук, каперсы и огурцы. Положите рукколу. Украсьте это все глазурью с добавлением бальзамического уксуса, а также желтком перепелиного яйца. Посолите.</div>',4);
INSERT INTO recipeStageEntity VALUES(20900,5492,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208135231-0-smes.jpg','<div>Нагрейте растительное масло в большой кастрюле на среднем или сильном огне.</div><div>В большой миске смешайте муку, кукурузную муку, сахар, разрыхлитель и соль. Влейте пахту и слегка взбитые яйца. Смешайте до однородной массы. Перелейте в высокий стакан для питья.</div>',1);
INSERT INTO recipeStageEntity VALUES(20904,5492,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208135231-1-stakan.jpg','Проткните сосиски тонкой палочкой для мороженого. Обваляйте нарезанные сосиски в муке и стряхните излишки. Обмакните посыпанные мукой сосиски в стакан с кукурузным тестом. Поднимите и снова окуните, чтобы полностью покрыть.',2);
INSERT INTO recipeStageEntity VALUES(20908,5492,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208135231-2-obzharit.jpg','Осторожно выложите корн-дог в горячее масло. Готовьте примерно по два за раз. Обжаривайте до золотистого цвета, примерно 3-5 минут, перевернув на полпути. Слейте излишки масла. Положите на тарелку, застеленную бумажными полотенцами, чтобы они впитали лишний жир. Переложите на решетку, поставленную на противень. Продолжайте жарить, пока все корн-доги не будут приготовлены. Поместите противень в разогретую духовку до 120 градусов.<div>Выложите кукурузу на противень и сбрызните оливковым маслом. Поместите под горячую жаровню и готовьте примерно 4-6 минут. Достаньте из-под жаровни и дайте настояться, пока не остынет, чтобы срезать ядра с початков.</div>',3);
INSERT INTO recipeStageEntity VALUES(20912,5492,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208135231-3-sous.jpg','В небольшой миске взбейте майонез, сметану, чеснок, соль, перец и щепотку кайенского перца до однородной массы. Полейте смесью нагретые корн-доги, посыпьте кукурузой, сыром, кинзой и кайенским перцем. Подавайте немедленно.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(20916,5496,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161139-0-_a7a5594.jpg','Для начинки тщательно перебейте погружным блендером до однородной массы творог, сахар (70-100 г), ванильный сахар, яйца, сметану и кукурузный крахмал.<div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(20920,5496,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161139-1-_a7a5597.jpg','Муку и какао просейте, добавьте сахар (80-100 г) и щепотку соли.',2);
INSERT INTO recipeStageEntity VALUES(20924,5496,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161139-2-_a7a5600.jpg','Охлажденное сливочное масло натрите на терке, потом руками разотрите в мелкую крошку.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(20928,5496,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161139-3-_a7a5615.jpg','2/3 крошки отправьте в форму, сформируйте дно и бортики.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(20932,5496,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161139-4-_a7a5619.jpg','Выложите творожную массу.',5);
INSERT INTO recipeStageEntity VALUES(20936,5496,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161139-5-_a7a5622.jpg','Присыпьте оставшейся крошкой. Выпекайте при 180 градусах 40 минут. Дайте полностью остыть при комнатной температуре, накройте пленкой и поставьте в холодильник на 1-2 часа.',6);
INSERT INTO recipeStageEntity VALUES(20940,5500,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161735-0-_a7a5741.jpg','Просейте муку.',1);
INSERT INTO recipeStageEntity VALUES(20944,5500,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161735-1-_a7a5742.jpg','Замешайте все ингредиенты в плотное тесто.',2);
INSERT INTO recipeStageEntity VALUES(20948,5500,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208161735-2-_a7a5745.jpg','Оставьте на 30 мин под пленкой при комнатной температуре.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(20952,5504,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208163510-0-ogurcy.jpg','<div>Огурчики помойте тщательно щеткой, замочите их в холодной воде на час.</div><div>В это время помойте зелень, листья и банки с крышками.&nbsp;</div>',1);
INSERT INTO recipeStageEntity VALUES(20956,5504,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208163510-1-nachalo_v_banke.jpg','На дно банки положите свежую зелень, все специи и чеснок.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(20960,5504,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208163510-2-ogurcy_v_banke.jpg','Наполните банку доверху чистыми огурчиками. Старайтесь хорошо их утрамбовать.',3);
INSERT INTO recipeStageEntity VALUES(20964,5504,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208163511-3-banka.jpg','Вскипятите воду в чайнике. Залейте кипятком огурцы так, чтобы вода перетекала за края банки, прикройте крышкой. Оставьте так на 15 минут.',4);
INSERT INTO recipeStageEntity VALUES(20968,5504,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208163511-4-marinad.jpg','Слейте воду с огурцов в сотейник, всыпьте в нее соль с горкой и сахар. Емкость поставьте на плиту, доведите до кипения. Как только закипит, влейте уксус и сразу же разлейте маринад по банкам, чтобы он вытекал из-под крышки.',5);
INSERT INTO recipeStageEntity VALUES(20972,5504,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208163511-5-perevernut.jpg','Быстро закрутите банки крышками или закатайте специальным ключом. Переверните вверх дном, накройте теплым одеялом и оставьте в таком положении до полного остывания.',6);
INSERT INTO recipeStageEntity VALUES(20976,5504,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208163511-6-final.jpg','<div>Когда заготовки полностью остынут, перенесите банки в кладовую на хранение. Если хотите, храните закрутки в темном прохладном месте. Готовыми огурчики будут приблизительно через месяц.</div>',7);
INSERT INTO recipeStageEntity VALUES(20980,5508,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208165148-0-sito.jpg','Ягоды с косточками предварительно перебрать и промыть под проточной водой, затем откинуть на дуршлаг для избавления от излишков жидкости.',1);
INSERT INTO recipeStageEntity VALUES(20984,5508,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208165149-1-vishnia_v_banke.jpg','Подготовленную ягоду заложить в чистую и заранее простерилизованную банку.',2);
INSERT INTO recipeStageEntity VALUES(20988,5508,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208165149-2-kompot.jpg','Залить кипятком, прикрыть пропаренной крышкой. В таком виде оставить на 15 минут. За это время вишня отдаст свой цвет и станет мягче.',3);
INSERT INTO recipeStageEntity VALUES(20992,5508,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208165150-3-kastriulia.jpg','Банку закрыть специальной крышкой с дырочками и через нее слить воду без ягод в кастрюлю, растворить в ней сахар, вкипятить.',4);
INSERT INTO recipeStageEntity VALUES(20996,5508,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201208165150-4-kompot_v_banke.jpg','<div>Снятый с огня сироп снова залить в банку с вишней, накрыть крышкой и через 15 минут повторить предыдущую процедуру.</div><div>Кипящим сиропом залить ягоды и герметично закатать. Перевернуть банку на крышку, закутать в одеяло и, после остывания, убрать на хранение в кладовку.</div>',5);
INSERT INTO recipeStageEntity VALUES(21000,5514,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209112035-0-luk.jpg','Помойте помидоры и зелень. Очистите чеснок и лук, нарежьте кольцами.',1);
INSERT INTO recipeStageEntity VALUES(21003,5514,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209112036-1-pomidory_v_banke.jpg','<div>Положите в сухие простерилизованные банки томаты, зелень, лук, зубчики чеснока, лавровый лист и перец.</div>',2);
INSERT INTO recipeStageEntity VALUES(21006,5514,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209112036-2-zalit.jpg','<div>Залейте кипятком на 10 минут, слейте и залейте ещё раз другим кипятком на 15 минут.</div>',3);
INSERT INTO recipeStageEntity VALUES(21009,5514,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209112036-3-kastriulia.jpg','<div>Слейте «вторую воду» в кастрюлю, добавьте соль, сахар, уксус. Вскипятите маринад.</div>',4);
INSERT INTO recipeStageEntity VALUES(21012,5514,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209112036-4-zakryt.jpg','<div>Залейте помидоры маринадом, закатайте крышками. Переверните вверх дном до остывания.</div>',5);
INSERT INTO recipeStageEntity VALUES(21015,5517,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209113814-0-slivy.jpg','Сливы помойте и отделите косточки.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21018,5517,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209113814-1-v_kastriule.jpg','Сливы сложите в кастрюльку мультиварки, включите программу Тушение на 40 мин. За это время сливы должны хорошо пропариться, и дойти почти до кипения.',2);
INSERT INTO recipeStageEntity VALUES(21021,5517,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209113815-2-skovoroda.jpg','Выключите мультиварку и переложите сливы в глубокий сотейник. Ручным блендером хорошо измельчите сливы.&nbsp;<div>Отвесьте в мисочку сахар. Добавьте пару ложек сахара, щепотку корицы.</div><div>Поставьте сотейник в включенную на 180 градусов духовку.&nbsp;<br>Дверцу плотно не прикрывайте.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21024,5517,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209113815-3-v_banke.jpg','Через каждые 20 - 30 мин добавляйте еще по 2 - 3 ст. л сахара, пока весь сахар не закончится и джем хорошо не загустеет. В общей сложности джем варится в духовке 3 - 3,5 часа. Готовый джем разложите по баночкам и плотно укупорите.&nbsp;<div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21027,5520,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209115308-0-slivy.jpg','Переберите, промойте и очистите от косточек сливы.',1);
INSERT INTO recipeStageEntity VALUES(21030,5520,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209115308-1-kastriulia.jpg','Смешайте в кастрюле воду, вино, часть сахара и специи. Сварите сироп.&nbsp;По мере варки сиропа отрегулируйте сладость, доведите до кипения и проварите на слабом огне еще 5 минут.',2);
INSERT INTO recipeStageEntity VALUES(21033,5520,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209115308-2-banka_1.jpg','Сливы разложите по банкам.',3);
INSERT INTO recipeStageEntity VALUES(21036,5520,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209115308-3-banka_2.jpg','Залейте сливы горячим сиропом.',4);
INSERT INTO recipeStageEntity VALUES(21039,5520,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209115308-4-banka_3.jpg','Накройте банки крышками.',5);
INSERT INTO recipeStageEntity VALUES(21042,5520,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209115308-5-sterilizovat.jpg','Простерилизуйте в кастрюле с водой 15 минут.',6);
INSERT INTO recipeStageEntity VALUES(21045,5520,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209115308-6-final.jpg','Закатайте банки, охладите в перевернутом положении, закутанными в одеяло.',7);
INSERT INTO recipeStageEntity VALUES(21048,5523,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209123116-0-soteinik.jpg','Сделайте заправку. Возьмите миску, налейте в нее горячую воду, добавьте соль и уксус, и тщательно размешайте до растворения.',1);
INSERT INTO recipeStageEntity VALUES(21051,5523,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209123117-1-v_vode.jpg','Огурцы хорошо промойте, уложите в глубокую емкость, залейте приготовленной заправкой и оставьте остывать.',2);
INSERT INTO recipeStageEntity VALUES(21054,5523,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209123117-2-zelen.jpg','Чеснок очистите от шелухи, зелень сполосните и все мелко нарежьте. Затем компоненты соедините вместе в отдельной миске.',3);
INSERT INTO recipeStageEntity VALUES(21057,5523,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209123117-3-nafarshirovannye.jpg','Когда настой полностью остынет, выньте огурцы, с помощью ножа сделайте на каждом продольный разрез не до краев и заполните их начинкой из зелени и чесночных зубчиков.',4);
INSERT INTO recipeStageEntity VALUES(21060,5523,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209123117-4-v_banke.jpg','<div>Подготовьте стерилизованную банку для заготовки.&nbsp;</div><div>Аккуратно уложите в нее фаршированные корнишоны практически до самого верха. После залейте рассолом, добавьте растительное масло, накройте крышкой и закатайте.</div><div>Охладите полученное блюдо в течение двух суток при комнатной температуре. Затем уберите на хранение.</div>',5);
INSERT INTO recipeStageEntity VALUES(21063,5526,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209125419-0-shipovnik.jpg','Яблоки промойте, просушите на полотенце, а плоды шиповника, так же подсушите и очистите от цветков.',1);
INSERT INTO recipeStageEntity VALUES(21066,5526,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209125419-1-iaboki.jpg','Яблоки нарежьте.',2);
INSERT INTO recipeStageEntity VALUES(21069,5526,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209125419-2-v_banke.jpg','В предварительно простерилизованные банки (лучше всего для этих целей подойдут трёхлитровые стеклянные банки) уложите на дно примерно на одну треть от ее объёма яблоки и шиповник по вкусу и вашим предпочтениям.<div>Залейте содержимое банки кипятком до самого верха. Накройте банку крышкой и дайте в таком положении постоять в течении пяти-десяти минут.</div>',3);
INSERT INTO recipeStageEntity VALUES(21072,5526,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209125419-3-sirop.jpg','Слейте воду в кастрюльку, добавьте туда сахарный песок и кипятите, до полного растворения сахара.',4);
INSERT INTO recipeStageEntity VALUES(21075,5526,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209125419-4-zakrutit.jpg','<div>Залейте обратно в банку и закройте ее крышкой при помощи закаточной машинки. Переверните банки вверх дном, тем самым проверяя прочность и плотность прилегания крышки, укутайте их одеялом и дайте полностью остыть.</div>',5);
INSERT INTO recipeStageEntity VALUES(21078,5529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209131616-0-iaica.jpg','<div>Яйца отварите вкрутую и разрежьте вдоль на 4 части.</div>',1);
INSERT INTO recipeStageEntity VALUES(21081,5529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209131616-1-redis.jpg','Редис нарежьте слайсами.',2);
INSERT INTO recipeStageEntity VALUES(21084,5529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209131616-2-pomidory.jpg','Помидоры разрежьте пополам.',3);
INSERT INTO recipeStageEntity VALUES(21087,5529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209131617-3-luk.jpg','Красный лук режьте перьями.',4);
INSERT INTO recipeStageEntity VALUES(21090,5529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209131617-4-zapravka.jpg','Заправка - оливковое масло смешивайте с лимонным соком, добавьте тимьян, соль и перец. Заправку взбейте венчиком или вилкой.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(21093,5529,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209131618-5-final.jpg','<div>Выложите на тарелку салат корн, на него яйца, редис, помидоры, лук и сверху полейте заправкой. Салат не перемешивайте.</div>',6);
INSERT INTO recipeStageEntity VALUES(21096,5532,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209133528-0-razdelili.jpg','Яйца разделите на желтки и белки.',1);
INSERT INTO recipeStageEntity VALUES(21099,5532,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209133528-1-zheltki.jpg','Желтки разотрите с сахаром. Добавьте молоко и растопленное сливочное масло. Перемешайте.',2);
INSERT INTO recipeStageEntity VALUES(21102,5532,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209133528-2-smes-2.jpg','<div>Добавьте муку, смешанную с какао и разрыхлителем.</div><div>Перемешайте.</div>',3);
INSERT INTO recipeStageEntity VALUES(21105,5532,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209133528-3-belki.jpg','<div>Белки взбейте с щепоткой соли в устойчивую пену.</div>',4);
INSERT INTO recipeStageEntity VALUES(21108,5532,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209133529-4-smes.jpg','Добавьте белки в тесто. Аккуратно перемешайте.&nbsp;<div>Тесто получится очень пышным и воздушным.</div>',5);
INSERT INTO recipeStageEntity VALUES(21111,5532,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209133529-5-vypekli.jpg','<div>На хорошо разогретую сухую сковороду выкладывайте 2-3 столовые ложки теста и готовьте панкейки на слабом огне.</div><div>Как только увидите на поверхности теста пузырьки, переворачивайте и пеките с другой стороны.</div><div>Готовые панкейки выкладывайте на решетку и в таком виде остудите полностью.</div>',6);
INSERT INTO recipeStageEntity VALUES(21114,5532,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209133529-6-final.jpg','<div>Пока панкейки остывают, приготовьте ганаш.</div><div>Выложите масло и кусок шоколада в миску, поставьте на малый огонь, и, постоянно помешивая, доведите смесь до нужной консистенции. Шоколад должен полностью растопиться, и смесь стать глянцевой. Но в данном случае от плиты не отходите.</div><div>Можно отдельно подогреть сливки на плите или в микроволновке (довести их до кипения), затем добавить в горячие сливки нарезанный мелко шоколад и перемешать.</div><div>Ганаш готов.</div><div>Подавайте панкейки либо вприкуску с ганашем, либо обильно полив панкейки ганашем.&nbsp;</div>',7);
INSERT INTO recipeStageEntity VALUES(21144,5540,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209142414-0-shar-testa.jpg','Компоненты для коржа (муку, 100 гр. холодного сливочного масла,<div>1 столовая ложка сахарной пудры, 1 яйцо, щепотка соли) быстро смешайте, оберните тесто пленкой и поставьте на 30 минут в холодильник.</div>',1);
INSERT INTO recipeStageEntity VALUES(21148,5540,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209142414-1-korzh.jpg','<div>Разъемную форму диаметром 22 см, застелите пергаментной бумагой и смажьте маслом и посыпьте мукой.</div><div>Тесто выложите в форму, проколите вилкой по всей поверхности и поставьте в духовку, предварительно нагретую до 190 ° градусов.</div><div>Выпекайте около 25 минут. Готовый корж достаньте из духовки и охладите.</div>',2);
INSERT INTO recipeStageEntity VALUES(21152,5540,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209142414-2-chereshnia.jpg','Черешню вымойте и удалите косточки.',3);
INSERT INTO recipeStageEntity VALUES(21156,5540,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209142414-3-zhelatin.jpg','Желатин положите в кастрюлю и налейте немного воды (около 1/3 стакана воды) и дайте постоять.',4);
INSERT INTO recipeStageEntity VALUES(21160,5540,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209142414-4-tvorog.jpg','Смешайте творог с сахарной пудрой, ванильным сахаром и сливками.',5);
INSERT INTO recipeStageEntity VALUES(21164,5540,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209142414-5-krem.jpg','<div>Разбухший желатин поставьте на медленный огонь, помешивая, пока не растворится. Снимите с огня.</div><div>Добавьте желатин в творожную начинку и перемешайте энергично. Добавьте черешню и перемешайте ложкой.</div><div>Выложите готовую начинку на корж в форме, выровняйте по всей поверхности и поставьте в холодильник на несколько часов.</div>',6);
INSERT INTO recipeStageEntity VALUES(21168,5540,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209142414-6-final.jpg','Растопите шоколад на пару или в микроволновой печи. Выложите красиво целые ягоды черешни и полейте растопленным шоколадом.',7);
INSERT INTO recipeStageEntity VALUES(21172,5544,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209144523-0-luk-nashinkovali.jpg','Нарежьте мелко лук. В сотейник отправьте сливочное масло и добавьте лук. Готовьте лук.',1);
INSERT INTO recipeStageEntity VALUES(21176,5544,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209144523-1-luk.jpg','Пока лук готовится нарежьте чили перцы, тоже мелко и отправьте к луку. Тушите до золотистости лука на среднем огне.',2);
INSERT INTO recipeStageEntity VALUES(21180,5544,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209144523-2-mango.jpg','Почистите манго и нарежьте мелко, добавьте к луку и чили тушиться.',3);
INSERT INTO recipeStageEntity VALUES(21184,5544,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209144524-3-mango-v-kastriule.jpg','Добавьте имбирь (сухой или мелко тертый), выжмите сок 1/2 лайма, добавьте уксус и сахар. Дайте потушиться минут 15.',4);
INSERT INTO recipeStageEntity VALUES(21188,5544,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209144524-4-svarili.jpg','Весь соус немного помните толкучкой (что бы размять манго), если куски еще твердые дайте еще потушиться (под крышкой). Как манго будет готово доведите (выпаривайте) соус до густоты.',5);
INSERT INTO recipeStageEntity VALUES(21192,5544,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209144524-5-v-banke.jpg','Залейте в соус в простерилизованную банку (250 г) и закройте.&nbsp;',6);
INSERT INTO recipeStageEntity VALUES(21196,5548,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209150307-0-miaso.jpg','Мясо помойте и порежьте на достаточно крупные куски - примерно 7-8 см.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21200,5548,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209150308-1-salo.jpg','Жир или сало порежьте на мелкие кубики.',2);
INSERT INTO recipeStageEntity VALUES(21204,5548,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209150309-2-lavrovyi_list.jpg','В сухую, чистую банку положите перец горошек и лавровый лист.',3);
INSERT INTO recipeStageEntity VALUES(21208,5548,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209150309-3-miaso_v_banke.jpg','Выложите мясо, укладывая плотно, но не утрамбовывая.<div><div>Когда до верха банки останется примерно 3-4 см, добавьте кусочки сала. Добавьте соль и воду.</div><div>Мясо в банке обязательно утрамбуйте перед духовкой.</div><div>Банку накройте сверху металлической крышкой для консервирования, предварительно вынув из нее резинку.</div></div>',4);
INSERT INTO recipeStageEntity VALUES(21212,5548,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209150310-4-tushenka.jpg','<div>Поставьте в разогретую до 200 градусов духовку и ждите закипания. Желательно ставить на противень, т.к. при кипении сок из банки может вытекать.</div><div>Следите, чтобы жидкость всегда закрывала банку за 2 см до горлышка. при необходимости подливайте кипятка.</div><div>Как только вы увидели, что тушенка начинает закипать, убавьте температуру до 150 градусов и забудьте о ней ровно на 3 часа.</div><div>По истечении времени, аккуратно достаньте тушенку, вставьте в крышку резинку и сразу же закатайте. Переверните банки вниз крышкой и оставить до полного остывания.&nbsp;</div>',5);
INSERT INTO recipeStageEntity VALUES(21240,5556,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209161044-0-muka.jpg','На обратной стороне листа пергаментной бумаги нарисуйте круги диаметром 3-4 см – это будет ваш трафарет для выпечки макарон.<div>Смешайте миндальную муку и сахарную пудру. Измельчите смесь еще раз в кофемолке или блендере. Просейте.<br><div>Разогрейте духовку до 150С, в течение 5 мин просушите смесь. Еще раз просейте и добавьте какао-порошок.</div></div>',1);
INSERT INTO recipeStageEntity VALUES(21244,5556,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209161045-1-belki.jpg','<div>В миску с мукой добавьте первую партию белков (55г) и отставьте емкость в сторону.</div><div>Белки (110 г) взбейте миксером в легкую пену, затем постепенно всыпайте сахар (30г).&nbsp; Продолжайте взбивать до мягких пиков.</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(21248,5556,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209161045-2-testo.jpg','<div>Одновременно в сотейнике доведите до кипения (100С) воду и сахар (300г), добавьте краситель.</div><div>Продолжайте держать сотейник на огне, пока сироп не прогреется до 117С. Сразу же тоненькой струйкой вливайте его во взбивающиеся на средней скорости белки.</div><div>Продолжайте взбивать до тех пор, пока масса не остынет и не станет блестящей, густой и гладкой. Это и есть ваша меренга.</div><div><div>Возьмите ранее отставленную в сторону миску с мукой и белками, разотрите смесь в миндальную пасту, и соедините ее с меренгой. Аккуратно перемешайте лопаткой, при этом второй рукой вращая миску по часовой стрелке.</div><div>Тесто должно стать мягким, блестящим, однородным.</div></div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21252,5556,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209161046-3-kryshechki.jpg','<div>Сложите тесто в кулинарный мешок или пакет и отсадите его кружочками на заготовленный пергамент. Оставьте заготовки при комнатной температуре на 30-40 минут.</div><div>Выпекайте меренги 12-15 мин в разогретой духовке до 150-155С.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21256,5560,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209162533-0-muka.jpg','<div>Просейте миндальную муку и сахарную пудру. Сухую смесь перемешайте в течение 3 минут на маленькой скорости миксером или в комбайне.&nbsp;</div><div>Разогрейте духовку до 150С, в течение 5 мин просушите смесь. Еще раз просейте вместе с какао-порошком.</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(21260,5560,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209162533-1-belki.jpg','В белки добавьте семена ванили. Взбейте белки в пушистую пену, постепенно небольшими порциями добавляйте сахар и продолжайте взбивать до устойчивых пиков. Оставьте белки на 2 минуты.',2);
INSERT INTO recipeStageEntity VALUES(21264,5560,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209162533-2-testo.jpg','<div>Аккуратно соедините взбитые белки с сухой смесью и перемешайте - снизу вверх, крутя миску по часовой стрелке.</div><div>Тесто должно стать блестящим, податливым и мягкой лентой стекать с лопатки.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21268,5560,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201209162534-3-kryshechki.jpg','Сложите тесто в кулинарный мешок или пакет и отсадите его кружочками на заготовленный пергамент. Вы замесили тесто правильно, если «хвостики» на поверхности пирожных сами расходятся, образуя гладкую ровную поверхность.<div><div>Оставьте заготовки при комнатной температуре на 30-60 минут. На поверхности теста должна образоваться очень тонкая пленочка (корочка), не липнущая к пальцам. При выпекании корочка становится хрустящей, а под ней образуется «юбочка».</div><div>Выпекайте меренги 13-15 мин в разогретой духовке до 150-155С.</div><div><br></div></div>',4);
INSERT INTO recipeStageEntity VALUES(21272,5564,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210121638-0-_a7a6498.jpg','Нарежьте кусочки хлеба и необходимые продукты.<div>Поджарьте хлеб на сухой сковороде.</div>',1);
INSERT INTO recipeStageEntity VALUES(21276,5564,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210121638-1-_a7a6572.jpg','Выложите на хлеб моцареллу.',2);
INSERT INTO recipeStageEntity VALUES(21280,5564,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210121638-2-_a7a6576.jpg','Добавьте листья базилика и помидоры.',3);
INSERT INTO recipeStageEntity VALUES(21284,5564,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210121638-3-_a7a6582.jpg','Смажьте все песто.&nbsp;',4);
INSERT INTO recipeStageEntity VALUES(21288,5564,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210121638-4-final.jpg','<div>Накройте все еще одним кусочком хлеба.&nbsp;</div><div>Обжарьте на гриле или на сковороде, прижимая сэндвич лопаткой.</div>',5);
INSERT INTO recipeStageEntity VALUES(21292,5568,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210123612-0-_a7a5947.jpg','Перемешайте в деже до однороднои? массы на 2-и? скорости миксера. Возьмите сметану и добавьте к креметте. Также перемешайте на 2-и? скорости.',1);
INSERT INTO recipeStageEntity VALUES(21296,5568,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210123612-1-_a7a5950.jpg','Возьмите 4 яи?ца, 2 желтка и 200 г сахара. Перемешайте и добавьте к креметте со сметанои?, взбейте до однороднои? массы. Взбивайте аккуратно, даже скорее просто хорошо перемешайте.',2);
INSERT INTO recipeStageEntity VALUES(21300,5568,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210123612-2-_a7a5953.jpg','Обязательно снимите смесь со стенок, чтобы не образовывалось комочков. Добавьте сливки. Перемешайте на 2-и? скорости до однороднои? массы.',3);
INSERT INTO recipeStageEntity VALUES(21304,5568,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210123612-3-_a7a5957.jpg','Вылейте в форму и запекайте час при 160 С.',4);
INSERT INTO recipeStageEntity VALUES(21308,5568,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210123612-4-_a7a6101_1.jpg','Когда запеканка запечется, положите в центр ложку варенои? сгущенки и посыпьте ее? сверху крупнои? солью.',5);
INSERT INTO recipeStageEntity VALUES(21312,5572,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210125041-0-_a7a5754.jpg','Молоко любой жирности и любого сорта перелейте в ковш или кастрюлю, поместите на плиту и прогрейте на умеренном нагреве примерно до 60-70 градусов. Оно не должно быть кипящим, иначе при его заливке в инжирном пюре пропадут все витамины. Выключите нагрев.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21316,5572,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210125041-1-_a7a5751.jpg','Промойте инжир в воде, срежьте хвостики с ягод. Разрежьте их на несколько частей и выложите в миску или пиалу, помните вилкой, превращая в пюре.&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(21320,5572,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210125042-2-_a7a5757.jpg','<div>Влейте теплое молоко, тщательно все перемешайте и оставьте на 3-5 минут, пока напиток не остынет до 35 градусов.&nbsp;</div>Перелейте молоко с инжиром в стаканы или чашки, добавьте по вкусу сахар или цветочный мед, если потребуется. Заранее напиток не готовьте, его нужно выпить в течение 10 минут с момента приготовления, иначе он может расслоиться.',3);
INSERT INTO recipeStageEntity VALUES(21324,5576,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210131950-0-testo.jpg','<div>Смешайте муку, рисовую муку, соль, разрыхлитель и сахар</div><div>Добавьте кокосовую стружку и масло.</div><div>Добавьте кокосовое молоко.</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(21328,5576,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210131950-1-blinchiki.jpg','Хорошо перемешайте, чтобы не было комочков. Выпекайте блины с двух сторон по 30 секунд, смазывая маслом горячую сковороду.',2);
INSERT INTO recipeStageEntity VALUES(21332,5576,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210131951-2-krem_chiz.jpg','Для начинки перемешайте крем чиз, кокосовое молоко и пудру.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(21336,5576,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210131951-3-nachinka.jpg','Нарежьте киви и клубнику.',4);
INSERT INTO recipeStageEntity VALUES(21340,5576,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210131951-4-svernut.jpg','Смешайте крем чиз с фруктами. Выложите смесь с одного края блина и сверните в рулет.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(21344,5576,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210131951-5-roll.jpg','Разрежьте получившийся рулет на несколько частей, чтобы получились порционные роллы.&nbsp;',6);
INSERT INTO recipeStageEntity VALUES(21348,5580,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210135016-0-shokolad.jpg','Смешайте льняное семя с водой.&nbsp;<div><div>Разогрейте духовку до 176 градусов и выстелите большой противень бумагой для выпечки.</div><div>Поместите все влажные ингредиенты в большую миску для смешивания. Затем, используя ручной электрический миксер, измельчите ингредиенты до получения однородной массы.</div></div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(21352,5580,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210135017-1-muka.jpg','Добавьте в смесь разрыхлитель и хорошо перемешайте. Повторите этот процесс для каждого из сухих ингредиентов (за исключением шоколада и зефира).&nbsp;',2);
INSERT INTO recipeStageEntity VALUES(21356,5580,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210135017-2-testo.jpg','<div>Тесто для печенья должно быть влажным, но не очень липким.</div><div>Грубо нарежьте шоколад и добавьте в миску. Перемешайте тесто, чтобы смешать шоколад.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21360,5580,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210135017-3-shar.jpg','Ложкой зачерпните тесто для печенья. Затем, используя руки, скатайте тесто в шар, а затем разровняйте примерно до толщины 1 см. Повторяйте этот процесс до тех пор, пока у вас не будет 16 дисков.',4);
INSERT INTO recipeStageEntity VALUES(21364,5580,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210135017-4-pechene.jpg','<div>Поместите 8 дисков теста для печенья на противень. Затем поместите маршмеллоу в середину каждого диска.</div><div>Поместите оставшиеся 8 дисков сверху. Затем один за другим прижмите друг к другу диски теста, пока зефир полностью не закроется.</div><div>Перед выпечкой в духовке убедитесь, что печенье хорошо разложено на подносе – оно будет расширяться в процессе приготовления</div><div>Выпекать в течение 12 минут.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(21368,5580,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210135017-5-final.jpg','Дайте печенью остыть на противне в течение 5 минут, а затем перенесите его на охлаждающую стойку еще на 10 минут.',6);
INSERT INTO recipeStageEntity VALUES(21372,5584,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210155415-0-piure.jpg','Для соуса малину и часть клубники (50 г) смешайте в блендере с сахаром и соком лайма и взбейте до состояния пюре. Протрите через сито, чтобы удалить косточки, и охладите.',1);
INSERT INTO recipeStageEntity VALUES(21376,5584,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210155415-1-klubnika_s_dynei.jpg','Для салата дыню, клубнику (100 г), персик (или нектарины) нарежьте мелкими кусочками и смешайте с ежевикой и черной смородиной, стараясь не повредить ягоды. Отправьте в холодильник на 1-1,5 часа.',2);
INSERT INTO recipeStageEntity VALUES(21380,5584,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210155416-2-final.jpg','Охлажденный салат выложите в две креманки, залейте ягодным соусом и шампанским. Украсьте листьями мяты.',3);
INSERT INTO recipeStageEntity VALUES(21384,5588,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210160622-0-greipfurt.jpg','Апельсин очистите от кожуры и белых пленок при помощи ножа. Затем вырежьте дольки, избегая пленок. Из оставшейся части с пленками выжмите сок в миску.',1);
INSERT INTO recipeStageEntity VALUES(21388,5588,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210160622-1-sous.jpg','<div>К апельсиновому соку добавьте&nbsp; рисовый уксус, сахар, соль. Взбейте венчиком, постепенно вливая оливковое масло.</div>',2);
INSERT INTO recipeStageEntity VALUES(21392,5588,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210160622-2-inzhir.jpg','Инжир нарежьте на крупные дольки.',3);
INSERT INTO recipeStageEntity VALUES(21396,5588,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210160622-3-avokado.jpg','Авокадо избавьте от кожуры и косточки и нарежьте полукольцами, сыр порежьте на крупные кусочки.',4);
INSERT INTO recipeStageEntity VALUES(21400,5588,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210160622-4-final.jpg','Рукколу смешайте с половиной заправки. Выложите на тарелку рукколу, аккуратно уложите дольки апельсина, инжира и кусочки сыра, полейте оставшимся соусом, украсьте жареными миндалем.',5);
INSERT INTO recipeStageEntity VALUES(21404,5592,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210162253-0-_a7a7154.jpg','<div>Тесто можно замесить двумя способами.</div><div>Замесите при помощи миксера: в чашу миксера насыпьте муку, добавьте воду, закваску, прессованные дрожжи и соль. Замешайте тесто в течение 5 минут на низкой скорости, а затем еще в течение 10 мине на высокой скорости. В конце замешивания добавьте грецкие орехи и перемешайте тесто на медленной скорости.</div><div><br></div><div>Замешайте вручную: насыпать муку горкой на рабочую поверхность или в большую миску и сделать углубление по центру. Влейте в это углубление половину указанного количества воды, добавьте закваску, раскрошите прессованные дрожжи и добавьте соль. Перемешайте, влейте оставшуюся воду. Вымешивайте тесто до тех пор, пока мука не впитает в себя всю жидкость, а само тесто не станет гладким и эластичным. В конце вымешивания добавьте грецкие орехи.</div><div><br></div><div>Сформируйте из теста шар и накройте влажным полотенцем. Оставьте его для подъема на 45 минут. Затем сложите пополам и снова оставьте для подъема на 45 минут. По истечении этого времени тесто должно увеличиться в объеме.</div>',1);
INSERT INTO recipeStageEntity VALUES(21408,5592,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210162253-1-_a7a7179.jpg','Присыпьте мукой рабочую поверхность стола. Разделите тесто на четыре равные части (каждая весом около 280 грамм). Сверните каждую рулетом, слегка вытягивая в длину, накройте полотенцем и оставьте отдыхать на 15 минут.',2);
INSERT INTO recipeStageEntity VALUES(21412,5592,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210162253-2-_a7a7176.jpg','Разрежьте горгонзолу на небольшие кусочки. Возьмите одну часть теста, слегка расплющьте ее ладонью. Сверху выложите четверть горгонзолы. Загните треть теста поверх основной массы и придавите кончиками пальцев. Переверните лепешку на 180 градусов и сложите тесто пополам в длину, плотно запечатайте края, прижимая их друг к другу основанием ладони. Раскатайте тесто руками, пока длина заготовки не достигнет 20 см. Точно так же сформируйте остальные части теста.',3);
INSERT INTO recipeStageEntity VALUES(21416,5592,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201210162253-3-final.jpg','<div>Поместите тестовые заготовки швом вниз на противень, застеленный бумагой для выпечки. Накройте заготовки влажным полотенцем и оставьте для расстойки на 1 час.</div><div>Разогрейте духовку до 250 градусов. На поверхности изделии? сделайте по одной продольной насечке и слегка увлажните их при помощи пульверизатора или кулинарной кисти.</div><div>Посадите заготовки в духовку и выпекайте в течение 18 минут. Вынув готовый хлеб из духовки, остудите его на решетке.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21420,5596,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211121556-0-bulon.jpg','Приготовьте бульон. В кипящую воду закиньте говяжьи кости и доведите до кипения на сильном огне.',1);
INSERT INTO recipeStageEntity VALUES(21424,5596,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211121558-1-luk_v_bulone.jpg','Первый бульон слейте сразу после закипания, далее поменяйте воду и варите на тихом огне 2-3 часа (или дольше). Не забывайте снимать пену. В бульон следует добавить гвоздику, семена кориандра, корицу, бадьян, репчатый лук (1-2 шт.) и имбирь (2 см).',2);
INSERT INTO recipeStageEntity VALUES(21428,5596,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211121559-2-luk.jpg','Чтобы бульон получился красивого цвета, лук и имбирь нужно обуглить на открытом огне или на сухой сковороде. Здесь все просто — почистите их, разрежьте луковицы пополам и держите, пока не станут коричневыми, после чего забросьте в бульон.&nbsp;<div>В процессе варки не забудьте добавить соль и сахар по вкусу.&nbsp;</div><div>Бульон должен получиться насыщенным, сладковато-соленым, чтобы он мог передать свой вкус и аромат рисовой лапше. Он должен быть темным, но прозрачным, иметь выраженный вкус мяса и аромат специй.</div>',3);
INSERT INTO recipeStageEntity VALUES(21432,5596,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211121559-3-sup.jpg','В самом конце, когда бульон уже будет полностью готов, добавьте в него зеленый лук — нам нужна только белая часть. Проварите буквально полминутки, чтобы луковички не переварились, а остались хрустящими, выньте их из бульона и отложите в сторону. Вытащите говядину, кости выбросьте, а мякоть нарежьте на кусочки, чтобы было удобно брать палочками. Бульон желательно процедить.',4);
INSERT INTO recipeStageEntity VALUES(21436,5596,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211121559-4-karpachcho.jpg','Далее возьмите карпаччо и добавьте немного соли и тертого имбиря, оставьте на время, пока будут готовиться прочие ингредиенты.&nbsp; Понадобится 6-8 штук на порцию.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(21440,5596,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211121559-5-ovoshi.jpg','Репчатый лук (1 шт.) почистите и нарежьте тоненькими кольцами (если горчит, можно на время залить холодной водой в пиале), острый перец нарежьте тонко, а стебли зеленого лука и петрушку (а еще лучше — кинзу) порубите ножом.',6);
INSERT INTO recipeStageEntity VALUES(21444,5596,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211121559-6-lapsha.jpg','Отварите рисовую лапшу, следуя инструкции на упаковке. Нам нужна лапша средней проварки. Вымочите ее в холодной воде, затем опустите в кипящую воду и варите до средней степени готовности. Промойте в проточной воде, чтобы лапша не склеивалась и была упругой.',7);
INSERT INTO recipeStageEntity VALUES(21448,5596,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211121559-7-final.jpg','Соберите суп Фо Бо.&nbsp;<div>В пиалу выложите лапшу горкой, добавьте кусочки вареной говядины, несколько кусочков карпаччо, пару колечек репчатого лука и мелко порезанную зелень.<div>Далее залейте суп горячим бульоном — старайтесь лить на ломтики сырого мяса, его цвет должен поменяться.</div><div>Подавайте вьетнамский суп сразу же, пока горячий, дополнив перцем чили и лаймом, нарезанным крупными дольками.&nbsp;</div></div>',8);
INSERT INTO recipeStageEntity VALUES(21452,5600,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211124531-0-kurica.jpg','<div>Приправьте курицу 1 столовой ложкой оливкового масла, хорошей щепоткой соли и перца, чесночным порошком, луковым порошком и кайенским перцем.&nbsp;</div><div>Разогрейте сковороду для гриля. Как только гриль нагреется, положите курицу на решетку и жарьте около 6 минут с каждой стороны или до тех пор, пока внутренняя температура не достигнет 75 градусов.&nbsp;</div>',1);
INSERT INTO recipeStageEntity VALUES(21456,5600,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211124531-1-narezat.jpg','Переложите на разделочную доску и дайте остыть около 10 минут, потом нарежьте тонкими ломтиками и отложите в сторону.',2);
INSERT INTO recipeStageEntity VALUES(21460,5600,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211124531-2-hleb.jpg','Разогрейте духовку до высокой температуры. Положите ломтики хлеба на противень, в один слой, и сбрызните оставшимся оливковым маслом. Жарьте пока хлеб не станет золотисто-коричневым и хрустящим с обеих сторон, примерно от 30 секунд до минуты. Выньте из духовки и, как только остынет, натрите ломтики тоста целым чесноком и приправьте щепоткой соли и перца. Отложите в сторону.',3);
INSERT INTO recipeStageEntity VALUES(21464,5600,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211124531-3-sous.jpg','Чтобы приготовить заправку, смешайте яичный желток, чеснок, лимонный сок, 1 или 2 филе анчоусов (в зависимости от того, как вы их любите), щепотку соли и перца, пармезан и вустерский соус в небольшом кухонном комбайне. Смешайте все вместе.&nbsp;Добавьте столько масла, сколько необходимо, чтобы создать гладкую загустевшую массу. Переложите в миску и перемешайте с майонезом. При необходимости добавьте больше соли или перца.',4);
INSERT INTO recipeStageEntity VALUES(21468,5600,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211124531-4-salat.jpg','В большой салатнице смешайте листья салата, несколько столовых ложек заправки (в зависимости от того, как вы любите заправлять салат) и сыр Пармезан до однородного состояния.',5);
INSERT INTO recipeStageEntity VALUES(21472,5600,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211124532-5-na_hlebe.jpg','<div>Сверху на ломтики хлеба положите нарезанную курицу, заправленную салатом. Посыпьте пармезаном и небольшим количеством черного перца.</div><div><br></div>',6);
INSERT INTO recipeStageEntity VALUES(21476,5604,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211141242-0-ryba_v_panirovke.jpg','<div>Рыбу нарежьте кусками одинакового размера.</div><div>Муку смешайте с солью, обваляйте куски рыбы.</div>',1);
INSERT INTO recipeStageEntity VALUES(21480,5604,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211141242-1-ryba.jpg','Обжарьте кусочки на растительном масле с обеих сторон. Выложите рыбу на салфетку, чтобы избавиться от лишнего масла.',2);
INSERT INTO recipeStageEntity VALUES(21484,5604,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211141242-2-luk.jpg','<div>Овощи почистите. Лук нарежьте полукольцами, морковь и петрушку натрите на крупной терке, чеснок порубите.</div><div>На растительном масле обжарьте лук, чтобы он стал прозрачным.&nbsp;</div>',3);
INSERT INTO recipeStageEntity VALUES(21488,5604,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211141242-3-luk_s_tomatami.jpg','Добавьте морковь и петрушку. Жарьте на среднем огне, помешивая, около 10 минут. Добавьте чеснок, пряности, томатную пасту. Готовьте еще пару минут, затем накройте сковороду крышкой и тушите периодически, помешивая, 10-15 минут.',4);
INSERT INTO recipeStageEntity VALUES(21492,5604,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211141242-4-sous.jpg','<div>Добавьте воду, уксус, сахар и соль, перемешайте, тушите 15 мин и доведите до вкуса.</div><div>Добавьте маринад к рыбе.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(21496,5608,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211143129-0-_a7a7510.jpg','Рис тщательно промойте до чистой воды. Затем отварите рис в большом количестве подсоленной воды до готовности. Рис откиньте на сито и дайте стечь воде. Охладите.',1);
INSERT INTO recipeStageEntity VALUES(21500,5608,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211143129-1-_a7a7504.jpg','<div>Консервированную горбушу достаньте из банки и отряхните от жидкости. Из рыбы удалите кости. Разомните рыбу вилкой на мелкие кусочки.</div><div>Огурцы вымойте, порежьте мелкими кубиками.</div>',2);
INSERT INTO recipeStageEntity VALUES(21504,5608,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211143129-2-_a7a7507.jpg','Зелень также промойте и мелко порубите.',3);
INSERT INTO recipeStageEntity VALUES(21508,5608,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211143129-3-_a7a7513.jpg','Яйца сварите вкрутую, охладите и очистите. Яйца порубите мелкими кубиками.',4);
INSERT INTO recipeStageEntity VALUES(21512,5608,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201211143129-4-_a7a7517.jpg','<div>В салатнике соедините рис, яйца, огурец, горбушу и зелень. Заправьте салат майонезом, поперчите и перемешайте.</div>',5);
INSERT INTO recipeStageEntity VALUES(21516,5612,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214130611-0-kurica-v-souse.jpg','<div>Смешайте в миске курицу, порошок чили, перец, корицу и щепотку соли и перца.</div><div><br></div><div>Нагрейте 1 столовую ложку оливкового масла в большой кастрюле с толстым дном на среднем или сильном огне. Когда масло начнет мерцать, добавьте курицу и обжарьте с обеих сторон примерно 3-4 минуты. Уменьшите огонь до минимума и добавьте 1,5 стакана соуса барбекю и пиво. Накройте и тушите курицу 1-2 часа. Если соус становится слишком густым, добавьте еще пива, чтобы соус стал более жидким. Когда курица будет готова, измельчите ее двумя вилками и добавьте кинзу и оставшийся соус барбекю, если хотите.&nbsp;</div>',1);
INSERT INTO recipeStageEntity VALUES(21520,5612,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214130611-1-salat.jpg','<div>Сделайте соус ранч. Смешайте все ингредиенты (авокадо, пахту, 16 г свежей кинзы, халапеньо, сок лайма, 1/2 ч. л. чесночного порошка, 1/2 ч. л. лукового порошка и щепотку кошерной соли)&nbsp; в блендере и взбивайте до получения однородной кремообразной массы, добавляя пахту, по 1 столовой ложке за раз, чтобы соус стал более жидким.</div><div><br></div><div><div>Чтобы приготовить салат, добавьте капусту в миску с несколькими ложками ранча и перемешайте. Добавьте кукурузу, оставшуюся кинзу и чеснок и приправьте солью.</div></div>',2);
INSERT INTO recipeStageEntity VALUES(21524,5612,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214130611-2-s-sousom.jpg','<div>Положите сыр и курицу в подогретые тортильи. Сверху добавьте капусту и ранч.&nbsp;</div>',3);
INSERT INTO recipeStageEntity VALUES(21528,5616,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214134803-0-zheltki.jpg','Отделите желтки от белков. Белки вылейте в миску, а желтки в другую.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21532,5616,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214134803-1-soteinik.jpg','<div>Просейте муку через сито.&nbsp;</div><div>Масло положите в кастрюлю, добавьте молоко. Поставьте на огонь и дождитесь пока масло растает.</div><div>Добавьте масло и молоко к муке. Перемешайте венчиком. Добавьте желтки, ванильный экстракт и хорошо все перемешайте снова.</div>',2);
INSERT INTO recipeStageEntity VALUES(21536,5616,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214134803-2-belki_belye.jpg','Белки смешайте с сахаром и взбивайте до густой пены.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(21540,5616,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214134803-3-testo.jpg','Добавьте полученную массу из желтков со взбитыми белками и перемешайте венчиком.',4);
INSERT INTO recipeStageEntity VALUES(21544,5616,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214134803-4-forma.jpg','В форму для выпечки положите бумагу для запекания. Вылейте массу. Поставьте форму в поддон и налейте горячую воду, доведенную до 80 градусов (сделайте горячую баню).',5);
INSERT INTO recipeStageEntity VALUES(21548,5616,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214134803-5-final.jpg','<div>Поставьте все в духовку на 60 мин и выпекайте при температуре 150 градусов.</div><div>Вытащите бисквит, уберите бумагу и порежьте на кусочки.</div><div><br></div>',6);
INSERT INTO recipeStageEntity VALUES(21552,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-0-farsh.jpg','Добавьте курицу, натертый лук-шалот, 1 зубчик чеснока, орегано, паприку и щепотку красного перца, соль и перец в миску. Перемешайте до полного смешивания.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21556,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-1-shariki.jpg','Намажьте руки небольшим количеством масла, скатайте мясо в шарики размером со столовую ложку (получится 14-15 фрикаделек).',2);
INSERT INTO recipeStageEntity VALUES(21560,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-2-mitboly.jpg','Нагрейте большую сковороду или голландскую духовку на среднем огне и добавьте 2 столовые ложки масла. Когда масло начнет мерцать, добавьте фрикадельки и обжарьте до хрустящей корочки, примерно 8-10 минут, переворачивая их 2-3 раза. Переложить на тарелку.',3);
INSERT INTO recipeStageEntity VALUES(21564,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-3-limon.jpg','В ту же сковороду добавить сливочное масло и ломтики лимона. Обжарьте лимон до золотистого цвета с каждой стороны, около 1 минуты. Выньте лимон из кастрюли и добавьте в тарелку с фрикадельками.',4);
INSERT INTO recipeStageEntity VALUES(21568,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-4-pasta.jpg','В ту же сковороду добавьте 1 зубчик чеснока и орзо. Готовьте, пока чеснок не станет ароматным и орзо не поджарится, около 2-3 минут. Добавьте 2,5 стакана воды и 1-2 столовые ложки сока из 1 лимона. Доведите до кипения на сильном огне и варите, часто помешивая, до аль денте, около 7-8 минут.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(21572,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-5-olivki.jpg','Добавьте оливки и укроп. Положите фрикадельки обратно в сковороду и готовьте, пока они не прогреются.',6);
INSERT INTO recipeStageEntity VALUES(21576,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-6-sous.jpg','Тем временем сделайте вяленый томатный винегрет. В миске взбейте вместе 1/4 стакана вяленого томатного масла, 2 столовые ложки лимонного сока и бальзамический уксус до полного смешивания. Добавьте вяленые помидоры, 1 столовую ложку укропа и приправьте солью, перцем и хлопьями красного перца.',7);
INSERT INTO recipeStageEntity VALUES(21580,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-7-sous_belyi.jpg','Сделать фету. Смешайте все ингредиенты (фета, йогурт и оливковое масло) в кухонном комбайне и взбейте до однородной консистенции. Фету можно хранить в холодильнике до 3 дней.',8);
INSERT INTO recipeStageEntity VALUES(21584,5620,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214154740-8-final.jpg','<div>Чтобы подать на стол, разложите фету на тарелки и полейте винегретом. Добавьте орзо, обжаренные лимоны и фрикадельки. Посыпать зеленью и зелеными травами.&nbsp;</div><div><br></div>',9);
INSERT INTO recipeStageEntity VALUES(21588,5624,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214162753-0-iogurt.jpg','В форме для выпечки смешайте йогурт, оливковое масло, лимонный сок, чеснок, паприку, орегано и по щепотке соли и черного перца. Добавьте курицу и перемешайте. Накройте крышкой и маринуйте в холодильнике не менее 1 часа.<div><div>Разогрейте духовку до 250 градусов. Возьмите курицу из холодильника и дайте ей постоять при комнатной температуре, пока духовка разогревается.</div><div>Переложите курицу в духовку и жарьте в течение 20-25 минут или до тех пор, пока курица не будет готова. Вытащите курицу из духовки и дайте ей остыть в течение 5 минут. Измельчите курицу двумя вилками или тонко нарежьте ее.</div></div>',1);
INSERT INTO recipeStageEntity VALUES(21592,5624,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214162753-1-specii.jpg','Приготовьте картофель фри. В небольшой миске смешайте оливковое масло, чеснок, орегано, кайенский перец, соль и черный перец по вкусу.',2);
INSERT INTO recipeStageEntity VALUES(21596,5624,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214162753-2-kartofel.jpg','Положите картофельные дольки в большую миску и залейте половиной чесночной смеси.',3);
INSERT INTO recipeStageEntity VALUES(21600,5624,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214162754-3-fri.jpg','Аккуратно перемешайте. Разделите картофель фри между двумя противнями, расположив их в один слой. Выпекайте в течение 15-20 минут, затем переверните, уменьшите температуру духовки до 200 градусов и выпекайте еще 15-20 минут. Выньте из духовки и перемешайте с оставшейся чесночной смесью и фетой.',4);
INSERT INTO recipeStageEntity VALUES(21604,5624,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214162754-4-final.jpg','Нафаршировать лепешки наан или питы курицей и подавать с картофелем фри, соусом цацики, маринованным красным луком, салатом, и редиской, при желании.',5);
INSERT INTO recipeStageEntity VALUES(21608,5628,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214164058-0-kefir.jpg','Кефир перелейте в герметичный пакет и заморозьте.',1);
INSERT INTO recipeStageEntity VALUES(21612,5628,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214164058-1-zamorozit.jpg','Кирпичик с замороженным кефиром подвесьте в марле над кастрюлей или миской.',2);
INSERT INTO recipeStageEntity VALUES(21616,5628,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201214164058-2-marlia.jpg','Из размороженного кефира получается творог.',3);
INSERT INTO recipeStageEntity VALUES(21620,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-0-kartoshka.jpg','Картошку хорошо промойте и отварите в мундирах до готовности. Главное, чтобы клубни не лопнули. Горячий картофель очистите от кожуры вилкой или ножом. Чем горячее будут клубни, тем лучше.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21624,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-1-piure.jpg','Очищенную картошку измельчите в пюре толкучкой или перетрите через сито. Пюре должно получиться гладким, однородным, без комков. Добавьте в картофель соль по вкусу и яичный желток, твердый мелко натёртый сыр или рубленую зелень. Перемешайте.',2);
INSERT INTO recipeStageEntity VALUES(21628,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-2-testo.jpg','Выложите смесь на посыпанную мукой ровную поверхность. Замесите тесто, постепенно добавляя муку. Готовое тесто должно слегка липнуть.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(21632,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-3-kolbaska.jpg','Тесто раскатайте в колбаску диаметром около 2 см. Колбаску разрежьте на кусочки длиной 2,5 см.',4);
INSERT INTO recipeStageEntity VALUES(21636,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-4-nokki.jpg','Каждый кусочек слегка придавите вилкой, чтобы появились характерные бороздки, которые будут лучше впитывать соус. Сварите картофельные ньокки в кипящей подсоленной воде. Как только заготовки всплыли, они готовы . Готовятся примерно 3-4 минуты.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(21640,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-5-luk.jpg','<div>Приступайте к приготовлению соуса Маринара.&nbsp;</div><div>Морковь натрите на мелкой терке, чеснок и лук очень мелко нарежьте.</div><div>На оливковом масле обжарьте лук до прозрачности.</div>',6);
INSERT INTO recipeStageEntity VALUES(21644,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-6-morkov.jpg','Добавьте чеснок и морковь, пассируйте овощи 3-5 минут.',7);
INSERT INTO recipeStageEntity VALUES(21648,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-7-tomaty.jpg','Томаты в собственном соку мелко нарежьте, петрушку нашинкуйте.',8);
INSERT INTO recipeStageEntity VALUES(21652,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-8-ovoshi.jpg','Добавьте в сковороду к овощам нарезанные помидоры, сок из них и петрушку. Приправьте соус солью и сушеным базиликом.',9);
INSERT INTO recipeStageEntity VALUES(21656,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-9-sous.jpg','Тушите соус на небольшом огне в течении 30 минут. При необходимости добавьте немного воды или увеличьте время приготовления для получения нужной консистенции.&nbsp;',10);
INSERT INTO recipeStageEntity VALUES(21660,5632,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215120748-10-final.jpg','Выложите готовые ньокки на тарелку и полейте соусом Маринара.&nbsp;',11);
INSERT INTO recipeStageEntity VALUES(21664,5636,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215123502-0-hlebopechka.jpg','Добавьте все ингредиенты в чашу и приготовьте в соответствии с инструкцией хлебопечки.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21668,5640,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215124821-0-testo.jpg','Добавьте все ингредиенты в чашу хлебопечки и замешайте в соответствии с инструкцией.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21672,5640,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215124821-1-final-testo.jpg','Тесто достаньте из печки и оставьте на 5 мин под полотенцем.',2);
INSERT INTO recipeStageEntity VALUES(21676,5640,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215124821-2-svernuli.jpg','Сформируйте булочки и выложите на пергамент.',3);
INSERT INTO recipeStageEntity VALUES(21680,5640,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215124822-3-final.jpg','<div>Смажьте яичком и выпекайте при 180 градусах до готовности.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21684,5644,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215151838-0-sous.jpg','<div>Разогрейте духовку до 230 градусов по Цельсию. Смажьте большой противень оливковым маслом.</div><div>В небольшой миске смешайте сливочное масло с чесноком, оливковым маслом, щепоткой морской соли, орегано, 1 столовой ложкой тимьяна, базиликом, хлопьями красного перца и щепоткой перца.</div>',1);
INSERT INTO recipeStageEntity VALUES(21688,5644,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215151838-1-testo.jpg','<div>Приготовьте тесто по рецепту на <a href="http://www.utkonos.ru/recipe/3993">сайте</a>.</div><div>На слегка посыпанной мукой поверхности раскатайте тесто, пока оно не станет довольно тонким (круг размером 25-30 сантиметров). Переложите тесто на подготовленный противень.</div>',2);
INSERT INTO recipeStageEntity VALUES(21692,5644,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215151838-2-smazali.jpg','Намажьте тесто сливочно-масляным соусом с травами.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(21696,5644,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215151838-3-shpinat.jpg','Добавьте шпинат и козий сыр.',4);
INSERT INTO recipeStageEntity VALUES(21700,5644,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215151838-4-tomaty.jpg','Затем выложите сверху ломтики помидоров. Приправьте солью и перцем.&nbsp; Сверху посыпьте моцареллой.<div>Переложите в духовку и запекайте 10-15 минут, пока корочка не станет золотистой, а сыр не расплавится.</div>',5);
INSERT INTO recipeStageEntity VALUES(21704,5644,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215151838-5-ovoshi.jpg','<div>Перемешайте свежие помидоры черри с оставшимся тимьяном, базиликом, щепоткой морской соли и бальзамическим уксусом и залейте смесью пиццу.</div>',6);
INSERT INTO recipeStageEntity VALUES(21708,5648,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215155459-0-muka.jpg','<div>Разогрейте духовку до 175 градусов Цельсия. Смажьте и выстелите форму для хлеба размером 25х10 см пергаментной бумагой.</div><div>Вымойте и обсушите малину.</div><div>Смешайте все влажные ингредиенты в большой миске и перемешивайте, пока сахар не растворится.</div><div>В другой миске взбейте все сухие ингредиенты, кроме малины.&nbsp;</div>',1);
INSERT INTO recipeStageEntity VALUES(21712,5648,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215155500-1-testo.jpg','<div>Вылейте влажную смесь поверх мучной смеси и взбивайте, пока она не смешается.</div>',2);
INSERT INTO recipeStageEntity VALUES(21716,5648,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215155501-2-malina.jpg','Добавьте кукурузный крахмал и малину в миску среднего размера и перемешайте, чтобы малина покрылась слоем крахмала. Аккуратно добавьте малину в тесто для торта, пока смесь не смешается.',3);
INSERT INTO recipeStageEntity VALUES(21720,5648,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215155501-3-v_forme.jpg','<div>Перелейте тесто в подготовленную форму.</div><div>Выпекайте 45-50 минут или пока зубочистка, вставленная в центр, не выйдет чистой.</div><div>Дайте пирогу остыть в форме 10 минут, прежде чем переложить на решетку. Дайте ему полностью остыть перед нарезкой / глазированием.</div>',4);
INSERT INTO recipeStageEntity VALUES(21724,5648,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215155501-4-krahmal.jpg','Глазурь: смешайте сахарную пудру с растительным молоком и лимонным соком. Вылейте глазурь поверх остывшего торта.',5);
INSERT INTO recipeStageEntity VALUES(21728,5652,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215162543-0-tykva.jpg','Тыкву натрите на мелкой тёрке.',1);
INSERT INTO recipeStageEntity VALUES(21732,5652,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215162543-1-testo.jpg','Соедините с творогом, яйцом, солью и сахаром. Добавьте муку с разрыхлителем и перемешайте.',2);
INSERT INTO recipeStageEntity VALUES(21736,5652,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215162543-2-syrniki.jpg','В сковороде разогрейте масло на среднем огне. Сформируйте сырники.',3);
INSERT INTO recipeStageEntity VALUES(21740,5652,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201215162543-3-skovoroda.jpg','Обжаривайте сырники по 3–5 минут с каждой стороны.',4);
INSERT INTO recipeStageEntity VALUES(21744,5656,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216133106-0-ovoshi.jpg','Сварите насыщенный куриный бульон. Для этого варите одну целую курицу с одной целой луковицей примерно 1,5 часа.&nbsp;<div>Галангал или имбирь порежьте на толстые пластины и добавьте в бульон.&nbsp;</div><div>Нарежьте лемонграсс на крупные куски (он нужен для вкуса бульон, есть его не надо). Отправьте его к имбирю вариться.&nbsp;</div><div>Помните листья кафир-лайма в руке, пока не почувствуете запах, и добавьте в суп.</div><div>Красный перец порежьте вдоль и удалите все косточки. Добавьте в суп. Добавьте соус&nbsp;Шрирача</div>',1);
INSERT INTO recipeStageEntity VALUES(21748,5656,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216133106-1-griby.jpg','Нарежьте шампиньоны на средние кусочки. Добавьте их в суп. Он должен вариться, но не кипеть.<div>Добавьте соевый соус, рыбный соус и пасту Том-Ям. Хорошо перемешайте.</div>',2);
INSERT INTO recipeStageEntity VALUES(21752,5656,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216133106-2-sup.jpg','Возьмите лук порей. Уберите верхний слой и отрежьте корень. Нарежьте его под углом на небольшие кружочки. Вам понадобится 4-5 кусочков, которые надо добавить в суп.&nbsp;<div>Выжмите&nbsp;кафир-лайм в кастрюлю.&nbsp;</div><div>Подогрейте сливки в микроволновке, чтобы они стали теплыми, и добавьте в суп. Туда же добавьте кокосовое молоко.&nbsp;</div>',3);
INSERT INTO recipeStageEntity VALUES(21756,5656,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216133106-3-krevetki.jpg','Нарежьте кальмар на кольца и добавьте в суп (4-5 шт.).&nbsp;<div>Мидии отделите от ножек и проверьте на наличие водорослей. Также добавьте в суп.&nbsp;</div><div>Очистите креветки от панциря и удалите хвостик. Удалите кишечный тракт с помощью ножа. Отправьте креветок в суп и дайте повариться 2-3 минуты.&nbsp;</div><div>Выложите суп на тарелки. Добавьте в каждую порцию по две половинки помидора черри. Выложите сверху кинзу для украшения.</div>',4);
INSERT INTO recipeStageEntity VALUES(21760,5656,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216133106-4-ris.jpg','Подавайте том-ям с предварительно отваренным рисом.',5);
INSERT INTO recipeStageEntity VALUES(21764,5660,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216142542-0-kurica.jpg','Разогрейте духовку до 200 градусов. Смажьте безопасную для духовки сковороду оливковым маслом.<div>Разрежьте грудку пополам по горизонтали.&nbsp;</div><div>Приправьте курицу солью и перцем.&nbsp;</div>',1);
INSERT INTO recipeStageEntity VALUES(21768,5660,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216142542-1-persik.jpg','Намажьте каждую грудку 1 ч. л. вареньем из инжира, затем оберните каждую грудку двумя кусочками ветчины. Переложите курицу в подготовленную сковороду. Уложите вокруг курицы лук-шалот, персики и тимьян. Полейте персики оливковым маслом, приправьте солью и перцем. Переложите в духовку и запекайте 15-20 минут, пока курица не будет готова.<div>Переключите духовку на жаркое. Переложите персики со сковороды на тарелку. Верните курицу в духовку и жарьте 1-2 минуты, пока прошутто не станет хрустящим. Выньте из духовки, выложите курицу на тарелку с персиками.</div>',2);
INSERT INTO recipeStageEntity VALUES(21772,5660,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216142543-2-chesnok.jpg','Поставьте сковороду с луком-шалотом и чесноком на средний огонь. Добавьте масло и щепотку сухого красного перца. Добавьте бальзамический уксус и вино. Доведите до кипения на сильном огне.&nbsp;',3);
INSERT INTO recipeStageEntity VALUES(21776,5660,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216142543-3-obzharit.jpg','Варите 3-5 минут, пока не уменьшится вдвое. Переложите курицу и персики обратно в сковороду и тушите 1 минуту. Снимите с огня.<div>Подавайте курицу с персиками, бурратой и свежим базиликом. Полейте оставшимся соусом. Подавайте вместе с орзо и хрустящим хлебом, чтобы он впитал соус.&nbsp;</div>',4);
INSERT INTO recipeStageEntity VALUES(21780,5664,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216151417-0-_a7a6015.jpg','В небольшой миске смешайте коричневый сахар, соль, черный перец, порошок чили, орегано, чесночный порошок, горчицу, кайенский перец, тмин и красный перец.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21784,5664,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216151417-1-_a7a6020.jpg','Приправьте свинину смесью коричневого сахара.<div>Нагрейте в высокой сковороде масло канолы; добавьте свинину и жарьте до однородного румяного цвета, примерно 3-4 минуты.</div>',2);
INSERT INTO recipeStageEntity VALUES(21788,5664,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216151417-2-_a7a6023.jpg','Добавьте куриный бульон и 4 столовые ложки уксуса, соскребая со дна сковороды подрумянившиеся кусочки.<div>Готовьте под крышкой 1,5-2 часа.</div>',3);
INSERT INTO recipeStageEntity VALUES(21792,5664,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216151417-3-_a7a6041.jpg','Выньте свиную лопатку из сковороды и измельчите мясо двумя вилками.',4);
INSERT INTO recipeStageEntity VALUES(21796,5664,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216151417-4-_a7a6044.jpg','<div>Добавьте оставшиеся 2 столовые ложки уксуса и приправьте солью и перцем по вкусу, если необходимо.</div>',5);
INSERT INTO recipeStageEntity VALUES(21800,5664,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216151417-5-_a7a6027.jpg','Нарежьте капусту.',6);
INSERT INTO recipeStageEntity VALUES(21804,5664,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216151417-6-_a7a6029.jpg','Нарежьте маринованные огурцы и перец.&nbsp;',7);
INSERT INTO recipeStageEntity VALUES(21808,5664,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201216151417-7-final.jpg','Соберите сэндвич.',8);
INSERT INTO recipeStageEntity VALUES(21812,5668,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217123247-0-drozhzhi.jpg','Смешайте теплое молоко (около 30 градусов), дрожжи, добавьте пару столовых ложек сахара и муки. Оставьте на 20-30 минут.&nbsp;',1);
INSERT INTO recipeStageEntity VALUES(21816,5668,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217123247-1-tvorog.jpg','Пока дрожжи завариваются, займитесь начинкой. Для начала, чтобы начинка была без крупинок, протрите творог через сито.',2);
INSERT INTO recipeStageEntity VALUES(21820,5668,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217123247-2-tvorogo_2.jpg','<div>Добавьте к творогу 2 столовые ложки сахара, 2 яичных желтка, сметану, 2/3 столовой ложки муки, ванильный сахар и 20 грамм сливочного масла. Сливочное масло должно быть мягким.</div><div><div>Все тщательно перемешайте. Начинка готова.</div></div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21824,5668,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217123247-3-testo.jpg','В чашу миксера (насадка крюк) добавьте 3-4 небольших яйца, оставшийся сахар, соль, муку и масло. Сверху вылейте дрожжевую смесь.<div>Замешайте все ингредиенты в тесто. Накройте чашу мокрым полотенцем, и поставьте в теплое место на 2-3,5 часа или на нижний уровень в заранее разогретую до 45 С духовку и подождите пока тесто увеличится.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21828,5668,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217123248-4-bulochki.jpg','Разделите тесто на порционные шарики.&nbsp;',5);
INSERT INTO recipeStageEntity VALUES(21832,5668,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217123248-5-vatrushki.jpg','Сделайте ватрушки. На каждый шарик положите примерно 100 г творожной начинки. Оставьте ватрушки на расстойку на 15 минут.&nbsp;<div>Перед тем как отправить ватрушки в духовке, смажьте их смесью молока с желтком.&nbsp;<br><div>Выпекайте в духовке при 180 градусов до румяной корочки.</div></div>',6);
INSERT INTO recipeStageEntity VALUES(21836,5672,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217130154-0-belki.jpg','На обратной стороне листа пергаментной бумаги нарисуйте круги диаметром 3-4 см – это будет ваш трафарет для выпечки макарон.<div><div>Просейте миндальную муку и сахарную пудру. Сухую смесь перемешайте в течение 3 минут на маленькой скорости миксером или в комбайне.&nbsp;</div><div>Разогрейте духовку до 150 С, в течение 5 мин просушите смесь. Еще раз просейте.</div><div>В белки добавьте семена ванили. Взбейте белки в пушистую пену, постепенно небольшими порциями добавляйте сахар и продолжайте взбивать до устойчивых пиков. Оставьте белки на 2 минуты.</div><div><br></div></div>',1);
INSERT INTO recipeStageEntity VALUES(21840,5672,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217130154-1-testo.jpg','<div>Аккуратно соедините взбитые белки с сухой смесью и растворимым кофе, перемешайте - снизу вверх, крутя миску по часовой стрелке.</div><div>Тесто должно стать блестящим, податливым и мягкой лентой стекать с лопатки.</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(21844,5672,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217130154-2-kryshechki.jpg','<div>Сложите тесто в кулинарный мешок или пакет и отсадите его кружочками на заготовленный пергамент. Вы замесили тесто правильно, если «хвостики» на поверхности пирожных сами расходятся, образуя гладкую ровную поверхность.</div><div>Оставьте заготовки при комнатной температуре на 30-60 минут. На поверхности теста должна образоваться очень тонкая пленочка (корочка), не липнущая к пальцам. При выпекании корочка становится хрустящей, а под ней образуется «юбочка».</div><div>Выпекайте меренги 13-15 мин в разогретой духовке до 150-155С.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21848,5672,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217130154-3-s-kofe.jpg','<div>Сливки почти доведите до кипения (90-95С).</div><div>Снимите с огня, добавьте кофе и перемешайте.&nbsp;</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21852,5672,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217130154-4-shokolad.jpg','Добавьте в горячие сливки измельченный белый шоколад, подождите около 1 минуты, перемешайте до однородной массы.',5);
INSERT INTO recipeStageEntity VALUES(21856,5672,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217130154-5-smes.jpg','<div>Добавьте ванильный сироп и уберите ганаш в холодильник для застывания.</div>',6);
INSERT INTO recipeStageEntity VALUES(21860,5672,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217130154-6-final.jpg','<div>Переложите ганаш в кондитерский мешок, отсадите на половинку макарон, накройте второй половинкой и уберите пирожное в холодильник еще на 24 часа.</div><div><br></div>',7);
INSERT INTO recipeStageEntity VALUES(21864,5676,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217161009-0-mindalnaia-muka.jpg','<div>На обратной стороне листа пергаментной бумаги нарисуйте круги диаметром 3-4 см – это будет ваш трафарет для выпечки макарон.</div><div>Смешайте миндальную муку и сахарную пудру. Измельчите смесь еще раз в кофемолке или блендере. Просейте.</div><div>Разогрейте духовку до 150С, в течение 5 мин просушите смесь. Еще раз просейте.</div><div>В миску с мукой добавьте первую партию белков (55г) и отставьте емкость в сторону.</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(21868,5676,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217161009-1-mindalnaia-smes.jpg','<div>Белки (110 г) взбейте миксером в легкую пену, затем постепенно всыпайте сахар (30г).&nbsp; Продолжайте взбивать до мягких пиков.</div><div><br></div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(21872,5676,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217161009-2-vzbili-s-zelen.jpg','<div>Одновременно в сотейнике доведите до кипения (100С) воду и сахар (300г), добавьте краситель.</div><div>Продолжайте держать сотейник на огне, пока сироп не прогреется до 117С. Сразу же тоненькой струйкой вливайте его во взбивающиеся на средней скорости белки.</div><div>Продолжайте взбивать до тех пор, пока масса не остынет и не станет блестящей, густой и гладкой. Это и есть ваша меренга.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21876,5676,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217161009-3-smes.jpg','<div>Возьмите ранее отставленную в сторону миску с мукой и белками, разотрите смесь в миндальную пасту, и соедините ее с меренгой. Аккуратно перемешайте лопаткой, при этом второй рукой вращая миску по часовой стрелке.</div><div>Тесто должно стать мягким, блестящим, однородным.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21880,5676,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217161009-4-na-pergamente.jpg','<div>Сложите тесто в кулинарный мешок или пакет и отсадите его кружочками на заготовленный пергамент. Оставьте заготовки при комнатной температуре на 30-40 минут.</div><div>Выпекайте меренги 12-15 мин в разогретой духовке до 150-155С.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(21884,5676,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217161009-5-naslo.jpg','<div>Растопите белый шоколад на водяной бане или в микроволновой печи.</div><div>Мягкое сливочное масло взбейте миксером, понемногу добавляя растопленный шоколад.</div><div><br></div>',6);
INSERT INTO recipeStageEntity VALUES(21888,5676,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217161009-6-final.jpg','<div>50 г ягод черники отложите, 200 г измельчите блендером в пюре. Добавьте пюре и цельные ягоды в шоколадную массу. Перемешайте и уберите в холодильник до загустения.</div><div>Соедините попарно половинки макарон начинкой и уберите на ночь в холодильник.</div><div><br></div>',7);
INSERT INTO recipeStageEntity VALUES(21892,5680,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217164507-0-belki.jpg','На обратной стороне листа пергаментной бумаги нарисуйте круги диаметром 3-4 см – это будет ваш трафарет для выпечки макарон.<div><div>Просейте миндальную муку и сахарную пудру. Сухую смесь перемешайте в течение 3 минут на маленькой скорости миксером или в комбайне.&nbsp;</div><div>Разогрейте духовку до 150С, в течение 5 мин просушите смесь. Еще раз просейте вместе с какао-порошком.</div><div>В белки добавьте семена ванили. Взбейте белки в пушистую пену, постепенно небольшими порциями добавляйте сахар и продолжайте взбивать до устойчивых пиков. Оставьте белки на 2 минуты.</div></div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(21896,5680,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217164507-1-testo.jpg','<div>Аккуратно соедините взбитые белки с сухой смесью и перемешайте - снизу вверх, крутя миску по часовой стрелке.</div><div>Тесто должно стать блестящим, податливым и мягкой лентой стекать с лопатки.</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(21900,5680,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217164507-2-kryshechki.jpg','<div>Сложите тесто в кулинарный мешок или пакет и отсадите его кружочками на заготовленный пергамент. Вы замесили тесто правильно, если «хвостики» на поверхности пирожных сами расходятся, образуя гладкую ровную поверхность.</div><div>Оставьте заготовки при комнатной температуре на 30-60 минут. На поверхности теста должна образоваться очень тонкая пленочка (корочка), не липнущая к пальцам. При выпекании корочка становится хрустящей, а под ней образуется «юбочка».</div><div>Выпекайте меренги 13-15 мин в разогретой духовке до 150-155С.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21904,5680,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217164507-3-shokolad_s_slivkami.jpg','<div>Растопите шоколад на водяной бане или в микроволновой печи.</div><div>Добавьте в растопленный шоколад мед. Перемешайте.</div><div>Сливки почти доведите до кипения (90-95С), и в три этапа подмешайте к шоколаду.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21908,5680,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217164507-4-ganash.jpg','<div>Охладите шоколадную массу до 40С и подмешайте в нее сливочное масло комнатной температуры.</div><div>Уберите ганаш в холодильник на 1-2 часа.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(21912,5680,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217164508-5-sok.jpg','<div>Замочите желатин в холодной воде и дайте ему набухнуть.</div><div>Нагрейте апельсиновый сок с семенами из стручка ванили. Одновременно в другой емкости нагрейте сливки. Сок и сливки должны быть горячими до их использования.</div><div><br></div>',6);
INSERT INTO recipeStageEntity VALUES(21916,5680,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217164508-6-v_kastriule.jpg','<div>Доведите сахар и глюкозу до карамельного цвета. Влейте сначала сливки, затем апельсиновый сок.</div><div>Введите сливочное масло, в конце добавьте уксус и отжатый желатин.</div><div>Взбейте крем блендером и уберите в холодильник на ночь.</div><div><br></div>',7);
INSERT INTO recipeStageEntity VALUES(21920,5680,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201217164508-7-final.jpg','<div>Переложите ганаш и карамель в два кондитерских мешка, первым отсадите на половинку макарон шоколадный ганаш. В центр ганаша выложите шарик апельсиновой карамели, накройте второй половинкой макарон и уберите пирожное в холодильник на 24 часа.</div><div><br></div>',8);
INSERT INTO recipeStageEntity VALUES(21924,5684,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218123429-0-mindalnaia-smes.jpg','На обратной стороне листа пергаментной бумаги нарисуйте круги диаметром 3-4 см – это будет ваш трафарет для выпечки макарон.<div><div>Смешайте миндальную муку и сахарную пудру. Измельчите смесь еще раз в кофемолке или блендере. Просейте.</div><div>Разогрейте духовку до 150С, в течение 5 мин просушите смесь. Еще раз просейте.</div><div>В миску с мукой добавьте первую партию белков (55г) и отставьте емкость в сторону.</div><div>Белки (110 г) взбейте миксером в легкую пену, затем постепенно всыпайте сахар (30г).&nbsp; Продолжайте взбивать до мягких пиков.</div></div>',1);
INSERT INTO recipeStageEntity VALUES(21928,5684,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218123429-1-vzbili-s-zelen.jpg','<div><div>Одновременно в сотейнике доведите до кипения (100С) воду и сахар (300г), добавьте краситель.</div></div><div>Продолжайте держать сотейник на огне, пока сироп не прогреется до 117С. Сразу же тоненькой струйкой вливайте его во взбивающиеся на средней скорости белки.</div><div>Продолжайте взбивать до тех пор, пока масса не остынет и не станет блестящей, густой и гладкой. Это и есть ваша меренга.</div><div><br></div>',2);
INSERT INTO recipeStageEntity VALUES(21932,5684,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218123429-2-smes.jpg','<div>Возьмите ранее отставленную в сторону миску с мукой и белками, разотрите смесь в миндальную пасту, и соедините ее с меренгой. Аккуратно перемешайте лопаткой, при этом второй рукой вращая миску по часовой стрелке.</div><div>Тесто должно стать мягким, блестящим, однородным.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21936,5684,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218123429-3-na-pergamente.jpg','<div>Сложите тесто в кулинарный мешок или пакет и отсадите его кружочками на заготовленный пергамент. Оставьте заготовки при комнатной температуре на 30-40 минут.</div><div>Выпекайте меренги 12-15 мин в разогретой духовке до 150-155С.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21940,5684,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218123429-4-shokolad.jpg','<div>Желатин замочите в холодной воде (150 г) и дайте ему набухнуть.</div><div>Белый шоколад растопите на водяной бане или в микроволновой печи.</div><div><br></div>',5);
INSERT INTO recipeStageEntity VALUES(21944,5684,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218123429-5-s-penkoi.jpg','<div>В сотейнике нагрейте воду, сахар (100 г), измельченные листья базилика, и тоненькой струйкой вылейте в растопленный шоколад.</div><div>Добавьте сливочное масло, набухший и отжатый желатин, «Бехеровку». Хорошо взбейте блендером.</div><div><br></div>',6);
INSERT INTO recipeStageEntity VALUES(21948,5684,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218123429-6-nakryli-kryshechkoi.jpg','<div>Накройте ганаш пленкой и уберите в холодильник до загустения.</div><div>Соедините попарно половинки макарон ганашем и уберите на ночь в холодильник.</div><div><br></div>',7);
INSERT INTO recipeStageEntity VALUES(21952,5688,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218135539-0-iagodyjpg.jpg','Замороженные фрукты, яблоки и шпинат (не размораживая!) перемелите в блендере.',1);
INSERT INTO recipeStageEntity VALUES(21956,5688,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218135539-1-final-.jpg','Добавьте сахарный сироп по вкусу и разбавьте соком до нужной вам консистенции (сок 1:1).',2);
INSERT INTO recipeStageEntity VALUES(21960,5692,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218141755-0-baranina.jpg','<div>Почистите картофель, порежьте клубни на четыре части и переложите в кастрюлю с вскипяченной подсоленной водой. Установите таймер на 15 минут или до полного приготовления.</div><div><br></div><div>Налейте оливковое масло в нагретую большую сковороду, затем добавьте мелко нарезанную баранину. Постепенно обжаривайте баранину до румяного коричневого цвета в течении нескольких минут, при этом необходимо постоянно помешивать деревянной ложкой. Выдавите сюда чеснок через пресс. Переложите в тарелку.</div><div><br></div>',1);
INSERT INTO recipeStageEntity VALUES(21964,5692,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218141755-1-ovoshi.jpg','В ту же сковороду, налейте немного оливкового масла и обжарьте на среднем огне нарезанные лук и лук-порей в течении 5 минут. Помешивая добавьте Вустерширский соус, томатную пасту.',2);
INSERT INTO recipeStageEntity VALUES(21968,5692,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218141755-2-guliash.jpg','<div>Возвращаем обжаренную баранину в сковороду, перемешиваем. Добавляем зеленый горошек, нарубленную кубиком морковь и еще раз перемешиваем. Вливаем красное вино и готовим в течении минуты или двух, так мы выпарим алкоголь.&nbsp;</div><div>Затем добавьте куриный\овощной бульон или воду и варите в течении 3 минут.</div><div><br></div><div>Возьмите розмарин, отделите зелень от стеблей и посыпьте ею мясо. Приправьте солью и перцем. Готовьте на медленном огне в течение 10-15 минут.</div><div><br></div>',3);
INSERT INTO recipeStageEntity VALUES(21972,5692,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218141755-3-piure.jpg','<div>Слейте воду и разомните картофель при помощи толкушки.&nbsp;</div><div>Мелко порежьте зеленый лук и вместе с молоком и сливочным маслом, добавьте в пюре. Перемешайте.</div><div><br></div>',4);
INSERT INTO recipeStageEntity VALUES(21976,5692,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218141755-4-pirog.jpg','Выложите готовое мясо в форму для выпечки, сверху аккуратно распределите пюре, при помощи ложки выровняйте поверхность пирога. Затем используя вилку проткните его несколько раз, чтобы придать блюду своеобразный вид. Щедро посыпьте сверху тертым сыром.',5);
INSERT INTO recipeStageEntity VALUES(21980,5692,'https://gcdn.utkonos.ru/resample/recipe-cooking-mobile/images/recipe/cooking/20201218141755-5-final.jpg','Поставьте пирог в разогретую до 180 C духовку и выпекайте в течении 20 минут до золотистой, румяной корочки.',6);

getRecipesByTagTitle:
SELECT recipeEntity.* FROM recipeEntity JOIN recipeTagEntity ON recipeEntity.id = recipeId WHERE recipeTagEntity.title = ? ORDER BY recipeEntity.rating DESC, recipeEntity.id DESC;

getRecipes:
SELECT recipeEntity.* FROM recipeEntity JOIN recipeTagEntity ON recipeEntity.id = recipeId;

getById:
SELECT * FROM recipeEntity WHERE id = ?;

insert:
INSERT OR REPLACE INTO `recipeEntity` (`id`,`title`,`text`,`complexity`,`personCount`,`imageURL`,`rating`,`ratingVotes`) VALUES (nullif(?, 0),?,?,?,?,?,?,?);

insertRecipe:
INSERT OR REPLACE INTO `recipeEntity`  VALUES ?;

insertComment:
INSERT OR REPLACE INTO `recipeCommentEntity`  VALUES ?;

insertIngredient:
INSERT OR REPLACE INTO `recipeIngredientEntity`  VALUES ?;

insertStage:
INSERT OR REPLACE INTO `recipeStageEntity`  VALUES ?;

insertTag:
INSERT OR REPLACE INTO `recipeTagEntity`  VALUES ?;

insertComments:
INSERT OR REPLACE INTO `recipeCommentEntity` (`id`,`recipeId`,`authorId`,`authorName`,`date`,`text`,`parentCommentId`,`photoURLs`) VALUES (nullif(?, 0),?,?,?,?,?,?,?);

insertIngredients:
INSERT OR REPLACE INTO `recipeIngredientEntity` (`id`,`recipeId`,`title`,`quantity`,`measureUnit`,`position`) VALUES (?,?,?,?,?,?);

insertStages:
INSERT OR REPLACE INTO `recipeStageEntity` (`id`,`recipeId`,`imageURL`,`text`,`step`) VALUES (?,?,?,?,?);

insertTags:
INSERT OR REPLACE INTO `recipeTagEntity` (`id`,`title`,`recipeId`) VALUES (?,?,?);

removeTag:
DELETE FROM recipeTagEntity WHERE recipeId = ? AND title  = ?;

searchRecipes:
SELECT * FROM recipeEntity WHERE recipeEntity.title LIKE ?;

searchIngredients:
SELECT * FROM recipeIngredientEntity WHERE recipeIngredientEntity.title LIKE ?;

getByIngredient:
SELECT recipeEntity.* FROM recipeEntity JOIN recipeIngredientEntity ON recipeEntity.id = recipeId WHERE recipeIngredientEntity.title = ?;

getCommentsByRecipe:
SELECT * FROM recipeCommentEntity WHERE recipeId = ?;

getTagsByRecipe:
SELECT * FROM recipeTagEntity WHERE recipeId = ?;

getIngredientsByRecipe:
SELECT * FROM recipeIngredientEntity WHERE recipeId = ?;

getStagesByRecipe:
SELECT * FROM recipeStageEntity WHERE recipeId = ?;