INSERT INTO recipeCommentEntity VALUES(1026648,4303,0,'Ольга','2020-04-16 16:39:07','В ингредиентах не указан мёд, а в способе приготовления написано "Разведите дрожжи с мёдом"... ИСПРАВЬТЕ ОПИСАНИЕ...',0,'[]');
